[
    {
        "author": "黄春伯",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "访白玉蟾值其出题壁",
        "preface": "",
        "paragraphs": [
            "怪访怪，怪不在。",
            "茅君山，来相待。"
        ],
        "notes": "",
        "id": "0b3bcc750d7cff0fbfe1b85542aca4b6"
    },
    {
        "author": "黄春伯",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "绝句",
        "preface": "",
        "paragraphs": [
            "半篙春水一蓑烟，抱月怀中枕斗眠。",
            "说与时人休问我，英雄回首即神仙。"
        ],
        "notes": "",
        "id": "8089a42c4c78fac50f52ff26525c8747"
    },
