[
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 顾亭林",
        "preface": "",
        "paragraphs": [
            "旦暮潮流急，东西径路斜。",
            "无从质疑字，共属野王家。"
        ],
        "notes": "",
        "id": "272e83e64ca9f9e964898e4b9ad87152"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 顾府君宅",
        "preface": "",
        "paragraphs": [
            "萧萧兰若地，知是府君居。",
            "祠事今谁奉，吾来一叹吁。"
        ],
        "notes": "",
        "id": "32cae1548221ca69a3299256fe1f9729"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 东庵异迹",
        "preface": "",
        "paragraphs": [
            "地胜风雷吼，人疑涌海潮。",
            "将军英爽在，何物敢兴妖。"
        ],
        "notes": "",
        "id": "5b77404dac6122eb1242de102f61cfc1"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 金山",
        "preface": "",
        "paragraphs": [
            "激浪闻澎湃，山神阴力多。",
            "风涛归指顾，海若敢谁何。"
        ],
        "notes": "",
        "id": "e9542b71175d1e9cfa9dc40c941422e0"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 安公像",
        "preface": "",
        "paragraphs": [
            "浮钵来沧海，幡然厌世尘。",
            "补陀崖畔月，长照定中身。"
        ],
        "notes": "",
        "id": "1bf87632bd26f9723b228c150f89e03c"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 寒穴泉",
        "preface": "",
        "paragraphs": [
            "𣸣涌悬崖下，泠泠注不穷。",
            "将期挹甘冷，弱水渐难通。"
        ],
        "notes": "",
        "id": "d157cd27f0e914219a690fbfd7bd8497"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 苏州洋",
        "preface": "",
        "paragraphs": [
            "已出天池外，狂澜尚尔高。",
            "蛮商识吴路，岁入几千艘。"
        ],
        "notes": "",
        "id": "dc7a958ee018997e9b09d7be4603057d"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 华亭谷",
        "preface": "",
        "paragraphs": [
            "弥漫达迥野，潮汐往来频。",
            "惯识松陵路，重来不问津。"
        ],
        "notes": "",
        "id": "8fa5958f7f1253559551203f4c3f5e3d"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 八角井",
        "preface": "",
        "paragraphs": [
            "浚极疑无底，丰棱定有因。",
            "沈沈中夜月，照影属何人。"
        ],
        "notes": "",
        "id": "b4ae5c4d2bc01914c14b5369064c6223"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 陆机茸",
        "preface": "",
        "paragraphs": [
            "二陆为童日，驱驰屡忘归。",
            "至今桑柘响，禽鸟尚惊飞。"
        ],
        "notes": "",
        "id": "4bd4cf16d6e0b313effbd73133e691f6"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 柘湖",
        "preface": "",
        "paragraphs": [
            "展武沈沦后，波澄一鉴明。",
            "桑田复更变，触目总柴荆。"
        ],
        "notes": "",
        "id": "83d40eed584b254e00202fe565b3bef4"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 秦女祠",
        "preface": "",
        "paragraphs": [
            "狼秦崇苛政，有女亦蒙冤。",
            "欲吊兴亡事，神应耻重论。"
        ],
        "notes": "",
        "id": "0ad51db9aabdd0a8c63c48557d4eb8b7"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 唳鹤湖",
        "preface": "",
        "paragraphs": [
            "洸荡云闲路，奫沦一水深。",
            "月光涵露重，遥听九皋音。"
        ],
        "notes": "",
        "id": "2dacac7309c0c1f0c1995fbd972f1675"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 三女冈",
        "preface": "",
        "paragraphs": [
            "玉殓知无有，香魂计已销。",
            "频来吟楚些，岁久若为招。"
        ],
        "notes": "",
        "id": "f6c413607cb85cda6b8304b08014f1ce"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 金山城",
        "preface": "",
        "paragraphs": [
            "治盛周康世，东游岂信然。",
            "城𬮱亦隳废，门堞漫相传。"
        ],
        "notes": "",
        "id": "6d17ed00d5c23bcac719b388c9e59bec"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 前京城",
        "preface": "",
        "paragraphs": [
            "庐落皆无有，依稀古堞存。",
            "登临认遗迹，林莽暮烟昏。"
        ],
        "notes": "",
        "id": "6cad5ccb46ba4a2b2be35c9fd27515fa"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 昆山",
        "preface": "",
        "paragraphs": [
            "岁久阡原变，平时草木深。",
            "英才没黄壤，旧事不堪寻。"
        ],
        "notes": "",
        "id": "37511ede9778ce43f0fd60118ee649fc"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 陆机宅",
        "preface": "",
        "paragraphs": [
            "华亭今何在，千年谩有名。",
            "披榛问遗裔，寂寞一伤情。"
        ],
        "notes": "",
        "id": "3f4416f4f54baf7f187822eb8ecd653e"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 谷水",
        "preface": "",
        "paragraphs": [
            "短櫂经由处，风披藻荇香。",
            "中宵孤鹤唳，片月映沧浪。"
        ],
        "notes": "",
        "id": "379c2a68c5ed1cf8ce1882841f928633"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 秦皇驰道",
        "preface": "",
        "paragraphs": [
            "叹昔秦皇帝，何年此逸游。",
            "迢迢大堽路，千古为嗟羞。"
        ],
        "notes": "",
        "id": "cef2f2d4befeaf8393853cad9bac1905"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 征北将军墓碑",
        "preface": "",
        "paragraphs": [
            "畸岖寻古隧，衰草隠寒原。",
            "欲读将军事，丰碑祗半存。"
        ],
        "notes": "",
        "id": "aa57da7f8ecdfd99c13e41351866e0aa"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 凤皇山",
        "preface": "",
        "paragraphs": [
            "览德来何代，慵归丹穴藏。",
            "甘心化为石，万岁瑞吾皇。"
        ],
        "notes": "",
        "id": "16abf4505d58f24f06afff187a5b6cfd"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 陆宝山",
        "preface": "",
        "paragraphs": [
            "秘宝封坚石，山灵谨护藏。",
            "欲为中帑助，时复露辉光。"
        ],
        "notes": "",
        "id": "947223a214c0e3a315f6eedecb182fd5"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 佘山",
        "preface": "",
        "paragraphs": [
            "人与室俱化，阴森松竹寒。",
            "时时见孤鹤，疑此守神丹。"
        ],
        "notes": "",
        "id": "d72e652e991bbc9ea7c63c4efb48f8b7"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 集贤里",
        "preface": "",
        "paragraphs": [
            "冠盖游从日，欣然萃一乡。",
            "田原总如昨，谁复嗣余芳。"
        ],
        "notes": "",
        "id": "09f180a3bf62ff0369c5e8e3a9ae6f89"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 白龙洞",
        "preface": "",
        "paragraphs": [
            "呼吸湖中水，山椒寄此身。",
            "洞门风雨夜，电火逐霜鳞。"
        ],
        "notes": "",
        "id": "fa2529518fc22a75b7128fe0e2843664"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 石鱼",
        "preface": "",
        "paragraphs": [
            "双鲤何年刻，悠然鬐鬛全。",
            "登临忽怀远，尺素若为传。"
        ],
        "notes": "",
        "id": "5a737882be0ab67ebdf88f26c08b5c08"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 秀道者塔",
        "preface": "",
        "paragraphs": [
            "辛勤成鴈塔，俄赴积薪焚。",
            "静夜眈眈影，疑来护刻文。"
        ],
        "notes": "",
        "id": "891f0a7a3c527ab2ee4fc84d2bb6adf2"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 灵峰庵",
        "preface": "",
        "paragraphs": [
            "路涌金沙地，高人此筑庵。",
            "为询林下客，妙语与谁参。"
        ],
        "notes": "",
        "id": "a8c71ab289e8d2e53f8edd1c485034f2"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 芥子庵",
        "preface": "",
        "paragraphs": [
            "林外三椽建，常思愒息时。",
            "莫教嫌迫窄，中有五须弥。"
        ],
        "notes": "",
        "id": "2c6535283bb5f71ff451a5853ee2110d"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 淀山",
        "preface": "",
        "paragraphs": [
            "殿阁辉金碧，遐观足画图。",
            "维舟一登览，误涉小方壶。"
        ],
        "notes": "",
        "id": "d287f9fe2f00fe8836d092867845a706"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 三姑庙",
        "preface": "",
        "paragraphs": [
            "神居阴物护，臬阈捍洪波。",
            "莫虑蛟龙怒，年来畏叱诃。"
        ],
        "notes": "",
        "id": "6aaeaa0d0632e27f80259df95ecd017c"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 莲巢",
        "preface": "",
        "paragraphs": [
            "记得荷香里，蛾眉唱采莲。",
            "千年龟欲总，不见叶田田。"
        ],
        "notes": "",
        "id": "b97c922cfdc73a6c74f9e34fd20ca0c7"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 鬼书",
        "preface": "",
        "paragraphs": [
            "壮士为儒耻，捐躯志所甘。",
            "无名豫青史，幽愤亦何堪。"
        ],
        "notes": "",
        "id": "aedaf2c298536e87dc8d192e642992be"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 孔宅",
        "preface": "",
        "paragraphs": [
            "庙貌逾千祀，泥封古碣昏。",
            "传云素王宅，真赝与谁论。"
        ],
        "notes": "",
        "id": "6fe9b04995a8cf3a3d5a08a850f57850"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 袁崧宅",
        "preface": "",
        "paragraphs": [
            "疆场功成后，归来卜一邱。",
            "当时应种德，名字播千秋。"
        ],
        "notes": "",
        "id": "ee6b000aab2d524a57fb16bdf8425b0d"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 俞塘",
        "preface": "",
        "paragraphs": [
            "延袤三乡外，东流与海通。",
            "河神屡加惠，帆借往来风。"
        ],
        "notes": "",
        "id": "60a120935868ec134156381634fa4c60"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 鹤坡",
        "preface": "",
        "paragraphs": [
            "索寞东郊远，仙禽尽此藏。",
            "梦回明月夜，林杪响圆吭。"
        ],
        "notes": "",
        "id": "a6cd83cd8f7440a1caa31a7033250a29"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 沙冈",
        "preface": "",
        "paragraphs": [
            "千里平沙地，联通江海湄。",
            "谩传因激浪，疑是蚌成基。"
        ],
        "notes": "",
        "id": "431ab70aff178204a73a81d594abdaae"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 御史泾",
        "preface": "",
        "paragraphs": [
            "疏凿人何在，流功几世余。",
            "农氓诵名氏，不数郑公渠。"
        ],
        "notes": "",
        "id": "430dd71f911c2156862a0da63d8fe92e"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 沪渎",
        "preface": "",
        "paragraphs": [
            "泛泛松江水，迢迢笠泽通。",
            "万年知禹力，灌溉有余功。"
        ],
        "notes": "",
        "id": "558fa1e62e9c49bf35bd987f83e4d8af"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 东堂",
        "preface": "",
        "paragraphs": [
            "厅左清闲处，金猊罩宝香。",
            "令君今学道，来此吸朝阳。"
        ],
        "notes": "",
        "id": "e6bc5eda746f7e2ebfa41e17c4686c65"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 思齐堂",
        "preface": "",
        "paragraphs": [
            "伊昔弦歌治，能声著海隅。",
            "升堂想遗迹，励志欲齐驱。"
        ],
        "notes": "",
        "id": "a2c1ca11ba923342be413a73ba2ed69a"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 月榭",
        "preface": "",
        "paragraphs": [
            "簷角衔墙固，明疏护翠罗。",
            "公余秉谈麈，竟夕对金波。"
        ],
        "notes": "",
        "id": "8aff9cafbf0e35e4dcb2dbf1f112aea1"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 濯缨亭",
        "preface": "",
        "paragraphs": [
            "舆梁斜倚处，簷影坠清流。",
            "往往抽簪者，常为孺子羞。"
        ],
        "notes": "",
        "id": "b4f9e28a828bb16298a4600f9c7ea57e"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 石兽",
        "preface": "",
        "paragraphs": [
            "刻凿知何谓，弥年亦有神。",
            "宵行无足怪，一吼已惊人。"
        ],
        "notes": "",
        "id": "842a580b11b22bbd2c2ee083d8dd0300"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 震桥",
        "preface": "",
        "paragraphs": [
            "瓦木明丹垩，初期镇此方。",
            "石梁今欲圮，尤益富蛮商。"
        ],
        "notes": "",
        "id": "4c04bd990443941a0cbccff7f7d26ffa"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 小隠园",
        "preface": "",
        "paragraphs": [
            "妆点林泉趣，销除市井心。",
            "平生跧伏计，到此意偏深。"
        ],
        "notes": "",
        "id": "d8d85d62cc06411183c6375213978596"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 姚将军庙",
        "preface": "",
        "paragraphs": [
            "阴功周邑境，南面固宜然。",
            "毋复论民事，神今惮改迁。"
        ],
        "notes": "",
        "id": "474dc08a43ea37fc4b818cf390f4ecb6"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 赵店",
        "preface": "",
        "paragraphs": [
            "日日黄尘路，喧豗涨市声。",
            "神灵自安此，不欲变名称。"
        ],
        "notes": "",
        "id": "e7fc8b812e506d32dba7f720b7aab4a2"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 望云桥",
        "preface": "",
        "paragraphs": [
            "忆昨登临日，卿云正四翔。",
            "重来穷目力，天末起祥光。"
        ],
        "notes": "",
        "id": "0e7563e4b45532542923e2c1570c3642"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 石幢",
        "preface": "",
        "paragraphs": [
            "矻立应千载，传因海眼成。",
            "蓬莱水清浅，曾不见敧倾。"
        ],
        "notes": "",
        "id": "2428eaaab416d81b335d33ada50df2d0"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 普照寺",
        "preface": "",
        "paragraphs": [
            "高门收画戟，即此建精蓝。",
            "施予恩虽在，缁流绝不谈。"
        ],
        "notes": "",
        "id": "16acdf8b79b35d7f59b7644f4adf6340"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 弥陀阁",
        "preface": "",
        "paragraphs": [
            "拄策徐徐上，回环佛顶游。",
            "九山俱在望，目断暮烟浮。"
        ],
        "notes": "",
        "id": "a7bd96efcf9d25728e501e76c53078f3"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 罗汉院",
        "preface": "",
        "paragraphs": [
            "邃宇中何有，台山五百尊。",
            "云何困人力，金像总尘昏。"
        ],
        "notes": "",
        "id": "5fc73291f07c786c50dbb2e40a5240f9"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 丁公桥",
        "preface": "",
        "paragraphs": [
            "令威仙去后，遗迹叹成非。",
            "华表成乌有，何年见鹤归。"
        ],
        "notes": "",
        "id": "d6b27380da3077fa7268f583e8cd7f1a"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 云闲馆",
        "preface": "",
        "paragraphs": [
            "邮亭临爽垲，杗廇架修虹。",
            "我亦轮蹄客，心期访士龙。"
        ],
        "notes": "",
        "id": "63957f8180704b583ae2c6dc11edf9f0"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 福顺庙",
        "preface": "",
        "paragraphs": [
            "门垣虽蠹坏，古屋尚深沈。",
            "鸟雀犹知避，斯民盍致钦。"
        ],
        "notes": "",
        "id": "fdb9c10398797e2390586502d7ff95b5"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 东岳行祠",
        "preface": "",
        "paragraphs": [
            "明主东封后，龟蒙望益尊。",
            "巍然此行宇，时许荐苹蘩。"
        ],
        "notes": "",
        "id": "13d2cbe035d3fa423c9a96248471ca77"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 西庵",
        "preface": "",
        "paragraphs": [
            "蚤为儒冠误，齐心学竺乾。",
            "而今林下塔，夜夜戒光圆。"
        ],
        "notes": "",
        "id": "b751953404b44c0430febd7b603635a5"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 白龙潭",
        "preface": "",
        "paragraphs": [
            "神物幽潜地，沧沧水接空。",
            "不缘尝应祷，谁识有殊功。"
        ],
        "notes": "",
        "id": "d4d94ef73183167c20849fd237376e66"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 毗卢庵",
        "preface": "",
        "paragraphs": [
            "扪萝通一径，修竹蔽中篱。",
            "只有奎章在，龙神暗护持。"
        ],
        "notes": "",
        "id": "42727976c1381e0a28f2f22ff906ee16"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 西林",
        "preface": "",
        "paragraphs": [
            "林外初晴后，斜阳满芰荷。",
            "频来指灵鹫，此去路无多。"
        ],
        "notes": "",
        "id": "f69dbe97239254de9ce36344c4f88c2c"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 陆瑁养鱼池",
        "preface": "",
        "paragraphs": [
            "谁得陶朱术，修池一水宽。",
            "皇恩浃鱼鼈，不复敢垂竿。"
        ],
        "notes": "",
        "id": "8758757b5c93a550df12cf884ee89a20"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 小湖",
        "preface": "",
        "paragraphs": [
            "猎猎风蒲外，渊渊明月秋。",
            "诗翁浮短艇，弭棹看浮鯈。"
        ],
        "notes": "",
        "id": "015e6f75e2f10f8b4cb04ca9e007ccf0"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 湖桥",
        "preface": "",
        "paragraphs": [
            "潋滟湖光好，荷风六月凉。",
            "倚栏吟不倦，鱼鸟亦相忘。"
        ],
        "notes": "",
        "id": "fc53187c6a2468e75200204a957498be"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 唳鹤滩",
        "preface": "",
        "paragraphs": [
            "养鹤人何在，湖边水尚清。",
            "唤回中夜梦，滩上戛然声。"
        ],
        "notes": "",
        "id": "76076178b84a6e801bfd28d045050205"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 泳波亭",
        "preface": "",
        "paragraphs": [
            "吕梁观已远，沂水浴应难。",
            "却倚亭前柳，时时照影寒。"
        ],
        "notes": "",
        "id": "5edb3685e162bad1d279f5b003d7fadd"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 风月堂",
        "preface": "",
        "paragraphs": [
            "夜静蒲萄皱，云舒玉鉴凉。",
            "筑堂人不见，樽酒暗相望。"
        ],
        "notes": "",
        "id": "693861e980deb8942804f465ed582ec6"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 湖光亭",
        "preface": "",
        "paragraphs": [
            "日暮虾须卷，亭中雅燕开。",
            "微风起苹末，波影动樽罍。"
        ],
        "notes": "",
        "id": "5b596f517005d65ce80966c77974de91"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 道院",
        "preface": "",
        "paragraphs": [
            "浄观今何在，唯余柏色新。",
            "栖栖抱瓮者，不是炼丹人。"
        ],
        "notes": "",
        "id": "57bafc3d77df00d5c179b0159b5e8be3"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 异木",
        "preface": "",
        "paragraphs": [
            "蟠根知永远，曲干耸蛟虬。",
            "闽贾蒙私庇，年年此荐羞。"
        ],
        "notes": "",
        "id": "8977444fdba1ace8172e2764ce31c97a"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 显忠庙",
        "preface": "",
        "paragraphs": [
            "俯拜祠堂下，遥思弼汉功。",
            "吁嗟孝宣帝，忘德太匆匆。"
        ],
        "notes": "",
        "id": "70e65b20f530f80de5b79ad36741d3b8"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 东寺",
        "preface": "",
        "paragraphs": [
            "爱此楞伽士，勤修戒行成。",
            "文龟亦知善，来听讲经声。"
        ],
        "notes": "",
        "id": "036ae2594622214eb405a3e9650d2359"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 证觉院",
        "preface": "",
        "paragraphs": [
            "有人明七浄，蠠没缔良因。",
            "叹惜流风替，谁为无垢人。"
        ],
        "notes": "",
        "id": "09514f4b0a843acc03865e73afb17db7"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 栖真庵",
        "preface": "",
        "paragraphs": [
            "仙路多官府，真人未上升。",
            "松窗霜月白，相对诵黄庭。"
        ],
        "notes": "",
        "id": "f421ec00aa2fbc480cfb68be83d07ef8"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 柳园",
        "preface": "",
        "paragraphs": [
            "芳草池塘暖，东风桃杏繁。",
            "春归人亦散，寂寞锁园门。"
        ],
        "notes": "",
        "id": "d061d0e410872d6ad274b5793330b3f5"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 陆四公庙",
        "preface": "",
        "paragraphs": [
            "晋杰云亡久，嘉名未遽湮。",
            "严祠坐遗像，犹解福吴民。"
        ],
        "notes": "",
        "id": "9f373d62fb6519f352504b8552f85726"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 南庵",
        "preface": "",
        "paragraphs": [
            "施水功成后，眈眈梵宇深。",
            "升堂瞻貌象，默会祖师心。"
        ],
        "notes": "",
        "id": "eff2548374eafe5caf879eaeba538d2d"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 姜庵",
        "preface": "",
        "paragraphs": [
            "阴曀楸梧里，鸟鸣落日中。",
            "九原人不作，明赞亦何功。"
        ],
        "notes": "",
        "id": "3f09f2817a26f5efcb0e0aba1fa31504"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 望仙桥",
        "preface": "",
        "paragraphs": [
            "鹤驾乘风去，千年竟不归。",
            "石梁凭望处，空复白云飞。"
        ],
        "notes": "",
        "id": "36d8c1276cd4c96ddcc0b5f2ce96a1f5"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 三洞庵",
        "preface": "",
        "paragraphs": [
            "勾曲山头客，遨游到市尘。",
            "星冠顶霜月，夜半正朝真。"
        ],
        "notes": "",
        "id": "24309eb4c05f503b268a2aa8b629f88e"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 浄居院",
        "preface": "",
        "paragraphs": [
            "明眼知何在，山门久寂然。",
            "宗风未沦替，还喜一镫传。"
        ],
        "notes": "",
        "id": "0fa579984eaaf7c62aad46a5fd9ab0a7"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 冰檗庵",
        "preface": "",
        "paragraphs": [
            "凫舄栖迟地，池流一径斜。",
            "先生久清苦，不肯竞纷华。"
        ],
        "notes": "",
        "id": "621689dfbc853c3753842d3d425614a5"
    },
    {
        "author": "许尚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "华亭百咏 圆珠庵",
        "preface": "",
        "paragraphs": [
            "定回人寂静，绕鼻碧莲香。",
            "却认循簷月，摩尼发夜光。"
        ],
        "notes": "",
        "id": "eaafd2ffe65060aec2d02ef3c1d27029"
    }
]