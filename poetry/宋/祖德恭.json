[
    {
        "author": "祖德恭",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "致政张安卿承事挽词三首  其一",
        "preface": "",
        "paragraphs": [
            "孝养人谁及，清修少比伦。",
            "旋闻丹旐具，俄见轴车存。",
            "空叹音尘远，犹思燕席温。",
            "安然向窀穸，感涕一销魂。"
        ],
        "notes": "",
        "id": "209593fd7ca808546b19207be111e758"
    },
    {
        "author": "祖德恭",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "致政张安卿承事挽词三首  其二",
        "preface": "",
        "paragraphs": [
            "清白传家旧，天钟间世才。",
            "大椿宜更寿，乔木遽先摧。",
            "难止交情泪，空遗宝烛灰。",
            "幽墟归葬处，惨惨暮云堆。"
        ],
        "notes": "",
        "id": "bc640d02370c9dce0dee3ea98fbc02c4"
    },
    {
        "author": "祖德恭",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "致政张安卿承事挽词三首  其三",
        "preface": "",
        "paragraphs": [
            "重叹张夫子，神襟遽弗宁。",
            "摧山倾岳算，萎哲奄椿龄。",
            "旧室惟图像，新阡但刻铭。",
            "何堪听歌挽，埋玉在幽坰。"
        ],
        "notes": "",
        "id": "ce2187dee21fbb6df83974246fe0141c"
    }
]