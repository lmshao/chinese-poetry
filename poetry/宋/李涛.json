[
    {
        "author": "李涛",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "南山",
        "preface": "",
        "paragraphs": [
            "南山思美人也",
            "彼美人兮，在南山之阳。",
            "岂不尔思，道阻且长。",
            "彼美人兮，在南山之侧。",
            "我不见兮，寤寐思服。",
            "彼美人兮，悠悠我思。",
            "何以予之，琼瑰玉佩。"
        ],
        "notes": "",
        "id": "9301e5363e47027e158e8f4f618fcc73"
    },
    {
