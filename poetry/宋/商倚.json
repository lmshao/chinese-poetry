[
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思诗呈同院诸公  其一",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，群贤道足依。",
            "心游黄卷静，目送白云归。",
            "宝玉荆山尽，文星禁掖稀。",
            "清谈欣屡接，纷若落琼玑。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思诗呈同院诸公  其二",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，云沉夕照光。",
            "蛩吟阶下草，萤照屋间梁。",
            "卧月搜诗句，挑灯认药囊。",
            "夜阑独衾枕，归梦楚山长。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思诗呈同院诸公  其三",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，愁肠夜九回。",
            "图书堆枕畔，歌吹隔墙隈。",
            "宿鸟翻枝去，凉风带雨来。",
            "新诗劳见寄，难继谪仙才。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思诗呈同院诸公  其四",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，群书昼可依。",
            "衰迟多病在，留滞故园归。",
            "但怕酒杯满，莫教诗卷稀。",
            "惟羞狂斐句，瓦砾报珍玑。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思诗呈同院诸公  其五",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，晨晖带冷光。",
            "彩云瞻凤阙，细雨忆鱼梁。",
            "久事班超笔，空嗟赵壹囊。",
            "帘闲坐无语，千丈旅愁长。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思诗呈同院诸公  其六",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，西风昨夜回。",
            "燕巢留屋角，蛛网挂墙隈。",
            "书任边韶懒，诗惭匡鼎来。",
            "人人皆抱璞，谁是济川才。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思秋日同文馆  其一",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，高堂燕集齐。",
            "杯盘供鲁酒，殽核欠张梨。",
            "杨孟持新论，韩庄鄙旧题。",
            "须知宗匠手，文采冠朝闺。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思秋日同文馆  其二",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，何人独擅场。",
            "养莪方在沚，琢玉已成章。",
            "笔列千锋阵，庭焚一炷香。",
            "得贤从此始，岂特致民康。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思秋日同文馆  其三",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，栖林看晚鸡。",
            "斗垂宫殿北，月转户庭西。",
            "酒劝杯中渌，诗分烛下题。",
            "何时还九陌，细雨莫成泥。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思未试即事杂书率用秋日同文馆为首句  其一",
        "preface": "",
        "paragraphs": [
            "气候逢秋晏，蛩鸣入户闱。",
            "簷空看雀鬭，天阔暮鸦归。",
            "风急飘庭叶，寒光著客衣。",
            "重阳故人境，应念赏心违。"
        ],
        "notes": "",
        "id": "0327f7b7a9d6669d67d2ed0344c7b60a"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思未试即事杂书率用秋日同文馆为首句  其二",
        "preface": "",
        "paragraphs": [
            "门深廛市远，幽寂兴何长。",
            "径菊晴披艳，窗风晚逗凉。",
            "圣经闲里醉，尘锁梦中忘。",
            "更苦年来病，时看肘后方。"
        ],
        "notes": "",
        "id": "3e12f925c008c20714731bf926ca8acb"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思未试即事杂书率用秋日同文馆为首句  其三",
        "preface": "",
        "paragraphs": [
            "旁舍回廊接，重帘昼日开。",
            "鸣蝉随雨退，疏雁拂云来。",
            "吟兴降难韵，羁怀寄浅杯。",
            "惊飙渐凄紧，又欲作寒媒。"
        ],
        "notes": "",
        "id": "8a98438aa0edd955c74da47f3282f9a8"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思初入试院",
        "preface": "",
        "paragraphs": [
            "承诏抡材敢倦行，广庭深锁待群英。",
            "分场自敌三千客，决胜谁降七十城。",
            "夜案尚闲涂卷笔，晓堂方听读书声。",
            "归时还有黄花否，已觉秋风满袖生。"
        ],
        "notes": "",
        "id": "cb85483cf2d6b4ed9391d3602f909cb9"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "次韵余干试院即事呈诸公",
        "preface": "",
        "paragraphs": [
            "风生帘幕撼层波，况值清秋一半过。",
            "堂上簪裾方鹭集，案间图史已星罗。",
            "东篱采菊朝尝酒，北里吹竽夜听歌。",
            "早晚奏名闻黼坐，望中无奈白云何。"
        ],
        "notes": "",
        "id": "4d8bd43813d01a1169fd49e79d44859c"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "再呈慎思诸公兼以言怀",
        "preface": "",
        "paragraphs": [
            "南窗寂若隠山河，时枉金门步见过。",
            "谈笑且看挥麈玉，篇章宁惜洒牋罗。",
            "已羞轩冕婴身住，更问功名抚剑歌。",
            "赊得凉秋好风月，不成吟醉奈愁何。"
        ],
        "notes": "",
        "id": "0cc54ada690d078239b343b7b845fe96"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "次韵重九之什  其一",
        "preface": "",
        "paragraphs": [
            "英豪献策广庭中，问辨纷纷似叩钟。",
            "名奏九重书密上，士欣千载运亲逢。",
            "鶑鸣定获乔林乐，凤举应容众羽从。",
            "盛世文章由此变，坐看风俗反醇浓。"
        ],
        "notes": "",
        "id": "52b7655ba2b9b5f0ac5f2d10eb8a2bd2"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "次韵重九之什  其二",
        "preface": "",
        "paragraphs": [
            "昔年此日故园东，烂醉牛山到暮钟。",
            "已往光阴那再得，如今羁旅又重逢。",
            "渊明把菊情虽厚，子美登台愿莫从。",
            "且看群书欹枕卧，病多仍负酒杯浓。"
        ],
        "notes": "",
        "id": "fc95a8ed21d10e70565b9f5adfac49f3"
    },
    {
        "author": "商倚",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和慎思同舍问及故山景物用钟字韵诗以答",
        "preface": "",
        "paragraphs": [
            "先生家住翠微峰，万卷诗书酒一钟。",
            "壁挂葛巾无客到，云藏古洞有仙逢。",
            "溪边晚钓呼鸥伴，花底春吟引鹤从。",
            "只恐旧山归未得，帝恩朝夕露浓浓。"
        ],
        "notes": "",
        "id": "362deae1d4f6d25790498d20f3e9f872"
    }
]