[
    {
        "author": "裴守真",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和太子纳妃太平公主出降三首 一",
        "preface": "",
        "paragraphs": [
            "瑜珮升青殿，秾华降紫微。",
            "还如桃李发，更似凤皇飞。",
            "金屋真离象，瑶台起婺徽。",
            "彩缨纷碧坐，[缋]羽泛褕衣。"
        ],
        "notes": "",
        "id": "3f9190a617f7f24aa4cbdb715334a447"
    },
    {
        "author": "裴守真",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和太子纳妃太平公主出降三首 二",
        "preface": "",
        "paragraphs": [
            "云路移彤辇，天津转明镜。",
            "仙珠照乘归，宝月重轮映。",
            "望园嘉宴洽，主第欢娱盛。",
            "丝竹扬帝熏，簪裾奉宸庆。"
        ],
        "notes": "",
        "id": "01f61f38fc3666b97b04e91cbb51ec73"
    },
    {
        "author": "裴守真",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和太子纳妃太平公主出降三首 三",
        "preface": "",
        "paragraphs": [
            "藂云霭晓光，湛露晞朝阳。",
            "天文天景丽，睿藻睿词芳。",
            "玉庭散秋色，银宫生夕凉。",
            "太平超邃古，万寿乐无疆。"
        ],
        "notes": "",
        "id": "141a3c318f9c9f286c4ba1850e9cc04a"
    }
