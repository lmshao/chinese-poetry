[
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和九日幸临渭亭登高应制得月字",
        "preface": "",
        "paragraphs": [
            "云物开千里，天行乘九月。",
            "丝言丹凤池，斾转苍龙阙。",
            "灞水欢娱地，秦京游侠窟。",
            "欣承解愠词，圣酒黄花发。"
        ],
        "notes": "",
        "id": "d9920dc9d6977d15366321b7e5b6f199"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和送金城公主适西蕃应制",
        "preface": "",
        "paragraphs": [
            "柔远安夷俗，和亲重汉年。",
            "军容旌节送，国命锦车传。",
            "琴曲悲千里，箫声恋九天。",
            "唯应西海月，来就掌珠圆。"
        ],
        "notes": "",
        "id": "4b33469d5fcfceea82d145ae3c8095ce"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "饯唐州高使君赴任",
        "preface": "",
        "paragraphs": [
            "桐栢膺新命，芝兰惜旧游。",
            "鸣臯夜鹤在，迁木早莺求。",
            "传拥淮源路，尊空灞水流。",
            "落花纷送远，春色引离忧。"
        ],
        "notes": "",
        "id": "80dabdbc1aba498c38192e0c2a946007"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "早朝",
        "preface": "",
        "paragraphs": [
            "震维芳月季，宸极众星尊。",
            "珮玉朝三陛，鸣珂度九门。",
            "挈壶分早漏，伏槛耀初暾。",
            "北倚苍龙阙，西临紫凤垣。",
            "词庭草欲奏，温室树无言。",
            "鳞翰空为忝，长怀圣主恩。"
        ],
        "notes": "",
        "id": "74cd8d03a694fa6af038fdfde9f0738e"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和立春游苑迎春应制",
        "preface": "",
        "paragraphs": [
            "灞涘长安恒近日，殷正腊月早迎新。",
            "池鱼戏叶仍含冻，宫女裁花已作春。",
            "向苑云疑承翠幄，入林风若起青苹。",
            "年年斗柄东无限，愿挹琼觞寿北辰。"
        ],
        "notes": "",
        "id": "0dc3370d7f839474ad332036344a5daf"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和圣制春日幸望春宫应制",
        "preface": "",
        "paragraphs": [
            "九重楼阁半山霞，四望韶阳春未赊。",
            "侍跸妍歌临灞涘，留觞艳舞出京华。",
            "危竿竞捧中街日，戏马争衔上苑花。",
            "景色欢娱长若此，承恩不醉不还家。"
        ],
        "notes": "",
        "id": "7ecdcda8ef73e7226d17abef9e59f0f3"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和人日宴大明宫恩赐彩缕人胜应制",
        "preface": "",
        "paragraphs": [
            "鸾凤旌旗拂晓陈，鱼龙角觝大明辰。",
            "青韶既肇人为日，绮胜初成日作人。",
            "圣藻凌云裁栢赋，仙歌促宴摘梅春。",
            "垂旒一庆宜年酒，朝野俱欢荐寿新。"
        ],
        "notes": "",
        "id": "7d5bd7979de057b3e71cf25d039bdb59"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和幸安乐公主山庄应制",
        "preface": "",
        "paragraphs": [
            "银河南渚帝城隅，帝辇平明出九衢。",
            "刻凤蟠螭凌桂邸，穿池叠石写蓬壶。",
            "琼箫暂下钧天乐，绮缀长悬明月珠。",
            "仙牓承恩争既醉，方知朝野更欢娱。"
        ],
        "notes": "",
        "id": "8febeafc276514279763183745938fb0"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "兴庆池侍宴应制",
        "preface": "",
        "paragraphs": [
            "沧池漭沆帝城边，殊胜昆明凿汉年。",
            "夹岸旌旗疏辇道，中流箫鼓振楼船。",
            "云峰四起迎宸幄，水树千重入御筵。",
            "宴乐已深鱼藻咏，承恩更欲奏甘泉。"
        ],
        "notes": "",
        "id": "6d315d154e4e862fa048137cc2613f05"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "夜宴安乐公主宅",
        "preface": "",
        "paragraphs": [
            "主第新成银作牓，宾筵广宴玉为楼。",
            "壶觞既卜仙人夜，歌舞宜停织女秋。"
        ],
        "notes": "",
        "id": "d8da72a5e225e8517dbd75b3cbfa8cd0"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "五言夏日游神泉诗（幷序）",
        "preface": "",
        "paragraphs": [
            "闻有濠梁地，驾言幷四美。",
            "契冥邀异迹，胜会不延晷。",
            "涧响若琴中，泉华疑镜里。",
            "形随员月正，制逐规虹起。",
            "澡流莹丹心，跂石凉玉趾。",
            "近焉将安适？行当润蒙泛。",
            "（此诗见《金石萃编》卷六十一。",
            "）（按诗题下原署「美原县尉韦元旦字烜」，碑刻文字系尹元凯所书。",
            "）。"
        ],
        "notes": "",
        "id": "82542984d0e4594362193706b68d415f"
    },
    {
        "author": "韦元旦",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "五言奉和姚元崇相公过楼岩寺诗（题拟）",
        "preface": "",
        "paragraphs": [
            "岩突金银台，登攀信美哉！白林丛万壑，珠缀结三台。",
            "应物尽标胜，冥心无去来。",
            "{汞夆/金}明巾铁柱，长欲助盐梅。",
            "（录自清胡聘之《山右石刻丛编》卷五）（按：此诗刻于姚崇五言《过楼岩寺》后，题仅作「五言奉和」，下署：「前朝议郎行左台监察御史摄宫尹司直韦元旦。",
            "」）。"
        ],
        "notes": "",
        "id": "d46346e10e855fcf4bf0f14a1cc1bb3f"
    }
]