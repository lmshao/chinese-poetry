[
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一",
        "preface": "",
        "paragraphs": [
            "朔风吹石裂，寒谷自春生。",
            "根老香全古，花疏格转清。",
            "园林千树秃，篱落一枝横。",
            "佩芷兼怀玉，悠然见此兄。"
        ],
        "notes": "",
        "id": "de84e7478b0820304237d96b9d61c84b"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其二",
        "preface": "",
        "paragraphs": [
            "苔封鹤膝枝，流水绕疏篱。",
            "一白雪相似，独清春不知。",
            "风流无俗韵，恬澹出天姿。",
            "霜月娟娟夜，吾今见所思。"
        ],
        "notes": "",
        "id": "2031ba888c4f071855010e45532cc3f7"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其三",
        "preface": "",
        "paragraphs": [
            "半枯顽铁石，特地数花生。",
            "迥立风尘表，长含水月清。",
            "屋头寒岭瘦，门外小溪横。",
            "万里今为客，相看如弟兄。"
        ],
        "notes": "",
        "id": "ad6e85ca194798aefd1efa33c877b816"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其四",
        "preface": "",
        "paragraphs": [
            "疎疎竹外枝，短短水边篱。",
            "南雪若相避，东风殊不知。",
            "兰荃皆弱植，桃杏总凡姿。",
            "坐叹逋仙远，清宵费梦思。"
        ],
        "notes": "",
        "id": "07fd3ebeedc3723d51e74c71dfab3e32"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其五",
        "preface": "",
        "paragraphs": [
            "不带吟诗癖，缘何太瘦生。",
            "肌肤姑射白，风骨伯夷清。",
            "格外宫粧别，天然画轴横。",
            "涪翁太多可，唤作水仙兄。"
        ],
        "notes": "",
        "id": "45b75c5d0cf52a7b699d7bfadaa386b3"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其六",
        "preface": "",
        "paragraphs": [
            "苍虬百岁枝，残雪数家篱。",
            "点俗那能染，孤芳只自知。",
            "肯回桃李面，要是雪霜姿。",
            "不见紫芝久，悠悠使我思。"
        ],
        "notes": "",
        "id": "0acbf86c62c4fc66c8616d67ddde6b04"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其七",
        "preface": "",
        "paragraphs": [
            "不与百花竞，春风蓦地生。",
            "故将天下白，独向雪中清。",
            "我辈诗仍要，谁家笛自横。",
            "岁寒堪共老，髯叟十年兄。"
        ],
        "notes": "",
        "id": "9383476419005b64d276b8a64cf923d0"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其八",
        "preface": "",
        "paragraphs": [
            "何处出斜枝，茅簷自竹篱。",
            "首回春一盼，最与月相知。",
            "严冷冰霜面，清癯山泽姿。",
            "几番将鹤去，倚树说相思。"
        ],
        "notes": "",
        "id": "f9a41163c012a71dcc11faef03fef5cc"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其九",
        "preface": "",
        "paragraphs": [
            "有月色逾淡，无风香自生。",
            "霜崖和树瘦，冰壑养花清。",
            "政尔疎还冷，忽然斜又横。",
            "千林成独韵，难弟又难兄。"
        ],
        "notes": "",
        "id": "351a86166e8c2a9798a79d610f8264ca"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一○",
        "preface": "",
        "paragraphs": [
            "雪后半横枝，溪边一带篱。",
            "春从穷腊透，香报老夫知。",
            "淡月弄疎影，嫩寒含令姿。",
            "天涯值西子，牢落慰吾思。"
        ],
        "notes": "",
        "id": "848e95a8a54e5d697d2e52c3de41af3c"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一一",
        "preface": "",
        "paragraphs": [
            "殷勤天女供，那复一尘生。",
            "质淡全身白，香寒到骨清。",
            "常留雪中看，遮莫鬓边横。",
            "万古月宫桂，犹吾异姓兄。"
        ],
        "notes": "",
        "id": "b9d3a4149c4c8591bb7cebab6f818783"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一二",
        "preface": "",
        "paragraphs": [
            "古藓护疎枝，幽花发短篱。",
            "唯宜霜月照，莫遣雪风知。",
            "数点玲珑玉，三生洒落姿。",
            "自从窗外见，风味至今思。"
        ],
        "notes": "",
        "id": "94edbe3962e9e6fa33983c1da7108a39"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一三",
        "preface": "",
        "paragraphs": [
            "玉肌元不粟，未怕夜寒生。",
            "雪里孤花发，山中一段清。",
            "几回和月看，独立到参横。",
            "不傍人篱落，谁呼石作兄。"
        ],
        "notes": "",
        "id": "391e84e649bae6f9b5a61b3eda6fb920"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一四",
        "preface": "",
        "paragraphs": [
            "春脚到寒枝，诗情满雪篱。",
            "每留孤鹤伴，不遣一蜂知。",
            "风漏腊前信，月描尘外姿。",
            "忆从归阆苑，终岁祇君思。"
        ],
        "notes": "",
        "id": "85f781b7726a2d038d751303113b8cad"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一五",
        "preface": "",
        "paragraphs": [
            "万壑寒皆沍，孤根暖自生。",
            "不随千卉艳，独负一身清。",
            "水际寒香迥，窗间夜影横。",
            "舆台桃李辈，谁弟又谁兄。"
        ],
        "notes": "",
        "id": "5cc37dd18b914caa9a012dc5f6bbd9bb"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一六",
        "preface": "",
        "paragraphs": [
            "突出一清枝，孤村雪拥篱。",
            "韵无凡眼识，香有自心知。",
            "不是神仙骨，何缘冰玉姿。",
            "看看金鼎实，此味几人思。"
        ],
        "notes": "",
        "id": "32feff288c4e545868276c5b652357b0"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一七",
        "preface": "",
        "paragraphs": [
            "参透香严境，都无灭与生。",
            "平分天女白，全借月娥清。",
            "雪屋人家好，霜桥野岸横。",
            "天寒倚修竹，齐弟伯夷兄。"
        ],
        "notes": "",
        "id": "6b798ca04c48a375c9ec4052d24d6fc9"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一八",
        "preface": "",
        "paragraphs": [
            "子真同一性，和靖是三生。",
            "山意冷愈澹，月香幽更清。",
            "只应霜鴈识，未怯玉龙横。",
            "密友惟清士，佳名称素兄。"
        ],
        "notes": "",
        "id": "c1b8c51cef20f0c92fa989e5b0b578ad"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其一九",
        "preface": "",
        "paragraphs": [
            "为爱岁寒枝，呼童旋插篱。",
            "花清吟思觉，香冷梦魂知。",
            "雪月成仙韵，风烟带野姿。",
            "江天暮云合，千里故人思。"
        ],
        "notes": "",
        "id": "c8b09e782c3d51d14d1612b2d5cd49a8"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "池州和同官咏梅花  其二○",
        "preface": "",
        "paragraphs": [
            "村墅苔为径，茅檐竹作篱。",
            "神清和月写，香远隔烟知。",
            "老树有余韵，别花无此姿。",
            "诗人风味似，梦寐也应思。"
        ],
        "notes": "",
        "id": "b5b6179ba0e133b9b87bab180d23c78c"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一",
        "preface": "",
        "paragraphs": [
            "和靖风流百世长，吟魂依旧化幽芳。",
            "已枯半树风烟古，才放一花天地香。",
            "不肯面随春冷暖，只将影共月行藏。",
            "悬知骨法清如许，传得仙人服玉方。"
        ],
        "notes": "",
        "id": "6d4f4ebe898db8a53279cde25d484a7e"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其二",
        "preface": "",
        "paragraphs": [
            "数花疏疏静处芳，便成佳景不荒凉。",
            "暖回穷谷春常早，影落寒溪水也香。",
            "自倚风流高格调，唯消质素淡衣裳。",
            "满天霜月花边宿，无复庄周蝶梦狂。"
        ],
        "notes": "",
        "id": "3266905953bd15fa8d0ef25d9896dbc9"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其三",
        "preface": "",
        "paragraphs": [
            "泠泠涧水石桥傍，春正浓时风味长。",
            "清介终持孤竹操，繁华不梦百花场。",
            "描来月地前生瘦，吹落风簷到死香。",
            "结习已空无染著，每来花下辄成狂。"
        ],
        "notes": "",
        "id": "39a696e9a0f9ab7edf1144c8d35eda4f"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其四",
        "preface": "",
        "paragraphs": [
            "冻花无多树更孤，一溪霜月照清癯。",
            "终身只友竹君子，雅志绝羞松大夫。",
            "白玉都拚雕作蕊，黄金不惜撚为须。",
            "亦知世有春风伴，问万花中著得无。"
        ],
        "notes": "",
        "id": "19863309cacd862dca27abb537931375"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其五",
        "preface": "",
        "paragraphs": [
            "孤芳嫌杀浑群芳，雪满山坳月满塘。",
            "韵士不随今世态，仙姝犹作古时粧。",
            "雪羞洁白常回避，春忌清高不主张。",
            "地僻何妨绝供给，饥来只用咽寒香。"
        ],
        "notes": "",
        "id": "3742eb6d9c2cc9f3df11023f535511d8"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其六",
        "preface": "",
        "paragraphs": [
            "政尔寒阴惨淡时，忽逢孤艳映疏篱。",
            "金紫气味无人识，玉雪襟怀只自知。",
            "竹屋纸窗清不俗，茶瓯禅榻两相宜。",
            "花边不敢高声语，羌管凄凉更忍吹。"
        ],
        "notes": "",
        "id": "324c3e6ef4c56426a3a2ac12affb89d2"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其七",
        "preface": "",
        "paragraphs": [
            "千林冻损积阴凝，一点春从底处生。",
            "玉色独钟天地正，铁心不受雪霜惊。",
            "孤芳若与东君背，数树能令南纪明。",
            "醉后惟愁踏花影，青鞋不敢近花行。"
        ],
        "notes": "",
        "id": "981c280b12ac5b3370a253cf01ca7709"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其八",
        "preface": "",
        "paragraphs": [
            "数株如玉照寒塘，无日无风自在香。",
            "谷冷难教春管领，山深自共雪商量。",
            "已成到骨诗家瘦，不卖入时宫样粧。",
            "乱插繁花花下醉，只应我似放翁狂。"
        ],
        "notes": "",
        "id": "d0037c1fc2796ea9081175bb21dd8a97"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其九",
        "preface": "",
        "paragraphs": [
            "行尽荒林一径苔，竹稍深处数枝开。",
            "绝知南雪羞相并，欲嫁东风耻自媒。",
            "无主野桥随月管，有根寒谷也春回。",
            "醉余不睡庭前地，只恐忽吹花落来。"
        ],
        "notes": "",
        "id": "10ce21ed274a37fc659c1e040f273340"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一○",
        "preface": "",
        "paragraphs": [
            "天然标格阆风乡，薄薄铅华淡淡粧。",
            "月地向谁孤弄影，雪天蓦地忽闻香。",
            "征鞍处处频回首，羌管声声欲断肠。",
            "天上玉妃新谪堕，游蜂不敢近花傍。"
        ],
        "notes": "",
        "id": "e3da0ab14e45a3ca74ce35554640e7e5"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一一",
        "preface": "",
        "paragraphs": [
            "才有梅花便自奇，清香分付入新诗。",
            "闲持杯酒临风处，独倚栏干待月时。",
            "试向园林千万树，何如篱落两三枝。",
            "霜天角里空哀怨，丘壑风流总不知。"
        ],
        "notes": "",
        "id": "e19f4ae17d42f24e2872c66ad736563e"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一二",
        "preface": "",
        "paragraphs": [
            "才有梅花便自清，孤山两句一条冰。",
            "问渠紫陌花间客，得似清溪树下僧。",
            "雅淡久无兰作伴，孤高惟有竹为朋。",
            "雪天枝上三更月，人在瑶台第几层。"
        ],
        "notes": "",
        "id": "c6360c441eca9145e496eb8f67cefa02"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一三",
        "preface": "",
        "paragraphs": [
            "天寒花信未能灵，伫立通宵户不扃。",
            "小萼欲争天下白，数条独向雪中青。",
            "肯教旅雁寻常见，未许游蜂取次经。",
            "一片唯愁汚尘土，寒苕和月扫中庭。"
        ],
        "notes": "",
        "id": "b583fedc3bf993b45c77a54adffb27c9"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一四",
        "preface": "",
        "paragraphs": [
            "才有梅花便不尘，和霜和月为精神。",
            "风流晋宋之间客，清旷羲皇以上人。",
            "年后腊前无尽意，水边林下自然春。",
            "万花锦绣东风閙，难浼翛翛玉雪身。"
        ],
        "notes": "",
        "id": "91990ff03c3a14ea52eb5ce36c9c97a6"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一五",
        "preface": "",
        "paragraphs": [
            "才有梅花便不村，其人如玉立黄昏。",
            "些些蕊里藏风韵，个个枝头带月魂。",
            "常挹清香来燕坐，可教落片点空樽。",
            "到腰深雪庭前白，心事寒松拟共论。"
        ],
        "notes": "",
        "id": "8c4694e3b01a61f0ceda56eb0f6afa25"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一六",
        "preface": "",
        "paragraphs": [
            "才有梅花便不同，一年清致雪霜中。",
            "疏疏篱落娟娟月，寂寂轩窗淡淡风。",
            "生长元从琼玉圃，安排合在水晶宫。",
            "何须更探春消息，自有幽香梦里通。"
        ],
        "notes": "",
        "id": "141b53e35ede7ac37ce34057d816b2e5"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一七",
        "preface": "",
        "paragraphs": [
            "天下无花白到梅，风前和我不尘埃。",
            "崚嶒鹤骨霜中立，偃蹇龙身雪里来。",
            "未许琼花为行辈，定教玉蕊作舆台。",
            "夜深立尽扶疏影，一路清溪踏月回。"
        ],
        "notes": "",
        "id": "e956d46efc658fee4075d216c80432aa"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一八",
        "preface": "",
        "paragraphs": [
            "几年冷树雪封骨，一夜东风春透怀。",
            "花里清含仙韵度，人中癯似我形骸。",
            "三点两点淡尤好，十枝五枝疏更佳。",
            "野意终多官意少，玉堂茅舍任安排。"
        ],
        "notes": "",
        "id": "e82afca12b2046f12108968aa956f014"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其一九",
        "preface": "",
        "paragraphs": [
            "爱疏爱淡爱枯枝，已爱梅花更爱奇。",
            "江路一年春好处，石桥半夜月明时。",
            "蹇馿积雪深须去，破帽严霜打不知。",
            "世上非无好颜色，诗人所赏是风姿。"
        ],
        "notes": "",
        "id": "ed6c14b71813c3af896acc2e92cb6373"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花二十首  其二○",
        "preface": "",
        "paragraphs": [
            "癖爱梅花不可医，开教探早落教迟。",
            "欲知无限春风意，尽在相将暮雪时。",
            "竹屿烟深寻得巧，茅簷月淡立成痴。",
            "梦骖鸾鹤相寻去，题徧江南寺寺诗。"
        ],
        "notes": "",
        "id": "c7b457302065a5ed293bd7ad97c2730c"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一",
        "preface": "",
        "paragraphs": [
            "夜月轻描玉样身，枉拚心力唤真真。",
            "藤床几度劳清梦，竹屋相看是故人。",
            "玉雪襟期惟我共，金兰气味更谁亲。",
            "一清頼有天知己，多少春花逐后尘。"
        ],
        "notes": "",
        "id": "79f3108eba1ae7e8d01deb92f179f1db"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其二",
        "preface": "",
        "paragraphs": [
            "南堂深处向阳身，第一东风信息真。",
            "树冷花清无俗韵，天寒日暮有佳人。",
            "林逋眼目才能识，何逊心情独见亲。",
            "养就枝头金鼎实，千花陌上已成尘。"
        ],
        "notes": "",
        "id": "8acf5f4e8394b0cbbec0d4c840f32486"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其三",
        "preface": "",
        "paragraphs": [
            "洒落胸襟不受尘，水沈为骨玉为神。",
            "直疑天上浑无雪，不许人闲别有春。",
            "驱遣山蜂离境界，招呼野鹤作比邻。",
            "分明清入仙臞格，人似孤山始得真。"
        ],
        "notes": "",
        "id": "acb303507744fcdb786b44268a729947"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其四",
        "preface": "",
        "paragraphs": [
            "清白家风不染尘，冰霜气骨玉精神。",
            "腊前恰见无多蕊，天下还他第一春。",
            "香在雪天孤有韵，影于月地四无邻。",
            "逃禅未到天然处，笔底如何写得真。"
        ],
        "notes": "",
        "id": "34d21d331a0352af8ad97cd341bd9313"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其五",
        "preface": "",
        "paragraphs": [
            "雪白都无半点尘，多应滕六即花神。",
            "孤根欲老冰霜国，一朵尽收天下春。",
            "流水断桥谁是主，寒松疎竹合为邻。",
            "寥寥不见古颜色，澹月微云到本真。"
        ],
        "notes": "",
        "id": "751a7ee4bc876c3b97de78d5480b05f7"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其六",
        "preface": "",
        "paragraphs": [
            "万雪千霜百岁枯，月中不数桂婆娑。",
            "香来野墅深深径，影落寒池浅浅波。",
            "众木凋零愁冻折，一枝的皪得春多。",
            "题诗未有惊人句，输与风流人姓何。"
        ],
        "notes": "",
        "id": "9c9fa1ad946b9d1ea4cc5f5649c72be0"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其七",
        "preface": "",
        "paragraphs": [
            "白玉花头碧玉柯，半枯安得树婆娑。",
            "云垂江路天将雪，霜滑溪桥水不波。",
            "林杪冬穷群木落，岭头春蚤一枝多。",
            "夜深立尽窗前月，其奈此兄风味何。"
        ],
        "notes": "",
        "id": "2a45b2f358e8746538e4d093d3b7bdfc"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其八",
        "preface": "",
        "paragraphs": [
            "雪虐风饕铁石柯，终然疎瘦欠婆娑。",
            "清姿雅澹嫌霜粉，疎影横斜浸月波。",
            "幽树百年浑未老，好春数点不须多。",
            "西湖处士风流远，善识花人更几何。"
        ],
        "notes": "",
        "id": "2fcd183e54db5025b9d320c2acb5e309"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其九",
        "preface": "",
        "paragraphs": [
            "藓护苔封白玉柯，沍寒蓦地转阳和。",
            "任枯半树韵尤古，才放一花春已多。",
            "神倩满庭明月写，香随隔水晚风过。",
            "古今真爱梅人少，愿祀诗家始祖何。"
        ],
        "notes": "",
        "id": "33234a0e9bdb7cf6a924576fe1cbbd15"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一○",
        "preface": "",
        "paragraphs": [
            "高标胜韵点人寒，残雪疎篱是水南。",
            "天上玉妃居第一，人闲石友结成三。",
            "广平心事今犹在，和靖家风晚最谙。",
            "莫讶清晨将鹤出，孤筇行过竹西庵。"
        ],
        "notes": "",
        "id": "5224ec375dc24efb76c905761521257e"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一一",
        "preface": "",
        "paragraphs": [
            "蓓蕾梢头破晚寒，风流无限涧之南。",
            "俯看春艳相千万，微挂月痕初二三。",
            "岩桂清应居此下，山桃素不与吾谙。",
            "一枝昨夜垂垂白，知在前村水竹庵。"
        ],
        "notes": "",
        "id": "f148975cd00e466348b54e070d0d465a"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一二",
        "preface": "",
        "paragraphs": [
            "莹彻无瑕照眼寒，无边春意满溪南。",
            "绝疑飞雪吹花六，独自临风遶树三。",
            "夜月襟怀谁与共，岁寒情味久相谙。",
            "软红尘土应无此，竹外清湍竹里庵。"
        ],
        "notes": "",
        "id": "6623b4f2929391c4f6b170b38f06e80c"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一三",
        "preface": "",
        "paragraphs": [
            "才有梅花便不凡，半依月涧半霜岩。",
            "斜欹独树疑龙卧，细啄残英笑鹤馋。",
            "雪里仙姿谁解写，腊前春信可容缄。",
            "孤山遗下十四字，千载一条冰样衔。"
        ],
        "notes": "",
        "id": "220645e5aad44c72aa5b616bd88583f5"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一四",
        "preface": "",
        "paragraphs": [
            "穷腊无花可入诗，何人翦玉著南枝。",
            "香浮风细月明夜，春在天寒日暮时。",
            "玉面亭亭成独立，冰心皎皎竟谁知。",
            "眼明数点窗前白，梦见鲁山元紫芝。"
        ],
        "notes": "",
        "id": "ebb5981a4aad396f315a871a1689fb2e"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一五",
        "preface": "",
        "paragraphs": [
            "兀坐蒲团眼辄惊，数花点得一窗明。",
            "月边再见古颜色，雪里独含春性情。",
            "骨格三生传岛瘦，风标万世说夷清。",
            "夜霜竹屋寒无奈，一笑持杯寿此兄。"
        ],
        "notes": "",
        "id": "6231350f123da7799b7a29233f761db7"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一六",
        "preface": "",
        "paragraphs": [
            "皎然玉色耿无邻，回首千花总后尘。",
            "数树罗浮山下月，一枝大庾岭头春。",
            "与时无竞抱幽质，于我有情真故人。",
            "众志独清天不管，自临野水照精神。"
        ],
        "notes": "",
        "id": "8274971a312204c3aaba290094eab279"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一七",
        "preface": "",
        "paragraphs": [
            "冰氏根苗出涸阴，一团玉雪缟千林。",
            "百年幽树古香澹，数个疎花春意深。",
            "俗眼共怜西子态，世人谁识广平心。",
            "孤芳不入骚经里，物色分留待我吟。"
        ],
        "notes": "",
        "id": "4b0949a40fe395cf4015174924937a71"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一八",
        "preface": "",
        "paragraphs": [
            "落尽精英宇宙荒，天将清白付幽芳。",
            "万花场里春无色，独树桥边月有香。",
            "怪怪奇奇依水石，潇潇洒洒带冰霜。",
            "琼肤耐得寒如许，薄薄轻绡翦作裳。"
        ],
        "notes": "",
        "id": "0e568d182d61bd3a30e0236a21595d2f"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其一九",
        "preface": "",
        "paragraphs": [
            "越疎越澹越幽奇，最是精神与月宜。",
            "残雪轩窗看到晓，冷烟篱落立移时。",
            "寄将水驿传千里，折得寒檐第几枝。",
            "只为花清诗自别，此花之外更无诗。"
        ],
        "notes": "",
        "id": "1d7507254febbeeba97b570c5af29859"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其二○",
        "preface": "",
        "paragraphs": [
            "花中气节老逾刚，万木摧残独自芳。",
            "天女不生凡意想，月娥偏尚澹梳妆。",
            "精神冰玉前生瘦，骨髓栴檀抵死香。",
            "说到调和天下口，轻轻桃杏敢承当。"
        ],
        "notes": "",
        "id": "1ccbcf27da0dbfaf7d986bca6babe068"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其二一",
        "preface": "",
        "paragraphs": [
            "子真世裔神仙种，和靖家风冰玉身。",
            "只此玲珑三数蕊，不知收拾几多春。",
            "香浮静夜天知己，影浸寒池月写真。",
            "留待楚骚千载恨，花中谁识独醒人。"
        ],
        "notes": "",
        "id": "2f3247b8bcbe9246ac66ed905a65f04b"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其二二",
        "preface": "",
        "paragraphs": [
            "天姿雅澹似高人，万树千红总后尘。",
            "花蚤花迟两年景，枝南枝北一家春。",
            "轻轻欲折还休去，得得相看不厌频。",
            "坐到参横浑未晓，夜窗无月更传神。"
        ],
        "notes": "",
        "id": "6ebbb6916aaf67a2c926e868a99d77da"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其二三",
        "preface": "",
        "paragraphs": [
            "乾坤收拾几精华，全付茅茨小小家。",
            "压尽园林千万蕊，只消篱落两三花。",
            "雪消数尺暗埋岭，月浸一痕微印沙。",
            "玉面亭亭太幽独，天寒空谷正堪嗟。"
        ],
        "notes": "",
        "id": "361658f9680495e54e73c3224440545a"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "梅花七律  其二四",
        "preface": "",
        "paragraphs": [
            "百余年树半苍苔，才有一枝花便佳。",
            "腊近已含春意思，香寒足慰晚情怀。",
            "澹烟孤寺藏山崦，残雪疎篱傍水涯。",
            "刚被幽香相触拨，消磨石路几青鞵。"
        ],
        "notes": "",
        "id": "433173993fe31e9220e72f2b2324fcc3"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "岭梅",
        "preface": "",
        "paragraphs": [
            "到处皆诗境，随时有物华。",
            "应酬都不暇，一岭是梅花。"
        ],
        "notes": "",
        "id": "4808294ce5c1263fcdd0742fb375de8b"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "千叶梅",
        "preface": "",
        "paragraphs": [
            "应怯春寒悄，重重叠叠衣。",
            "本来飞燕瘦，翻作玉环肥。"
        ],
        "notes": "",
        "id": "e8986c702edec1989ee0db35e33c117d"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "照水梅",
        "preface": "",
        "paragraphs": [
            "照影寒溪水，溪中水也香。",
            "佳人临宝鉴，自作寿阳妆。"
        ],
        "notes": "",
        "id": "72823d8c03a3f03bd40a9cf9824e48a7"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "瓶梅",
        "preface": "",
        "paragraphs": [
            "春在胆瓶闲，朝看复暮看。",
            "小屏遮护著，不怕玉肌寒。"
        ],
        "notes": "",
        "id": "d5be7800b5ef27977221b7baaac4a5b9"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "寻梅",
        "preface": "",
        "paragraphs": [
            "澹澹一痕月，疎疎数个花。",
            "春风也公道，先到野人家。"
        ],
        "notes": "",
        "id": "c228328f0f1ae663c36cd035a5834516"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "折梅",
        "preface": "",
        "paragraphs": [
            "日暮天寒小驿亭，竹边流水浅泠泠。",
            "一枝蚤落诗人手，无恨春风入胆瓶。"
        ],
        "notes": "",
        "id": "a229bb4325bb7bd617da714231b5ace2"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嗅梅",
        "preface": "",
        "paragraphs": [
            "一夜吹香过别村，瘴江洗尽水云昏。",
            "真堪持作维摩供，鼻观微参透顶门。"
        ],
        "notes": "",
        "id": "224b368957af840c1f82ebe1283e6e7f"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "访梅",
        "preface": "",
        "paragraphs": [
            "梅花欲放遶溪行，隔水香来分外清。",
            "拄杖过桥寻欲遍，竹林疎处数花明。"
        ],
        "notes": "",
        "id": "e0bb27bee94c585017f1cd4436f75af8"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "忆梅",
        "preface": "",
        "paragraphs": [
            "到得芳天閙众葩，相思一夜鬓成华。",
            "阆风归去无消息，梦寐犹应见此花。"
        ],
        "notes": "",
        "id": "df7963f9dbb3cf4007679e72985b39d9"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "墨梅",
        "preface": "",
        "paragraphs": [
            "一枝的的照人寒，绝胜溪桥立马看。",
            "祇恐东风解相怨，漏他消息入毫端。"
        ],
        "notes": "",
        "id": "e8e49dbff2e53e521d25ba7778372574"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "瓶梅",
        "preface": "",
        "paragraphs": [
            "寒水一瓶春数枝，清香不减小溪时。",
            "横斜烛底无人见，莫与微云澹月知。"
        ],
        "notes": "",
        "id": "4d83810523d88d693ed258450442c332"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "见梅",
        "preface": "",
        "paragraphs": [
            "寒梅冲雪领春回，桃李纷纷趁脚来。",
            "一点阳和无尽处，从头分与百花开。"
        ],
        "notes": "",
        "id": "d24f966c386f28fbf20a1b5a2b2c723f"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "寻梅",
        "preface": "",
        "paragraphs": [
            "平生春兴十分深，长恐梅花负赏心。",
            "偶有一枝斜照水，前村踏雪也须寻。"
        ],
        "notes": "",
        "id": "94b720d71edc11c97d2413201a4c4cdb"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "对梅  其一",
        "preface": "",
        "paragraphs": [
            "老年不著梅花眼，得见梅花子细看。",
            "我不似梅梅似我，风流心事一般般。"
        ],
        "notes": "",
        "id": "09f1e99c679386bfea264e6af96f44a8"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "对梅  其二",
        "preface": "",
        "paragraphs": [
            "一点春风天正冬，不妨花下倒金钟。",
            "扳条拣蕊轻轻折，只嗅清香醉杀侬。"
        ],
        "notes": "",
        "id": "36d792cdff4de300808675194296d462"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "对梅  其三",
        "preface": "",
        "paragraphs": [
            "数点枝头黏白玉，一年春意动黄钟。",
            "为渠拚受风流罪，只恐风流不到侬。"
        ],
        "notes": "",
        "id": "c16ce443491a8e6a8fe29556d2fbcb15"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "对梅  其四",
        "preface": "",
        "paragraphs": [
            "秋水娟娟隔美人，江东日暮几重云。",
            "孤灯竹屋霜清夜，梦到梅花即见君。"
        ],
        "notes": "",
        "id": "ae7c3412b3655cf7bc43524c0fdaac67"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一",
        "preface": "",
        "paragraphs": [
            "百余年树未为古，三四点花何限春。",
            "欲折一枝无处赠，世间少有识花人。"
        ],
        "notes": "",
        "id": "7ba0181969064247b08137f60c0f1f3e"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其二",
        "preface": "",
        "paragraphs": [
            "此兄自是风尘表，半在山巅半水涯。",
            "老子怕寒浑不出，夜窗明月看横斜。"
        ],
        "notes": "",
        "id": "fe1f4c70ae65c11379abaeebfad54dd4"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其三",
        "preface": "",
        "paragraphs": [
            "疎花清晓嫩寒时，粉蝶霜禽总不知。",
            "未有风流如此老，为渠费尽雪桥诗。"
        ],
        "notes": "",
        "id": "d68c883cc9ef360338177fb334c92519"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其四",
        "preface": "",
        "paragraphs": [
            "风流百世林和靖，洒落三生杨补之。",
            "一自阆风归去后，几回展画又观诗。"
        ],
        "notes": "",
        "id": "33c2fbce32441e7aa432e9baaa055aec"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其五",
        "preface": "",
        "paragraphs": [
            "夜深月透小窗纱，梦到瑶池阿母家。",
            "急遣山僮开户看，不知是雪是梅花。"
        ],
        "notes": "",
        "id": "ddf83aeaf0c607e6bd867debb4279d84"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其六",
        "preface": "",
        "paragraphs": [
            "孤山风味梅花句，先落人闲万古香。",
            "应笑老夫无一语，千回寂寞遶花傍。"
        ],
        "notes": "",
        "id": "e23a9957c82604878e15b99b293519b3"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其七",
        "preface": "",
        "paragraphs": [
            "三生种植得梅缘，每见梅花辄欲颠。",
            "清梦化为蝴蝶去，也应只在此花边。"
        ],
        "notes": "",
        "id": "0e55fe65af01dcac8dc770c2f1783622"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其八",
        "preface": "",
        "paragraphs": [
            "昔年诗担过罗浮，曾见梅花一夕留。",
            "翠凤几回清夜梦，月明还挂旧枝头。"
        ],
        "notes": "",
        "id": "67a0f0a630454448a4e3524f01cfd2e7"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其九",
        "preface": "",
        "paragraphs": [
            "茶灶熏炉与笔床，黄昏待月在东厢。",
            "无人知得花清处，影落寒池水亦香。"
        ],
        "notes": "",
        "id": "4f9b7f3ab8482678b7335012c7b1ea5e"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一○",
        "preface": "",
        "paragraphs": [
            "不是梅花契分深，与谁共话岁寒心。",
            "隔溪竹外无人见，自有香来不用寻。"
        ],
        "notes": "",
        "id": "7b1c1f94a9c4a5999d7482303340a841"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一一",
        "preface": "",
        "paragraphs": [
            "万里闲关庾岭回，岭头曾见此花来。",
            "远游何似在家好，屋角玲珑一树开。"
        ],
        "notes": "",
        "id": "d5e262d2d7ab93635ee285b917aae860"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一二",
        "preface": "",
        "paragraphs": [
            "数点疎花不满枝，倚栏惟恨月明迟。",
            "风流一段无人共，正是霜飞屋角时。"
        ],
        "notes": "",
        "id": "e2a401e18b6d2fc1bc0e489e8e05a76c"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一三",
        "preface": "",
        "paragraphs": [
            "小小茅茨野老家，不随桃李竞春华。",
            "欲知格韵真孤绝，秃尽千林始一花。"
        ],
        "notes": "",
        "id": "06ca931b5da601c9be7a03e7f6ca1d28"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一四",
        "preface": "",
        "paragraphs": [
            "正是天荒地老时，芙蓉憔悴菊离披。",
            "呼童试探溪南路，春到寒梅第几枝。"
        ],
        "notes": "",
        "id": "d6ffa82a97884650dbdb722cbbc4e3fc"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "咏梅杂诗  其一五",
        "preface": "",
        "paragraphs": [
            "南枝太蚤北枝迟，非蚤非迟恰好时。",
            "竹屋一痕微月照，老夫树下正吟诗。"
        ],
        "notes": "",
        "id": "701976ba4f822f8d585ce1615fa241d4"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和方虚谷豫章徐子苍沿制檄抵秋浦",
        "preface": "",
        "paragraphs": [
            "词林日凋疎，生意不复新。",
            "慎勿大崛奇，崛奇惊世人。",
            "玉堂老仙坡，过海走一巡。",
            "非天寔罚之，语言累其身。"
        ],
        "notes": "",
        "id": "2abed1ea813139105592ac4b99b8dfc4"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "包山书院",
        "preface": "",
        "paragraphs": [
            "衔命龙荒万里余，归寻水竹与同居。",
            "早从洙泗传心学，晚辟包山教子书。",
            "细草幽花香笔砚，清风明月满庭除。",
            "一生宇宙皆春意，此乐颜鲁亦自如。"
        ],
        "notes": "",
        "id": "358f4666662feb30b3c630bee6c10ae3"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "句  其一",
        "preface": "",
        "paragraphs": [
            "前生薝葡林中梦，到死旃檀国里香。"
        ],
        "notes": "",
        "id": "b08c21cf47b0f507d22f7ff39a9b7884"
    },
    {
        "author": "张道洽",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "句  其二",
        "preface": "",
        "paragraphs": [
            "早缘服玉肌能白，不为薰衣骨亦香。"
        ],
        "notes": "",
        "id": "4487b7b380ef3ce7fb1bd1dcb16ded77"
    }
]