[
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 胥山",
        "preface": "",
        "paragraphs": [
            "马革浮尸去，君王太忍人。",
            "此山空庙貌，何以劝忠臣。"
        ],
        "notes": "",
        "id": "7f847937c85f7411f48c47b521440e27"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 殳山",
        "preface": "",
        "paragraphs": [
            "丹灶功成后，寥寥掩竹扉。",
            "但闻蝉已蜕，不见鹤来归。"
        ],
        "notes": "",
        "id": "dc0fff255fabe1dedd14ad8698618523"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 滮湖",
        "preface": "",
        "paragraphs": [
            "四境田相接，烟澜自渺弥。",
            "客来吟此景，无或比滮池。"
        ],
        "notes": "",
        "id": "0e95564e3d0c2db783e88da90ae4158f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 鸳鸯湖",
        "preface": "",
        "paragraphs": [
            "东西两湖水，相并比鸳鸯。",
            "湖里鸳鸯鸟，双双锦翼长。"
        ],
        "notes": "",
        "id": "62c9581e35cd8e0f6558165c27186012"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 长水乡",
        "preface": "",
        "paragraphs": [
            "古来名未灭，人好水还清。",
            "一读高僧疏，尘劳悟此生。"
        ],
        "notes": "",
        "id": "9d75b985ae09e66e7051244e6355f096"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 谷水",
        "preface": "",
        "paragraphs": [
            "短棹经行处，风披藻荇香。",
            "中宵孤鹤唳，片月在沧浪。"
        ],
        "notes": "",
        "id": "c0791128bf988fd9971464c3f5fd6e43"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 天星河",
        "preface": "",
        "paragraphs": [
            "拍岸浮春渌，菰蒲四远生。",
            "网罗人不到，鱼鸟亦忘情。"
        ],
        "notes": "",
        "id": "e2a02f93315fb77a0ff320b19d639335"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 穆溪",
        "preface": "",
        "paragraphs": [
            "静练明田外，源流笠泽通。",
            "不因逢岁旱，谁识济时功。"
        ],
        "notes": "",
        "id": "2a0396e52c5c3066422c9eb0a3841ba8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 韭溪",
        "preface": "",
        "paragraphs": [
            "终与双溪接，分流入郭来。",
            "市桥人影合，不解洗尘埃。"
        ],
        "notes": "",
        "id": "6dc4b2065380e5d21533bef48903b08c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 双溪",
        "preface": "",
        "paragraphs": [
            "柔橹声相应，名齐汉魏收。",
            "可怜一溪水，分作两塘流。"
        ],
        "notes": "",
        "id": "f91a105d9ca565ddd211c2a2c69cdd48"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 秀水",
        "preface": "",
        "paragraphs": [
            "好景明于昼，长浮五色波。",
            "一竿吾欲钓，来此听渔歌。"
        ],
        "notes": "",
        "id": "740f7712b610f405e97765b924749abb"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白龙潭",
        "preface": "",
        "paragraphs": [
            "呼吸风云合，飞潜自有时。",
            "只愁潭底浅，头角被人窥。"
        ],
        "notes": "",
        "id": "0a074164a499ab45206899116d543941"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 御儿",
        "preface": "",
        "paragraphs": [
            "用此临吴战，何人为越谋。",
            "夫差曾不寤，亡国始知羞。"
        ],
        "notes": "",
        "id": "3c454294c02ee31a1c9f89e1cdd5ead9"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 范蠡湖",
        "preface": "",
        "paragraphs": [
            "少伯曾居此，螺纹吐彩丝。",
            "一奁秋镜好，犹可照西施。"
        ],
        "notes": "",
        "id": "f8a9099246e445d47c7efb020d590c20"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 梧桐泾",
        "preface": "",
        "paragraphs": [
            "落落梧桐树，何年作凤鸣。",
            "试看千古翠，流尽一溪声。"
        ],
        "notes": "",
        "id": "cc61552de00da7237c73cd172dd09902"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 汾湖",
        "preface": "",
        "paragraphs": [
            "我是沧浪叟，闲来系钓艭。",
            "如何一湖水，丰秀半吴江。"
        ],
        "notes": "",
        "id": "edbecc37cd4f90d2902e83d1fbaaa47c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 柘湖",
        "preface": "",
        "paragraphs": [
            "神居阴阳护，寻国捍洪波。",
            "莫虑蛟龙怒，年来畏叱呵。"
        ],
        "notes": "",
        "id": "8777904b0e418a6db83a45c67520492b"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 马塘泾",
        "preface": "",
        "paragraphs": [
            "疑著风波恶，民谣亦畏人。",
            "筑塘成堰后，斩马祭河神。"
        ],
        "notes": "",
        "id": "ab11a3c61ff96ca961b0d87c82783a0a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 杉青堰",
        "preface": "",
        "paragraphs": [
            "旧吏家何在，空寻古柳隄。",
            "世情今亦尔，莫笑买臣妻。"
        ],
        "notes": "",
        "id": "5f07d2175f42c9c2885646638343417b"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 凤凰洲",
        "preface": "",
        "paragraphs": [
            "竹秀水色浄，忽来双凤凰。",
            "岐周昔鸣瑞，今复见文王。"
        ],
        "notes": "",
        "id": "7498f27d5232f98738f6adc4c3d724ad"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 龙舌头",
        "preface": "",
        "paragraphs": [
            "一吸长川静，烟云忽有无。",
            "谁看颔底月，误欲探骊珠。"
        ],
        "notes": "",
        "id": "09f78b9f919d0bea791c3f5ad74b1067"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 月壕",
        "preface": "",
        "paragraphs": [
            "入径深于坞，沧波近马蹄。",
            "每寻莲社友，咫尺走东西。"
        ],
        "notes": "",
        "id": "f918854daee2a0b88faa88a22a6e4f72"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 灵光井",
        "preface": "",
        "paragraphs": [
            "玉甃祥光发，难藏世上名。",
            "定多慈护力，一饮百疴轻。"
        ],
        "notes": "",
        "id": "a3bf894cd9f48f658c4dfaa67db2c656"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 幽澜泉",
        "preface": "",
        "paragraphs": [
            "神女钟灵处，真堪疗渴羌。",
            "满甖秋玉色，一酌洒清凉。"
        ],
        "notes": "",
        "id": "733c46d89d1858612096104d2af12593"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 槜李城",
        "preface": "",
        "paragraphs": [
            "螳螂方捕楚，黄雀遽乘吴。",
            "交怨终亡国，君王到死愚。"
        ],
        "notes": "",
        "id": "47d8f1dd5c5743ce420916d1aacd42e1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 射襄城",
        "preface": "",
        "paragraphs": [
            "此地连江海，曾经古战争。",
            "干戈今不见，空有射襄名。"
        ],
        "notes": "",
        "id": "69dbe45280bac7ee37a42a0372fa3f51"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 四城",
        "preface": "",
        "paragraphs": [
            "吴越争雄日，区区在用兵。",
            "空余争战地，无处不高城。"
        ],
        "notes": "",
        "id": "601300326189af6f9c3864ac104febdd"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 何城",
        "preface": "",
        "paragraphs": [
            "报越心何壮，贪齐计已轻。",
            "忠言殊不听，何用筑斯城。"
        ],
        "notes": "",
        "id": "58206d8896220bb67721c14d26d356f2"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 由拳废县",
        "preface": "",
        "paragraphs": [
            "故城人不见，击剑泪空沾。",
            "一绝悲声远，今犹说孝廉。"
        ],
        "notes": "",
        "id": "830c7ad98bde0c14639a0deb36232649"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 嘉禾墩",
        "preface": "",
        "paragraphs": [
            "寂寞黄龙后，纷然垅上碑。",
            "废兴无处问，空咏草离离。"
        ],
        "notes": "",
        "id": "813efdaeb282838662d95b5ec591aaa6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 古战场",
        "preface": "",
        "paragraphs": [
            "自昔干戈地，城空草自荒。",
            "渔樵怀旧事，何敢议兴亡。"
        ],
        "notes": "",
        "id": "5dce19a48da97d1dd5a3b4e5b09a1d9a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 村门口",
        "preface": "",
        "paragraphs": [
            "昔日荒闲地，今闻市井喧。",
            "人情看促迫，无复似当年。"
        ],
        "notes": "",
        "id": "3fbe17e4cb1cc8caf8958eed015d4d61"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 宣公桥",
        "preface": "",
        "paragraphs": [
            "祗因裴少保，功业败垂成。",
            "往往桥边客，今犹恨未平。"
        ],
        "notes": "",
        "id": "ded1bb7dd161552a6d72f38ed2b2c626"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 五柳桥",
        "preface": "",
        "paragraphs": [
            "为怀陶靖节，无复见其人。",
            "谁种桥边柳，犹含旧日春。"
        ],
        "notes": "",
        "id": "4524f858fe53a6f67129e64030d20e21"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 百步桥",
        "preface": "",
        "paragraphs": [
            "旧址依然在，荒墟未百年。",
            "还分一桥水，流入几家田。"
        ],
        "notes": "",
        "id": "ed6574d7192790c0332c89b89aeb3142"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 跨塘桥",
        "preface": "",
        "paragraphs": [
            "路接张泾近，塘连谷水长。",
            "一声清鹤唳，片月在沧浪。"
        ],
        "notes": "",
        "id": "dd5258fbc83032c4a51b32b92c83d342"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 菩萨桥",
        "preface": "",
        "paragraphs": [
            "片云初出岫，南北自飞扬。",
            "旧日桥边路，犹闻舄履香。"
        ],
        "notes": "",
        "id": "c598f453a05b326ac7ea1f44ccdb33c8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 竹桥",
        "preface": "",
        "paragraphs": [
            "腰枕政亭北，横斜古渡头。",
            "风吹双泪落，恨逐水东流。"
        ],
        "notes": "",
        "id": "691425e67b27772c16022b1f8d18f1ca"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 河内亭",
        "preface": "",
        "paragraphs": [
            "掾死缘非命，儿来哭未休。",
            "旧时亭下泪，疑与水长流。"
        ],
        "notes": "",
        "id": "c66811e229bc2230eeca81fe02320681"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 嘉禾亭",
        "preface": "",
        "paragraphs": [
            "五马来何暮，经营指一弹。",
            "石碑书近事，留与后人看。"
        ],
        "notes": "",
        "id": "0b08f5c0982cb93350f7b597fac78569"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 列岫亭",
        "preface": "",
        "paragraphs": [
            "吾州风物好，惟是欠青山。",
            "忽有洞庭色，来从一笑间。"
        ],
        "notes": "",
        "id": "4969e354160ac0539af7da9833ef30d7"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 水心亭",
        "preface": "",
        "paragraphs": [
            "波光磨不尽，镜面绝纤尘。",
            "照得红亭子，亭亭似美人。"
        ],
        "notes": "",
        "id": "f58accaf948a7da4a77076dd8ef34000"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 乐郊亭",
        "preface": "",
        "paragraphs": [
            "竹下清风好，时来挂幅巾。",
            "待逢田叟问，知是乐郊人。"
        ],
        "notes": "",
        "id": "06645482157da32dd2ddefd80da2c017"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 会景亭",
        "preface": "",
        "paragraphs": [
            "有景道不出，须来此水滨。",
            "十分花鸟色，团作一亭春。"
        ],
        "notes": "",
        "id": "f49649d46e40d86aea3b9e59e4a33ef9"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 南坞",
        "preface": "",
        "paragraphs": [
            "岸回波曲曲，城转草层层。",
            "忽见桃花落，还知到武陵。"
        ],
        "notes": "",
        "id": "dee959b31ff4103b2ea17cd7e774f699"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 海棠亭",
        "preface": "",
        "paragraphs": [
            "藓砌迎烟渚，花开忽满株。",
            "春风吹烂漫，如展蜀川图。"
        ],
        "notes": "",
        "id": "6e5424f3e5a8d10d04c8c57ec32bdbfe"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白莲沼",
        "preface": "",
        "paragraphs": [
            "沧波围四面，艳艳玉开花。",
            "自可除帘幕，清香不用遮。"
        ],
        "notes": "",
        "id": "40d75ffa12e0c92586aa4912ff8e322f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 桃花亭",
        "preface": "",
        "paragraphs": [
            "不待东风拆，池塘已自春。",
            "试看源上客，犹恐是秦人。"
        ],
        "notes": "",
        "id": "4f59363943d4bd11c4a53c85cc1bcd60"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 红薇迳",
        "preface": "",
        "paragraphs": [
            "渐放秋风晚，花无烂漫香。",
            "浅深红似锦，散影在沧浪。"
        ],
        "notes": "",
        "id": "6ef354d6737a76dfd52078a07096be81"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 茶溪",
        "preface": "",
        "paragraphs": [
            "茶林那复有，零落付樵人。",
            "旧日溪边叟，空悲二月春。"
        ],
        "notes": "",
        "id": "54c17ebc1dc550125440241e76028fef"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 仙鹤亭",
        "preface": "",
        "paragraphs": [
            "杳杳云间路，重重隔翠林。",
            "数声清夜后，来伴月边吟。"
        ],
        "notes": "",
        "id": "fe547c3c9298d7fdeea7579d6cf7706d"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 芙蓉塘",
        "preface": "",
        "paragraphs": [
            "一发连千丈，红云出素波。",
            "秋风吹欲老，无奈落花何。"
        ],
        "notes": "",
        "id": "b9dfeb001ad2c79c777422a4f02b76bb"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白苎桥",
        "preface": "",
        "paragraphs": [
            "遍陇敷青绿，桑麻共几家。",
            "满城罗绮子，应笑不栽花。"
        ],
        "notes": "",
        "id": "816a2277d0ceef81bae5523c66880fa3"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 渔溆",
        "preface": "",
        "paragraphs": [
            "野水连天阔，沧波引钓缗。",
            "寄言溪上叟，莫学直钩人。"
        ],
        "notes": "",
        "id": "34d45fd6010095a4700ee0a38d3cb07e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 怀苏亭",
        "preface": "",
        "paragraphs": [
            "犹使樽前客，常怀没后名。",
            "好风吹远籁，如有笑歌声。"
        ],
        "notes": "",
        "id": "7dfb005ebbfc59efed23738b14473596"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 披云阁",
        "preface": "",
        "paragraphs": [
            "兹楼当胜地，高望极崚嶒。",
            "欲覩青天色，还须上一层。"
        ],
        "notes": "",
        "id": "b2a32d0801188a83b08bc8bba95e942a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 景龙楼",
        "preface": "",
        "paragraphs": [
            "千秋传异彩，百尺护雕阑。",
            "欲识金龙瑞，丹霄试仰看。"
        ],
        "notes": "",
        "id": "e27362e159e1b0024d4ce7ec59f2fc2b"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 烟雨楼",
        "preface": "",
        "paragraphs": [
            "妙手谁烘染，梳烟沐雨姿。",
            "一声长笛晚，人在倚楼时。"
        ],
        "notes": "",
        "id": "04bb75c68fce14e30c4b9bf671edf757"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 月波楼",
        "preface": "",
        "paragraphs": [
            "天阔冰壶浄，溪平玉鉴寒。",
            "卷帘看晚色，鸥鸟并阑干。"
        ],
        "notes": "",
        "id": "30cf89049c16dc573004f9cc5517a8bd"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 真如院",
        "preface": "",
        "paragraphs": [
            "田家轻脱屣，聊结浄人缘。",
            "影外香灯寂，清风百世传。"
        ],
        "notes": "",
        "id": "701f485315ef3f70c1f54cb6aebec9ed"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 雪峰庵",
        "preface": "",
        "paragraphs": [
            "一语合领悟，师承岂在身。",
            "戏言庐墓客，未是报恩人。"
        ],
        "notes": "",
        "id": "391c0355e6c4b6f74e73805df2856d4e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 景德禅院",
        "preface": "",
        "paragraphs": [
            "白毫光未灭，胜事百年中。",
            "好是传灯地，林泉有古风。"
        ],
        "notes": "",
        "id": "73077f18317c4b1d625bcdc22e9a270c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 三塔",
        "preface": "",
        "paragraphs": [
            "石匮埋神物，灵光照夜多。",
            "龙归天上后，无复更风波。"
        ],
        "notes": "",
        "id": "3f3226a51fc99e56bc38453540a17ce6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 学绣塔",
        "preface": "",
        "paragraphs": [
            "孤塔岧峣立，犹传学绣人。",
            "五湖如好在，针线不胜春。"
        ],
        "notes": "",
        "id": "f51d26a6a01ade879fd85161c447bd73"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 水陆院",
        "preface": "",
        "paragraphs": [
            "白昼重门寂，苍苔古殿深。",
            "老僧香火罢，松柏夜森森。"
        ],
        "notes": "",
        "id": "3fd43bae1907e7e2533a2f90ad226df6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 泗洲庵",
        "preface": "",
        "paragraphs": [
            "旧寺丘墟后，重兴岁月赊。",
            "莫将闲祸福，容易托僧伽。"
        ],
        "notes": "",
        "id": "b58f6ae0d9c27f6f06a28c6cdb969c91"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 云门家舍",
        "preface": "",
        "paragraphs": [
            "满箧藏锋句，家传世几何。",
            "远孙无悟解，却笑葛藤多。"
        ],
        "notes": "",
        "id": "10c70f360cb3bda2428c4d71bcfb3df2"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白莲上方",
        "preface": "",
        "paragraphs": [
            "古屋深于洞，门前种白莲。",
            "好寻当日社，重结此生缘。"
        ],
        "notes": "",
        "id": "a287ff3d0d9af5319e2d6cced08514c1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 普济院",
        "preface": "",
        "paragraphs": [
            "祗应曾布地，往往化为鱼。",
            "一遇丁千骑，禅人便结庐。"
        ],
        "notes": "",
        "id": "e53a2e3a9c7e89e49d5bf3174778b6c3"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 兜率寺",
        "preface": "",
        "paragraphs": [
            "昔年池满地，今见屋耽耽。",
            "不结莲花界，龟鱼死未甘。"
        ],
        "notes": "",
        "id": "a8c48941ce9c59622229c39a20d5306d"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 姜庵",
        "preface": "",
        "paragraphs": [
            "不入红尘市，安居三十年。",
            "至今庵下路，芳草碧芊芊。"
        ],
        "notes": "",
        "id": "4a7aa45319e8bc1418f8271ed567610c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 惠明堂",
        "preface": "",
        "paragraphs": [
            "地僻无人到，松风鹤梦清。",
            "一灯禅榻畔，自向暗中明。"
        ],
        "notes": "",
        "id": "1e2a38981f49032b780c51ff35f1929a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 三过堂",
        "preface": "",
        "paragraphs": [
            "题诗三过此，坡老昔时曾。",
            "悟得成禅指，来为打睡僧。"
        ],
        "notes": "",
        "id": "4b16bad4d46fb13a13bdbe0bfe35faba"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 寂照庵",
        "preface": "",
        "paragraphs": [
            "寂灭跏趺地，灯光照四时。",
            "高僧穿地脉，此意少人知。"
        ],
        "notes": "",
        "id": "ad1c3f3ddd36704bbef92e61b77e1886"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 天庆观",
        "preface": "",
        "paragraphs": [
            "寥阳龙已化，华表鹤空悲。",
            "欲问梁朝事，惟应老桧知。"
        ],
        "notes": "",
        "id": "ab194c13c72f28a1f5abb1efa8bcd6d0"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 福顺庙",
        "preface": "",
        "paragraphs": [
            "老屋无丹雘，空阶只翠苔。",
            "遶檐看箭镞，谁记赤眉来。"
        ],
        "notes": "",
        "id": "deeb169875d21b84a91b078aef542603"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 昌武庙",
        "preface": "",
        "paragraphs": [
            "祗因吴士季，千载仰公神。",
            "欲借驱痁术，来临刻木人。"
        ],
        "notes": "",
        "id": "a960ba32e4e6c593c419595dc08a3eaa"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 杨姑庙",
        "preface": "",
        "paragraphs": [
            "孝德襄陵久，天然与众殊。",
            "生男不能养，应合愧杨姑。"
        ],
        "notes": "",
        "id": "6f568bad41a185479e488041634c7725"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 徐偃王庙",
        "preface": "",
        "paragraphs": [
            "王有岐夷德，千年貌亦存。",
            "我怀三刺史，谁是后来孙。"
        ],
        "notes": "",
        "id": "09975b0388763358f43f5be7b833e75c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 曹使君祠",
        "preface": "",
        "paragraphs": [
            "一笑淮兵却，功成未易论。",
            "岂惭颜刺史，诗酒在平原。"
        ],
        "notes": "",
        "id": "f376bf70c00e001186c9fa247d785567"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 高使君祠",
        "preface": "",
        "paragraphs": [
            "一瞑移藩日，邦人昔被恩。",
            "步兵碑尚在，功业敢经纶。"
        ],
        "notes": "",
        "id": "c3be039fde71277739db0c11937e6cf8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 寥阳殿",
        "preface": "",
        "paragraphs": [
            "人归天上去，空识旧衣冠。",
            "秘录藏金匮，清风满殿寒。"
        ],
        "notes": "",
        "id": "a1a3c0f383d06265eea10bfd44ac7e2f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 霍将军庙",
        "preface": "",
        "paragraphs": [
            "弼汉功非浅，谗夫可奈何。",
            "海神今亦畏，安敢布风波。"
        ],
        "notes": "",
        "id": "4c882870260acaa5032bcb2c419b06bc"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 读书堆",
        "preface": "",
        "paragraphs": [
            "平林标大道，曾是野王居。",
            "往事将谁语，凄凉六代余。"
        ],
        "notes": "",
        "id": "f6a298ddb6134768db2bffddc048fac9"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 简斋",
        "preface": "",
        "paragraphs": [
            "诗老追唐句，闲居筑简斋。",
            "芙蓉寒浦外，红落感秋怀。"
        ],
        "notes": "",
        "id": "df40a52599cd0e9c343ecb1be1523a7c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 陈贤良隠居",
        "preface": "",
        "paragraphs": [
            "发策名犹在，回头事已非。",
            "池塘春草绿，空忆谢公归。"
        ],
        "notes": "",
        "id": "4396bbd17cb307695330fb9785950a3f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 徐长者园",
        "preface": "",
        "paragraphs": [
            "丹砂犹未就，白发忽成翁。",
            "却有怜春意，栽花向此中。"
        ],
        "notes": "",
        "id": "9328565747a14f7dca2b96a3c9e13ef1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 赵老园",
        "preface": "",
        "paragraphs": [
            "藏书几万卷，归老此林泉。",
            "不为寻莼鲙，于公亦有贤。"
        ],
        "notes": "",
        "id": "424083621c925b61cf2f89e6afc50f55"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 施墓",
        "preface": "",
        "paragraphs": [
            "苍苔侵坏道，晚雀散空林。",
            "莫问人贤否，浮生一古今。"
        ],
        "notes": "",
        "id": "04dec44b261e01d8e50bcd98a5a920bb"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 卫墓",
        "preface": "",
        "paragraphs": [
            "惆怅黎阳客，飘流浙水滨。",
            "定忘空腹恨，知是赤心人。"
        ],
        "notes": "",
        "id": "533d3562888a3772ecbe6bc8f63a3c7e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 刘伶墓",
        "preface": "",
        "paragraphs": [
            "荷锸从吾后，叮咛死便埋。",
            "人皆讥嗜酒，我独爱忘怀。"
        ],
        "notes": "",
        "id": "74c877d177cdc86d9ac4c74b8ae29214"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 严忌墓",
        "preface": "",
        "paragraphs": [
            "有子如君少，终缘叛国亡。",
            "冢前无后嗣，切勿怨张汤。"
        ],
        "notes": "",
        "id": "e3106b756dce32e4be8185c6f12d8e5f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 朱买臣墓",
        "preface": "",
        "paragraphs": [
            "世事春风转，荣枯一梦间。",
            "绣衣人不见，孤冢旧家山。"
        ],
        "notes": "",
        "id": "9f069947241bb13fe416e39b335ccd3f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 周都官墓",
        "preface": "",
        "paragraphs": [
            "不识君名字，应难问子孙。",
            "一丘浮水际，神怪欲谁论。"
        ],
        "notes": "",
        "id": "c640e56f089aa2c1cdf00a2bbc45698a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 苏小小墓",
        "preface": "",
        "paragraphs": [
            "泉下骨应朽，幽魂独未消。",
            "几番清夜月，孤影度南桥。"
        ],
        "notes": "",
        "id": "65510bdf38f078d62d5d565e02cda380"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 羞墓",
        "preface": "",
        "paragraphs": [
            "伤哉负薪子，五十始登朝。",
            "空使埋羞处，山花复采樵。"
        ],
        "notes": "",
        "id": "69490b295657802bd9872a56fb406977"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 木纹观音",
        "preface": "",
        "paragraphs": [
            "双瞳分日月，眉际一星悬。",
            "照见人间苦，三辰不在天。"
        ],
        "notes": "",
        "id": "851793168b455ae60b29c8daea38c547"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 邦药王龛",
        "preface": "",
        "paragraphs": [
            "父老怀遗爱，谁传肘后方。",
            "药囊同日葬，空忆紫元香。"
        ],
        "notes": "",
        "id": "51a186eceb26f64fe7268655f2bc0fb7"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 浄相佳李",
        "preface": "",
        "paragraphs": [
            "地重因名果，如分沆瀣浆。",
            "伤心吴越战，未敢尽情尝。"
        ],
        "notes": "",
        "id": "932376effbf555d77343e00d9ffafc9f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 符姥瑞竹",
        "preface": "",
        "paragraphs": [
            "一幅霜缣外，青青出自然。",
            "天机何敢议，珍重白衣仙。"
        ],
        "notes": "",
        "id": "d266d8d395e381c1d14cc72a5bfa9fc1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "徐家庄",
        "preface": "",
        "paragraphs": [
            "节义穷方见，高名荐玉京。",
            "一封天子敕，千载故乡荣。"
        ],
        "notes": "",
        "id": "90a256dd08c2e04d9f20a821086506de"
    }
]