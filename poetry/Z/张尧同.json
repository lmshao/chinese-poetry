[
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 胥山",
        "preface": "",
        "paragraphs": [
            "马革浮尸去，君王太忍人。",
            "此山空庙貌，何以劝忠臣。"
        ],
        "notes": "",
        "id": "4c5b229ac4aaac29b1bf2c4e5c74004e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 殳山",
        "preface": "",
        "paragraphs": [
            "丹灶功成后，寥寥掩竹扉。",
            "但闻蝉已蜕，不见鹤来归。"
        ],
        "notes": "",
        "id": "81d4b9b9a7adb762488ecd60f1983c47"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 滮湖",
        "preface": "",
        "paragraphs": [
            "四境田相接，烟澜自渺弥。",
            "客来吟此景，无或比滮池。"
        ],
        "notes": "",
        "id": "3facdd51c86939a78a218d2afca6a742"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 鸳鸯湖",
        "preface": "",
        "paragraphs": [
            "东西两湖水，相并比鸳鸯。",
            "湖里鸳鸯鸟，双双锦翼长。"
        ],
        "notes": "",
        "id": "6294658758705416c90434ea41a60d95"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 长水乡",
        "preface": "",
        "paragraphs": [
            "古来名未灭，人好水还清。",
            "一读高僧疏，尘劳悟此生。"
        ],
        "notes": "",
        "id": "f926638cef18c1ad8e1178e302cbc437"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 谷水",
        "preface": "",
        "paragraphs": [
            "短棹经行处，风披藻荇香。",
            "中宵孤鹤唳，片月在沧浪。"
        ],
        "notes": "",
        "id": "83380023ff9a7cfbd8d5fbc247389b25"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 天星河",
        "preface": "",
        "paragraphs": [
            "拍岸浮春渌，菰蒲四远生。",
            "网罗人不到，鱼鸟亦忘情。"
        ],
        "notes": "",
        "id": "b94e2db1b15251190b5b2472f8318b0f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 穆溪",
        "preface": "",
        "paragraphs": [
            "静练明田外，源流笠泽通。",
            "不因逢岁旱，谁识济时功。"
        ],
        "notes": "",
        "id": "245a2739559a8879483d302f606b9281"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 韭溪",
        "preface": "",
        "paragraphs": [
            "终与双溪接，分流入郭来。",
            "市桥人影合，不解洗尘埃。"
        ],
        "notes": "",
        "id": "9f7e13bad7265a22bff64f154daed39c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 双溪",
        "preface": "",
        "paragraphs": [
            "柔橹声相应，名齐汉魏收。",
            "可怜一溪水，分作两塘流。"
        ],
        "notes": "",
        "id": "597680803bd69680a22e7f67d56d1c35"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 秀水",
        "preface": "",
        "paragraphs": [
            "好景明于昼，长浮五色波。",
            "一竿吾欲钓，来此听渔歌。"
        ],
        "notes": "",
        "id": "0dcd2d74224fe5975c292ef3b7a81d75"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白龙潭",
        "preface": "",
        "paragraphs": [
            "呼吸风云合，飞潜自有时。",
            "只愁潭底浅，头角被人窥。"
        ],
        "notes": "",
        "id": "44d0a31033e5a9d41b7b9dd9d57a788e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 御儿",
        "preface": "",
        "paragraphs": [
            "用此临吴战，何人为越谋。",
            "夫差曾不寤，亡国始知羞。"
        ],
        "notes": "",
        "id": "dbdf1618520a0af7b07e950510454f67"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 范蠡湖",
        "preface": "",
        "paragraphs": [
            "少伯曾居此，螺纹吐彩丝。",
            "一奁秋镜好，犹可照西施。"
        ],
        "notes": "",
        "id": "0530e305572b2836ec888d568c6dedc8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 梧桐泾",
        "preface": "",
        "paragraphs": [
            "落落梧桐树，何年作凤鸣。",
            "试看千古翠，流尽一溪声。"
        ],
        "notes": "",
        "id": "8808f262d0d41aba314a2e0453e6cc19"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 汾湖",
        "preface": "",
        "paragraphs": [
            "我是沧浪叟，闲来系钓艭。",
            "如何一湖水，丰秀半吴江。"
        ],
        "notes": "",
        "id": "9d0c4d5ad1026c5a569c15b3fbc0e5b0"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 柘湖",
        "preface": "",
        "paragraphs": [
            "神居阴阳护，寻国捍洪波。",
            "莫虑蛟龙怒，年来畏叱呵。"
        ],
        "notes": "",
        "id": "426c95cc304e3c906696caa7d954ceb4"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 马塘泾",
        "preface": "",
        "paragraphs": [
            "疑著风波恶，民谣亦畏人。",
            "筑塘成堰后，斩马祭河神。"
        ],
        "notes": "",
        "id": "f73237e679b0914182b8268d4f384645"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 杉青堰",
        "preface": "",
        "paragraphs": [
            "旧吏家何在，空寻古柳隄。",
            "世情今亦尔，莫笑买臣妻。"
        ],
        "notes": "",
        "id": "c0085297214c34ff7be639709e88232c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 凤凰洲",
        "preface": "",
        "paragraphs": [
            "竹秀水色浄，忽来双凤凰。",
            "岐周昔鸣瑞，今复见文王。"
        ],
        "notes": "",
        "id": "4cfc6f119552fc7237d445b29c5d8ef0"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 龙舌头",
        "preface": "",
        "paragraphs": [
            "一吸长川静，烟云忽有无。",
            "谁看颔底月，误欲探骊珠。"
        ],
        "notes": "",
        "id": "46dfff865da58fd959c299bb9d409a30"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 月壕",
        "preface": "",
        "paragraphs": [
            "入径深于坞，沧波近马蹄。",
            "每寻莲社友，咫尺走东西。"
        ],
        "notes": "",
        "id": "c90a2c3585b6439014dc47b8f96c380f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 灵光井",
        "preface": "",
        "paragraphs": [
            "玉甃祥光发，难藏世上名。",
            "定多慈护力，一饮百疴轻。"
        ],
        "notes": "",
        "id": "ab23d4c34d56831bb8fef1859f430320"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 幽澜泉",
        "preface": "",
        "paragraphs": [
            "神女钟灵处，真堪疗渴羌。",
            "满甖秋玉色，一酌洒清凉。"
        ],
        "notes": "",
        "id": "63a6c652098bbf4a2029a23cd1ca38ff"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 槜李城",
        "preface": "",
        "paragraphs": [
            "螳螂方捕楚，黄雀遽乘吴。",
            "交怨终亡国，君王到死愚。"
        ],
        "notes": "",
        "id": "23969acf727b80ff3413cb3a9a6b9534"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 射襄城",
        "preface": "",
        "paragraphs": [
            "此地连江海，曾经古战争。",
            "干戈今不见，空有射襄名。"
        ],
        "notes": "",
        "id": "6d877e3780f2aea9719c621a1c9b289b"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 四城",
        "preface": "",
        "paragraphs": [
            "吴越争雄日，区区在用兵。",
            "空余争战地，无处不高城。"
        ],
        "notes": "",
        "id": "9223564a8ab4dd81b031d4285bb03675"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 何城",
        "preface": "",
        "paragraphs": [
            "报越心何壮，贪齐计已轻。",
            "忠言殊不听，何用筑斯城。"
        ],
        "notes": "",
        "id": "a21fa4ac70111b41fae9b18aa20c11f3"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 由拳废县",
        "preface": "",
        "paragraphs": [
            "故城人不见，击剑泪空沾。",
            "一绝悲声远，今犹说孝廉。"
        ],
        "notes": "",
        "id": "456c2f64ee3d81c3636289287de515c8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 嘉禾墩",
        "preface": "",
        "paragraphs": [
            "寂寞黄龙后，纷然垅上碑。",
            "废兴无处问，空咏草离离。"
        ],
        "notes": "",
        "id": "60e5268e94dc91a7c9de7315cdf5dbe0"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 古战场",
        "preface": "",
        "paragraphs": [
            "自昔干戈地，城空草自荒。",
            "渔樵怀旧事，何敢议兴亡。"
        ],
        "notes": "",
        "id": "40731c60d157b134637621070dc87986"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 村门口",
        "preface": "",
        "paragraphs": [
            "昔日荒闲地，今闻市井喧。",
            "人情看促迫，无复似当年。"
        ],
        "notes": "",
        "id": "51f46919c5928f3f3faebb87f8334716"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 宣公桥",
        "preface": "",
        "paragraphs": [
            "祗因裴少保，功业败垂成。",
            "往往桥边客，今犹恨未平。"
        ],
        "notes": "",
        "id": "be634ee02f94b90a6ade1d3d2c1c1a3f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 五柳桥",
        "preface": "",
        "paragraphs": [
            "为怀陶靖节，无复见其人。",
            "谁种桥边柳，犹含旧日春。"
        ],
        "notes": "",
        "id": "e49c00f8dba26d56176e8ccca344465c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 百步桥",
        "preface": "",
        "paragraphs": [
            "旧址依然在，荒墟未百年。",
            "还分一桥水，流入几家田。"
        ],
        "notes": "",
        "id": "54633e21fa27d5f6d20952574ea60a78"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 跨塘桥",
        "preface": "",
        "paragraphs": [
            "路接张泾近，塘连谷水长。",
            "一声清鹤唳，片月在沧浪。"
        ],
        "notes": "",
        "id": "1cdeabea52fa54fd7e2db5f49ee6dafc"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 菩萨桥",
        "preface": "",
        "paragraphs": [
            "片云初出岫，南北自飞扬。",
            "旧日桥边路，犹闻舄履香。"
        ],
        "notes": "",
        "id": "e3515a950dcdea21bd4df43d3e3cf0f6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 竹桥",
        "preface": "",
        "paragraphs": [
            "腰枕政亭北，横斜古渡头。",
            "风吹双泪落，恨逐水东流。"
        ],
        "notes": "",
        "id": "cbd947ca951d52780ddb2c4b8c6256d8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 河内亭",
        "preface": "",
        "paragraphs": [
            "掾死缘非命，儿来哭未休。",
            "旧时亭下泪，疑与水长流。"
        ],
        "notes": "",
        "id": "c5006a1d81fa8a4496711e949c604033"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 嘉禾亭",
        "preface": "",
        "paragraphs": [
            "五马来何暮，经营指一弹。",
            "石碑书近事，留与后人看。"
        ],
        "notes": "",
        "id": "08bc6f416456514ab94e935618a3aea3"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 列岫亭",
        "preface": "",
        "paragraphs": [
            "吾州风物好，惟是欠青山。",
            "忽有洞庭色，来从一笑间。"
        ],
        "notes": "",
        "id": "322b012f55a79768a1666adfc893d1e1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 水心亭",
        "preface": "",
        "paragraphs": [
            "波光磨不尽，镜面绝纤尘。",
            "照得红亭子，亭亭似美人。"
        ],
        "notes": "",
        "id": "ee8df933621ecb9416edc838fc49d1cb"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 乐郊亭",
        "preface": "",
        "paragraphs": [
            "竹下清风好，时来挂幅巾。",
            "待逢田叟问，知是乐郊人。"
        ],
        "notes": "",
        "id": "b45c0e895432a287ec6b9f67af76162c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 会景亭",
        "preface": "",
        "paragraphs": [
            "有景道不出，须来此水滨。",
            "十分花鸟色，团作一亭春。"
        ],
        "notes": "",
        "id": "7c7d7311b44f19dcbae10df3a4ba8be5"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 南坞",
        "preface": "",
        "paragraphs": [
            "岸回波曲曲，城转草层层。",
            "忽见桃花落，还知到武陵。"
        ],
        "notes": "",
        "id": "d468e625bf6bf49c4919638f8b209535"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 海棠亭",
        "preface": "",
        "paragraphs": [
            "藓砌迎烟渚，花开忽满株。",
            "春风吹烂漫，如展蜀川图。"
        ],
        "notes": "",
        "id": "b3fb0e6e4c42c7de0f66d7487d5590c3"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白莲沼",
        "preface": "",
        "paragraphs": [
            "沧波围四面，艳艳玉开花。",
            "自可除帘幕，清香不用遮。"
        ],
        "notes": "",
        "id": "dd391a690ce6386f828cecc97768600d"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 桃花亭",
        "preface": "",
        "paragraphs": [
            "不待东风拆，池塘已自春。",
            "试看源上客，犹恐是秦人。"
        ],
        "notes": "",
        "id": "2ce29919a230911b3106caee6a8a6b7d"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 红薇迳",
        "preface": "",
        "paragraphs": [
            "渐放秋风晚，花无烂漫香。",
            "浅深红似锦，散影在沧浪。"
        ],
        "notes": "",
        "id": "410d7abeff00e308f07fd177d4b358e8"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 茶溪",
        "preface": "",
        "paragraphs": [
            "茶林那复有，零落付樵人。",
            "旧日溪边叟，空悲二月春。"
        ],
        "notes": "",
        "id": "df02aede7adfd632d6bce2338b5839bf"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 仙鹤亭",
        "preface": "",
        "paragraphs": [
            "杳杳云间路，重重隔翠林。",
            "数声清夜后，来伴月边吟。"
        ],
        "notes": "",
        "id": "c0ebc7b863361f486a42ff55e9770673"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 芙蓉塘",
        "preface": "",
        "paragraphs": [
            "一发连千丈，红云出素波。",
            "秋风吹欲老，无奈落花何。"
        ],
        "notes": "",
        "id": "ea6de7c70de77f4860b30beb00afad08"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白苎桥",
        "preface": "",
        "paragraphs": [
            "遍陇敷青绿，桑麻共几家。",
            "满城罗绮子，应笑不栽花。"
        ],
        "notes": "",
        "id": "65da9b23a87dd0370ba8ff2d7a2dc33b"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 渔溆",
        "preface": "",
        "paragraphs": [
            "野水连天阔，沧波引钓缗。",
            "寄言溪上叟，莫学直钩人。"
        ],
        "notes": "",
        "id": "9f9b21bfbc626e10c44cff4bc1a21222"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 怀苏亭",
        "preface": "",
        "paragraphs": [
            "犹使樽前客，常怀没后名。",
            "好风吹远籁，如有笑歌声。"
        ],
        "notes": "",
        "id": "43a0994a03cd6077c7687da0f0d2777c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 披云阁",
        "preface": "",
        "paragraphs": [
            "兹楼当胜地，高望极崚嶒。",
            "欲覩青天色，还须上一层。"
        ],
        "notes": "",
        "id": "e2557e7325a9635ee783f4bcd067db62"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 景龙楼",
        "preface": "",
        "paragraphs": [
            "千秋传异彩，百尺护雕阑。",
            "欲识金龙瑞，丹霄试仰看。"
        ],
        "notes": "",
        "id": "c7148c3d69a965ee6aba1be9c36fadd7"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 烟雨楼",
        "preface": "",
        "paragraphs": [
            "妙手谁烘染，梳烟沐雨姿。",
            "一声长笛晚，人在倚楼时。"
        ],
        "notes": "",
        "id": "7a1cfa7cd00f016d7609e17b49fb85e9"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 月波楼",
        "preface": "",
        "paragraphs": [
            "天阔冰壶浄，溪平玉鉴寒。",
            "卷帘看晚色，鸥鸟并阑干。"
        ],
        "notes": "",
        "id": "1ad4ca1339fd3f67279f8b769e4d544f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 真如院",
        "preface": "",
        "paragraphs": [
            "田家轻脱屣，聊结浄人缘。",
            "影外香灯寂，清风百世传。"
        ],
        "notes": "",
        "id": "cedb87c2498fba295b1fcbefc639e697"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 雪峰庵",
        "preface": "",
        "paragraphs": [
            "一语合领悟，师承岂在身。",
            "戏言庐墓客，未是报恩人。"
        ],
        "notes": "",
        "id": "80828b9a038d7f507a64cebfbb36d8a1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 景德禅院",
        "preface": "",
        "paragraphs": [
            "白毫光未灭，胜事百年中。",
            "好是传灯地，林泉有古风。"
        ],
        "notes": "",
        "id": "1ff761a7a2ed6e1262a0e0b501f6ec6e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 三塔",
        "preface": "",
        "paragraphs": [
            "石匮埋神物，灵光照夜多。",
            "龙归天上后，无复更风波。"
        ],
        "notes": "",
        "id": "f95d38027c0f0925dbe2ef2e924e0d1a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 学绣塔",
        "preface": "",
        "paragraphs": [
            "孤塔岧峣立，犹传学绣人。",
            "五湖如好在，针线不胜春。"
        ],
        "notes": "",
        "id": "76d0744187cfdde1241784a6403efe54"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 水陆院",
        "preface": "",
        "paragraphs": [
            "白昼重门寂，苍苔古殿深。",
            "老僧香火罢，松柏夜森森。"
        ],
        "notes": "",
        "id": "4b23935a9069e9d839da391de54edb84"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 泗洲庵",
        "preface": "",
        "paragraphs": [
            "旧寺丘墟后，重兴岁月赊。",
            "莫将闲祸福，容易托僧伽。"
        ],
        "notes": "",
        "id": "26816632d90271c7cd76fa75878224e9"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 云门家舍",
        "preface": "",
        "paragraphs": [
            "满箧藏锋句，家传世几何。",
            "远孙无悟解，却笑葛藤多。"
        ],
        "notes": "",
        "id": "d12fe9722b38d4555a88f0076864e07a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 白莲上方",
        "preface": "",
        "paragraphs": [
            "古屋深于洞，门前种白莲。",
            "好寻当日社，重结此生缘。"
        ],
        "notes": "",
        "id": "6802bcc696a7266d76dcf8ba6e28c2bc"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 普济院",
        "preface": "",
        "paragraphs": [
            "祗应曾布地，往往化为鱼。",
            "一遇丁千骑，禅人便结庐。"
        ],
        "notes": "",
        "id": "88dc21ea67a85fa5483af21deb0e72a6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 兜率寺",
        "preface": "",
        "paragraphs": [
            "昔年池满地，今见屋耽耽。",
            "不结莲花界，龟鱼死未甘。"
        ],
        "notes": "",
        "id": "94848e4e8d1562241698e4797a04a345"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 姜庵",
        "preface": "",
        "paragraphs": [
            "不入红尘市，安居三十年。",
            "至今庵下路，芳草碧芊芊。"
        ],
        "notes": "",
        "id": "8fbbc1669f4fae6cd20bc247a1f1e78c"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 惠明堂",
        "preface": "",
        "paragraphs": [
            "地僻无人到，松风鹤梦清。",
            "一灯禅榻畔，自向暗中明。"
        ],
        "notes": "",
        "id": "bda7d860ee4ccd4644f5f02f30928f32"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 三过堂",
        "preface": "",
        "paragraphs": [
            "题诗三过此，坡老昔时曾。",
            "悟得成禅指，来为打睡僧。"
        ],
        "notes": "",
        "id": "08296ae79e6423a83681024c5448205a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 寂照庵",
        "preface": "",
        "paragraphs": [
            "寂灭跏趺地，灯光照四时。",
            "高僧穿地脉，此意少人知。"
        ],
        "notes": "",
        "id": "95ba0a972a90987c42e8fa4791695f23"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 天庆观",
        "preface": "",
        "paragraphs": [
            "寥阳龙已化，华表鹤空悲。",
            "欲问梁朝事，惟应老桧知。"
        ],
        "notes": "",
        "id": "523149320a85146209ec09aee679cd1e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 福顺庙",
        "preface": "",
        "paragraphs": [
            "老屋无丹雘，空阶只翠苔。",
            "遶檐看箭镞，谁记赤眉来。"
        ],
        "notes": "",
        "id": "27cee1b06a833f2e995328ccb5adddce"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 昌武庙",
        "preface": "",
        "paragraphs": [
            "祗因吴士季，千载仰公神。",
            "欲借驱痁术，来临刻木人。"
        ],
        "notes": "",
        "id": "18b1cdbe49745ccb38b3c55b2f6af3ab"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 杨姑庙",
        "preface": "",
        "paragraphs": [
            "孝德襄陵久，天然与众殊。",
            "生男不能养，应合愧杨姑。"
        ],
        "notes": "",
        "id": "08225b88e41e3f8aa3212dac466de6d4"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 徐偃王庙",
        "preface": "",
        "paragraphs": [
            "王有岐夷德，千年貌亦存。",
            "我怀三刺史，谁是后来孙。"
        ],
        "notes": "",
        "id": "46215efcb877093e957530d54fe294a6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 曹使君祠",
        "preface": "",
        "paragraphs": [
            "一笑淮兵却，功成未易论。",
            "岂惭颜刺史，诗酒在平原。"
        ],
        "notes": "",
        "id": "92ab5dbc47c40cbb2b5d91cc6b881549"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 高使君祠",
        "preface": "",
        "paragraphs": [
            "一瞑移藩日，邦人昔被恩。",
            "步兵碑尚在，功业敢经纶。"
        ],
        "notes": "",
        "id": "f7ae90ecbb9b04aec92c5c86374d2e7a"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 寥阳殿",
        "preface": "",
        "paragraphs": [
            "人归天上去，空识旧衣冠。",
            "秘录藏金匮，清风满殿寒。"
        ],
        "notes": "",
        "id": "359859887ca697b40edccf0101bccd74"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 霍将军庙",
        "preface": "",
        "paragraphs": [
            "弼汉功非浅，谗夫可奈何。",
            "海神今亦畏，安敢布风波。"
        ],
        "notes": "",
        "id": "9973d8ead3a45053cae94aa465338913"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 读书堆",
        "preface": "",
        "paragraphs": [
            "平林标大道，曾是野王居。",
            "往事将谁语，凄凉六代余。"
        ],
        "notes": "",
        "id": "024a10257c4fb814e4c7479b350b65ed"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 简斋",
        "preface": "",
        "paragraphs": [
            "诗老追唐句，闲居筑简斋。",
            "芙蓉寒浦外，红落感秋怀。"
        ],
        "notes": "",
        "id": "bd9ce6439dcbcec16260b3de582665d3"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 陈贤良隠居",
        "preface": "",
        "paragraphs": [
            "发策名犹在，回头事已非。",
            "池塘春草绿，空忆谢公归。"
        ],
        "notes": "",
        "id": "daafc3ae97fe6542117f6a33238b74a7"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 徐长者园",
        "preface": "",
        "paragraphs": [
            "丹砂犹未就，白发忽成翁。",
            "却有怜春意，栽花向此中。"
        ],
        "notes": "",
        "id": "e15a156e0053dad9b4038431a7ecbef1"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 赵老园",
        "preface": "",
        "paragraphs": [
            "藏书几万卷，归老此林泉。",
            "不为寻莼鲙，于公亦有贤。"
        ],
        "notes": "",
        "id": "ccadee051721d17b3f93f3597316a087"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 施墓",
        "preface": "",
        "paragraphs": [
            "苍苔侵坏道，晚雀散空林。",
            "莫问人贤否，浮生一古今。"
        ],
        "notes": "",
        "id": "127e92e923877416fbaf0cce190768eb"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 卫墓",
        "preface": "",
        "paragraphs": [
            "惆怅黎阳客，飘流浙水滨。",
            "定忘空腹恨，知是赤心人。"
        ],
        "notes": "",
        "id": "c0b34e1e71395c00c246945731d2bce0"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 刘伶墓",
        "preface": "",
        "paragraphs": [
            "荷锸从吾后，叮咛死便埋。",
            "人皆讥嗜酒，我独爱忘怀。"
        ],
        "notes": "",
        "id": "098b5615837b9ba3f008c97d848eca1f"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 严忌墓",
        "preface": "",
        "paragraphs": [
            "有子如君少，终缘叛国亡。",
            "冢前无后嗣，切勿怨张汤。"
        ],
        "notes": "",
        "id": "020bcb5b6d6ece062704bcf09c25dbc0"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 朱买臣墓",
        "preface": "",
        "paragraphs": [
            "世事春风转，荣枯一梦间。",
            "绣衣人不见，孤冢旧家山。"
        ],
        "notes": "",
        "id": "eba30becc7dd065031b499aafee68098"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 周都官墓",
        "preface": "",
        "paragraphs": [
            "不识君名字，应难问子孙。",
            "一丘浮水际，神怪欲谁论。"
        ],
        "notes": "",
        "id": "af59886f31ee6bf8889a72e350930758"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 苏小小墓",
        "preface": "",
        "paragraphs": [
            "泉下骨应朽，幽魂独未消。",
            "几番清夜月，孤影度南桥。"
        ],
        "notes": "",
        "id": "b7d8e4f0da724f75d28b1f144c865b4e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 羞墓",
        "preface": "",
        "paragraphs": [
            "伤哉负薪子，五十始登朝。",
            "空使埋羞处，山花复采樵。"
        ],
        "notes": "",
        "id": "0990844449c3b780ec88fb2166b1f4a6"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 木纹观音",
        "preface": "",
        "paragraphs": [
            "双瞳分日月，眉际一星悬。",
            "照见人间苦，三辰不在天。"
        ],
        "notes": "",
        "id": "436ba434358f10534bfc05a4a6c45ff4"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 邦药王龛",
        "preface": "",
        "paragraphs": [
            "父老怀遗爱，谁传肘后方。",
            "药囊同日葬，空忆紫元香。"
        ],
        "notes": "",
        "id": "5a663f6e2b2f33247e2edf3383fa6029"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 浄相佳李",
        "preface": "",
        "paragraphs": [
            "地重因名果，如分沆瀣浆。",
            "伤心吴越战，未敢尽情尝。"
        ],
        "notes": "",
        "id": "5dbc86cc66e78e8380d15c3b139fdadc"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嘉禾百咏 符姥瑞竹",
        "preface": "",
        "paragraphs": [
            "一幅霜缣外，青青出自然。",
            "天机何敢议，珍重白衣仙。"
        ],
        "notes": "",
        "id": "260f432811bd84cb880baa6e7a2ab01e"
    },
    {
        "author": "张尧同",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "徐家庄",
        "preface": "",
        "paragraphs": [
            "节义穷方见，高名荐玉京。",
            "一封天子敕，千载故乡荣。"
        ],
        "notes": "",
        "id": "1b5563143f652cd5104bc878b8664276"
    }
]