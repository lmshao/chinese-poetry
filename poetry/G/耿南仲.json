[
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思诗呈同院诸公三首  其一",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，闲无一事依。",
            "鬬羊谁得隽，倦鸟客思归。",
            "寝处还相向，过从不患稀。",
            "何当有星聚，谁为问璇玑。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思诗呈同院诸公三首  其二",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，褎然观国光。",
            "泗滨何限磬，岱麓易求梁。",
            "剑气频冲斗，锥锋定出囊。",
            "于余惭拙匠，操尺预量长。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思诗呈同院诸公三首  其三",
        "preface": "",
        "paragraphs": [
            "秋日同文馆，新凉逐雨回。",
            "棘垣婴吏局，蕙帐忆山隈。",
            "射为巧狙发，田因文豹来。",
            "西城随分鎻，夫我室多才。"
        ],
        "notes": "",
        "id": "4e0f10bf656659430d4d277f652cc5d8"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思秋日同文馆诗三首  其一",
        "preface": "",
        "paragraphs": [
            "诏命连催发，英髦一日齐。",
            "周瑜醇似酒，崔远爽于梨。",
            "高馆清无梦，寒灯坐赋题。",
            "还同宿山驿，风雨闭重闺。"
        ],
        "notes": "",
        "id": "2e7f7a8a54d15f6059607615172369f7"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思秋日同文馆诗三首  其二",
        "preface": "",
        "paragraphs": [
            "槐市任花黄，赓歌且辟场。",
            "醒心流水曲，警耳白云章。",
            "冷竹枝枝翠，芳兰叶叶香。",
            "满前皆俊彦，何但有三康。"
        ],
        "notes": "",
        "id": "75c106c4708ac266b403f112e2d5eb7b"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思秋日同文馆诗三首  其三",
        "preface": "",
        "paragraphs": [
            "翠幕春云合，场登看舞鸡。",
            "冥搜人不到，惊盼日沉西。",
            "矒学愁精鉴，英材喜巨题。",
            "何人当首选，双觡起蚖泥。"
        ],
        "notes": "",
        "id": "4cf0db95b1bc37775f93caaa1e818cdc"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思未试即事杂书率用秋日同文馆为首句三首  其一",
        "preface": "",
        "paragraphs": [
            "败叶扫还飞，秋容肃粉闱。",
            "青钱如已选，白璧会须归。",
            "燕坐书横案，孤吟月满衣。",
            "还同潘散骑，鱼鸟性都违。"
        ],
        "notes": "",
        "id": "67c3e3624a4c9b55e29f1d454012c376"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思未试即事杂书率用秋日同文馆为首句三首  其二",
        "preface": "",
        "paragraphs": [
            "大轴累千箱，凫中见鹤长。",
            "静临窓几读，爽入肺肝凉。",
            "夸咤争求胜，疲劳忽自忘。",
            "由来京邑贡，正合冠多方。"
        ],
        "notes": "",
        "id": "80c46709b8eaad64cb9563867a5b735a"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思未试即事杂书率用秋日同文馆为首句三首  其三",
        "preface": "",
        "paragraphs": [
            "蔼蔼儒冠至，恢恢贤路开。",
            "才须金鉴别，人自玉山来。",
            "谢客书千卷，嵇康酒一杯。",
            "如君富术业，当与雉为媒。"
        ],
        "notes": "",
        "id": "5bdfd403f2f2ada51d21ad783c500cac"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思初入试院",
        "preface": "",
        "paragraphs": [
            "昭回云汉九天横，冠带彬彬尽俊英。",
            "梁国有珠光照乘，赵人怀璧价连城。",
            "道山邃处分群彦，文律持来考正声。",
            "秋晚定知谁入彀，叔通还进鲁诸生。"
        ],
        "notes": "",
        "id": "80aaeabba51fac6e447bf46a85a8da11"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和余樗年试院即事呈诸公",
        "preface": "",
        "paragraphs": [
            "重门键钥阻山河，顾有良朋不厌过。",
            "唾落如珠倾巨斛，袂挥成幕散轻罗。",
            "惭分处士吹竽俸，谩学东人掩鼻歌。",
            "笑语哄堂俄又夜，客愁还奈我曹何。"
        ],
        "notes": "",
        "id": "cf96a110612bac752f9b3cc57f74740c"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思重九考罢试卷书呈同院诸公",
        "preface": "",
        "paragraphs": [
            "贡珍已选茂良充，犹被拘縻类䌸钟。",
            "渌酒强陪高馆饮，黄花不似故园逢。",
            "碁枰苦战挑灯坐，藓壁闲题捧砚从。",
            "风叶满庭秋索寞，更筹向尽睡方浓。"
        ],
        "notes": "",
        "id": "3478135aed857080efeb893511b83543"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和邓慎思同舍问及故山景物用钟字韵诗以答",
        "preface": "",
        "paragraphs": [
            "草庐高卧对千峰，白眼谁能顾万锺。",
            "仄席不因天子诏，藏诗未许世人逢。",
            "他年社禝勋如立，归老烟霞志会从。",
            "五柳一松应好在，月明空自结阴浓。"
        ],
        "notes": "",
        "id": "b301fba52070e9befc2e8c018bd24869"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "延祥寺",
        "preface": "",
        "paragraphs": [
            "罗浮仙客隐，准拟访丹台。",
            "为国身将老，逢山眼倍开。",
            "月依松上过，风自竹间来。",
            "僧欲延清话，茶香屡拨灰。"
        ],
        "notes": "",
        "id": "c8305f59ae9b62a36abc53c8525d4174"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "题丹灶  其一",
        "preface": "",
        "paragraphs": [
            "穉川初隐地，山邃枕如城。",
            "宝鼎丹何在，云衣骨亦轻。"
        ],
        "notes": "",
        "id": "e0132bb3bef8778ab40639665f11de7c"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "题丹灶  其二",
        "preface": "",
        "paragraphs": [
            "金鼎交龙虎，当年想闭扃。",
            "一丸丹既就，五色土犹灵。"
        ],
        "notes": "",
        "id": "69899f6e13d63d374250f58d0d75ab67"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "题丹灶  其三",
        "preface": "",
        "paragraphs": [
            "始知勾漏地，犹胜步兵营。",
            "往躅今谁继，游人空慕名。"
        ],
        "notes": "",
        "id": "50ba9d6864f057a2740bbc41b688d692"
    },
    {
        "author": "耿南仲",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "题丹灶  其四",
        "preface": "",
        "paragraphs": [
            "过客空寻访，飞仙已杳冥。",
            "他山多可买，归去读黄庭。"
        ],
        "notes": "",
        "id": "b2680d682a882221602439832393ea9c"
    }
]