[
    {
        "author": "释智策",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "偈",
        "preface": "",
        "paragraphs": [
            "著意忘怀，掘地深埋。",
            "空洞无象，髑髅妄想。",
            "譬如两镜相照，中间早已立象。",
            "直饶东涧水流西涧水，南山烧炭北山红。"
        ],
        "notes": "",
        "id": "9a6dbe1223e102466932f450dbbcacbf"
    },
    {
        "author": "释智策",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "辞众偈",
        "preface": "",
        "paragraphs": [
            "四大既分飞，烟云任意归。",
            "秋天霜夜月，万里转光辉。"
        ],
        "notes": "",
        "id": "bdfb31749cf914d7c6d47653c869afa2"
    }
]