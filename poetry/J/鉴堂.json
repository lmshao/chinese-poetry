[
    {
        "author": "鉴堂",
        "dynasty": "宋",
        "type": "词",
        "rhythmic": "菩萨蛮",
        "title": "",
        "preface": "",
        "paragraphs": [
            "浦南回桨归庭户。",
            "户庭归桨回南浦。",
            "帘卷欲晴天。",
            "天晴欲卷帘。",
            "月光交映雪。",
            "雪映交光月。",
            "残漏怯宵寒。",
            "寒宵怯漏残。"
        ],
        "notes": "",
        "id": "ddd443fe67158208c7052cfadc0f19eb"
    },
    {
        "author": "鉴堂",
        "dynasty": "宋",
        "type": "词",
        "rhythmic": "菩萨蛮",
        "title": "",
        "preface": "",
        "paragraphs": [
            "砌风鸣叶繁霜坠。",
            "坠霜繁叶鸣风砌。",
            "山外水潺潺。",
            "潺潺水外山。",
            "冷衾愁夜永。",
            "永夜愁衾冷。",
            "砧响更蛩吟。",
            "吟蛩更响砧。"
        ],
        "notes": "",
        "id": "b68933b31c300633583d4843de2302ed"
    },
    {
        "author": "鉴堂",
        "dynasty": "宋",
        "type": "词",
        "rhythmic": "菩萨蛮",
        "title": "",
        "preface": "",
        "paragraphs": [
            "竹枝高映荷池绿。",
            "绿池荷映高枝竹。",
            "流水碧浮鸥。",
            "鸥浮碧水流。",
            "酒尊陪旧友。",
            "友旧陪尊酒。",
            "吟客坐时斟。",
            "斟时坐客吟。"
        ],
        "notes": "",
        "id": "2fad191c1bf81db0d028822abec3862b"
    },
    {
        "author": "鉴堂",
        "dynasty": "宋",
        "type": "词",
        "rhythmic": "菩萨蛮",
        "title": "",
        "preface": "",
        "paragraphs": [
            "落花丛外风惊鹊。",
            "鹊惊风外丛花落。",
            "乡梦困时长。",
            "长时困梦乡。",
            "暮天江口渡。",
            "渡口江天暮。",
            "林远度栖禽。",
            "禽栖度远林。"
        ],
        "notes": "",
        "id": "34c48689687c32e6619285722c088d69"
    }
]