[
    {
        "author": "王缙",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "杂曲歌辞 古别离",
        "preface": "",
        "paragraphs": [
            "下阶欲离别，相对映兰丛。",
            "含辞未及吐，泪落兰丛中。",
            "高堂静秋日，罗衣飘暮风。",
            "谁能待明月，回首见床空。"
        ],
        "notes": "",
        "id": "794e59ab42eaafa13ce1331bcef808f2"
    },
