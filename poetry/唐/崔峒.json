[
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "扬州选蒙相公赏判雪后呈上",
        "preface": "",
        "paragraphs": [
            "自得山公许，休畊海上田。",
            "慙看长史传，欲弃钓鱼船。",
            "穷巷殷忧日，芜城雨雪天。",
            "此时瞻相府，心事比旌悬。"
        ],
        "notes": "",
        "id": "85826f86f6f83fa8890b7e2af04dfe76"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "客舍书情寄赵中丞",
        "preface": "",
        "paragraphs": [
            "东楚复西秦，浮云类此身。",
            "关山劳策蹇，僮仆惯投人。",
            "孤客来千里，全家托四邻。",
            "生涯难自料，中夜问[情亲]。"
        ],
        "notes": "",
        "id": "6ba38bd607290431ed38c4bdb40dc2bc"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "客舍有怀因呈诸在事",
        "preface": "",
        "paragraphs": [
            "读书常苦节，待诏岂辞贫。",
            "暮雪犹驱马，晡飡又寄人。",
            "愁来占吉梦，老去惜良辰。",
            "延首平津阁，家山日已春。"
        ],
        "notes": "",
        "id": "ac038a6c17763d83cf8fad02dbdb4c44"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "书怀寄杨郭李王判官",
        "preface": "",
        "paragraphs": [
            "惯作云林客，因成懒漫人。",
            "吏欺从政拙，妻笑理家贫。",
            "李郭应时望，王杨入幕频。",
            "从容丞相阁，知忆故园春。"
        ],
        "notes": "",
        "id": "bf4fed505516892988af640f0113a91d"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉和给事寓直",
        "preface": "",
        "paragraphs": [
            "桂枝家共折，鸡树代相传。",
            "忝向鸾台下，仍看鴈影连。",
            "夜闲方步月，漏尽欲朝天。",
            "知去丹墀近，明王许荐贤。"
        ],
        "notes": "",
        "id": "bac4e9e69e457b1e49a5362694fa98f5"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "初入集贤院赠李献仁",
        "preface": "",
        "paragraphs": [
            "燕代官初罢，江湖路便分。",
            "九迁从命薄，四十幸人闻。",
            "迹愧趋丹禁，身曾系白云。",
            "何由返沧海，昨日谒明君。"
        ],
        "notes": "",
        "id": "b42bd169912cd5ac086cf8a4c0ff4752"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "酬李补阙雨中寄赠",
        "preface": "",
        "paragraphs": [
            "十年随马宿，几度受人恩。",
            "白发还乡井，微官有子孙。",
            "竹窗寒雨滴，苦砌夜虫喧。",
            "独媿东垣友，新诗慰旅魂。"
        ],
        "notes": "",
        "id": "19df569d844c42f6e03680ddf2476512"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "初除拾遗酬丘二十二见寄",
        "preface": "",
        "paragraphs": [
            "江海久垂纶，朝衣忽挂身。",
            "丹墀初谒帝，白发免羞人。",
            "才愧文章士，名当谏诤臣。",
            "空余荐贤分，不敢负交亲。"
        ],
        "notes": "",
        "id": "1f561523e67f46a4e69f9e2495a7fe3f"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "刘展下判官相招以诗答之",
        "preface": "",
        "paragraphs": [
            "国有非常宠，家承异姓勋。",
            "背恩慙皎日，不义若浮云。",
            "但使忠贞在，甘从玉石焚。",
            "窜身如有地，梦寐见明君。"
        ],
        "notes": "",
        "id": "4b1f33416ea23072b3def25fd59b61bb"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送侯山人赴会稽",
        "preface": "",
        "paragraphs": [
            "仙客辞萝月，东来就一官。",
            "且归沧海住，犹向白云看。",
            "猨呌江天暮，虫声野浦寒。",
            "时游镜湖里，为我把鱼竿。"
        ],
        "notes": "",
        "id": "9d34460b8abd26d1398902561b2b072d"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿禅智寺上方演大师院",
        "preface": "",
        "paragraphs": [
            "石林高几许，金刹在中峰。",
            "白日空山梵，清霜后夜钟。",
            "竹窗回翠壁，苔径入寒松。",
            "幸接无生法，疑心怯所从。"
        ],
        "notes": "",
        "id": "0bd597268b447612130f5a2c08b2d504"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题空山人石室",
        "preface": "",
        "paragraphs": [
            "早晚悟无生，头陀不到城。",
            "云山知夏腊，猨鸟见修行。",
            "地僻无溪路，人寻逐水声。",
            "年年深谷里，谁识远公名。"
        ],
        "notes": "",
        "id": "e186c93f8a8d57e59cd4fe95f5c8f7ac"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "登蒋山开善寺",
        "preface": "",
        "paragraphs": [
            "山殿秋云里，香烟出翠微。",
            "客寻朝磬至，僧背夕阳归。",
            "下界千门见，前朝万事非。",
            "看心兼送目，葭菼暮依依。"
        ],
        "notes": "",
        "id": "2e09515b231d264b228b523be7faa9a8"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题崇福寺禅院",
        "preface": "",
        "paragraphs": [
            "僧家竟何事，扫地与焚香。",
            "清磬度山翠，闲云来竹房。",
            "身心尘外远，岁月坐中长。",
            "向晚禅堂掩，无人空夕阳。"
        ],
        "notes": "",
        "id": "48540813767b0a4a6984e4fab1e17f88"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "秋晚送丹徒许明府赴上国因寄江南故人",
        "preface": "",
        "paragraphs": [
            "秋暮之彭泽，篱花远近逢。",
            "君书前日至，别后此时重。",
            "寒夜江边月，晴天海上峰。",
            "还知南地客，招引住新丰。"
        ],
        "notes": "",
        "id": "9f8573f4cdec8660968fa808849356d6"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送薛仲方归扬州",
        "preface": "",
        "paragraphs": [
            "佳句应无敌，贞心不有猜。",
            "慙为丈人行，怯见后生才。",
            "泛舸贪斜月，浮桡值早梅。",
            "绿杨新过雨，芳草待君来。"
        ],
        "notes": "",
        "id": "a4703c6fe2c04cda116dc055d0d352b3"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送韦员外还京",
        "preface": "",
        "paragraphs": [
            "十年离乱后，此去若为情。",
            "春晚香山绿，人稀豫水清。",
            "野陂看独树，关路逐残莺。",
            "前殿朝明主，应怜白发生。"
        ],
        "notes": "",
        "id": "969ca8af8cf52e45efb067c06c22d3fb"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "润州送友人",
        "preface": "",
        "paragraphs": [
            "见君还此地，洒泪向江边。",
            "国士劳相问，家书无处传。",
            "荒城胡马迹，塞木戍人烟。",
            "一路堪愁思，孤舟何渺然。"
        ],
        "notes": "",
        "id": "755c02c956e2697d2c5a24cdda641667"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送张芬东归",
        "preface": "",
        "paragraphs": [
            "喧喧五衢上，鞍马自驱驰。",
            "落日临阡陌，贫交欲别离。",
            "早知时事异，堪与世人随。",
            "握手将何赠，君心我独知。"
        ],
        "notes": "",
        "id": "4d18998146e167de4b6aafb0d3c12b1e"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送苏修游上饶",
        "preface": "",
        "paragraphs": [
            "爱尔无羁束，云山恣意过。",
            "一身随远岫，孤棹任轻波。",
            "世事关情少，渔家寄宿多。",
            "芦花浅淡处，江月奈人何。"
        ],
        "notes": "",
        "id": "6608bdec8aaa95b2b793217c04b1cc10"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送陆明府之盱眙",
        "preface": "",
        "paragraphs": [
            "陶令之官去，穷愁惨别魂。",
            "白烟横海戍，红叶下淮村。",
            "澹浪摇山郭，平芜到县门。",
            "政成堪吏隐，免负府公恩。"
        ],
        "notes": "",
        "id": "8007ad6f165b9eabcfca92493edca6b7"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "江南回逢赵曜因送任十一赴交城主簿",
        "preface": "",
        "paragraphs": [
            "江上长相忆，因高北望看。",
            "不知携老幼，何处度艰难。",
            "屈指同人尽，伤心故里残。",
            "遥怜驱匹马，白首到微官。"
        ],
        "notes": "",
        "id": "2b0916adbcf2b901fc2871ee334adf64"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送薛良史往越州谒从叔",
        "preface": "",
        "paragraphs": [
            "辞家年已久，与子分偏深。",
            "易得相思泪，难为欲别心。",
            "孤云随浦口，几日到山阴。",
            "遥想兰亭下，清风满竹林。"
        ],
        "notes": "",
        "id": "a295adc0ca7126f3c98fbc74e1461891"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送丘二十二之苏州",
        "preface": "",
        "paragraphs": [
            "积水与寒烟，嘉禾路几千。",
            "孤猨啼海岛，群雁起湖田。",
            "曾见长洲苑，尝闻大雅篇。",
            "却将封事去，知尔爱闲眠。"
        ],
        "notes": "",
        "id": "4e29378af7219645b97d6d4a80636953"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "登润州芙蓉楼",
        "preface": "",
        "paragraphs": [
            "上古人何在，东流水不归。",
            "往来潮有信，朝暮事成非。",
            "烟树临沙静，云帆入海稀。",
            "郡楼多逸兴，良牧谢玄晖。"
        ],
        "notes": "",
        "id": "7dc91ee579b871a6d52c5d969512de7e"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "江上书怀",
        "preface": "",
        "paragraphs": [
            "骨肉天涯别，江山日落时。",
            "泪流襟上血，发变镜中丝。",
            "胡越书难到，存亡梦岂知。",
            "登高回首罢，形影自相随。"
        ],
        "notes": "",
        "id": "546551de9ebe95550fb1b9949cfc83c4"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "春日忆姚氏外甥",
        "preface": "",
        "paragraphs": [
            "离乱人相失，春秋鴈自飞。",
            "祗缘行路远，未必寄书稀。",
            "二月花无数，频年意有违。",
            "落晖看过后，独坐泪沾衣。"
        ],
        "notes": "",
        "id": "540c0a228d16cdc6917f60c4e662f0e5"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送真上人还兰若",
        "preface": "",
        "paragraphs": [
            "得道云林久，年深暂一归。",
            "出山逢世乱，乞食觉人稀。",
            "半偈初传法，中峰又掩扉。",
            "爱憎应不染，尘俗自依依。"
        ],
        "notes": "",
        "id": "ef126d0628e1d28d646cb4e6926a37f0"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "润州送师弟自江夏往台州",
        "preface": "",
        "paragraphs": [
            "远客乘流去，孤帆向夜开。",
            "春风江上使，前日汉阳来。",
            "别路犹千里，离心重一杯。",
            "剡溪木未落，羡尔过天台。"
        ],
        "notes": "",
        "id": "c8906c30bc350a02b1ea15aaa05e690e"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送李道士归山",
        "preface": "",
        "paragraphs": [
            "秋城临古路，城上望君还。",
            "旷野入寒草，独行随远山。",
            "授人鸿宝内，将犬白云间。",
            "早晚烧丹罢，遥知冰雪寒。"
        ],
        "notes": "",
        "id": "4c833595e25f532df1fad10d044a7489"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿江西窦主簿厅",
        "preface": "",
        "paragraphs": [
            "广庭方缓步，星汉话中移。",
            "月满关山道，乌啼霜树枝。",
            "时艰难会合，年长重亲知。",
            "前事成金石，凄然泪欲垂。"
        ],
        "notes": "",
        "id": "9d1e09705547182258a98e8a319e7582"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "喜逢妻弟郑损因送入京",
        "preface": "",
        "paragraphs": [
            "乱后自江城，相逢喜复惊。",
            "为经多载别，欲问小时名。",
            "对酒悲前事，论文畏后生。",
            "遥知盈卷轴，纸贵在江城。"
        ],
        "notes": "",
        "id": "bb35fcc22797dbd80278c2956f1734d4"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "咏门下画小松上元王杜三相公",
        "preface": "",
        "paragraphs": [
            "昔闻生涧底，今见起毫端。",
            "众草此时没，何人知岁寒。",
            "岂能裨栋宇，且贵出门阑。",
            "只在丹青意，凌云也不难。"
        ],
        "notes": "",
        "id": "0f247f5f6543a936d24ddc16b6473dd6"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寄上礼部李侍郎",
        "preface": "",
        "paragraphs": [
            "吴楚相逢处，江湖共泛时。",
            "任风舟去远，待月酒行迟。",
            "白发常同欢，青云本要期。",
            "贵来君却少，秋至老偏悲。",
            "玉佩明朝盛，苍苔陋巷滋。",
            "追寻恨无路，唯有梦相思。"
        ],
        "notes": "",
        "id": "d31fdd8be65deb69b479c3f11f1dab63"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "书情寄上苏州韦使君兼呈吴县李明府",
        "preface": "",
        "paragraphs": [
            "数年湖上谢浮名，竹杖纱巾遂性情。",
            "云外有时逢寺宿，日西无事傍江行。",
            "陶潜县里看花发，庾亮楼中对月明。",
            "谁念献书来万里，君王深在九重城。"
        ],
        "notes": "",
        "id": "a0e5a614aeb7e9f2b44a8d91f82f77c8"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题桐庐李明府官舍",
        "preface": "",
        "paragraphs": [
            "讼堂寂寂对烟霞，五柳门前聚晓鵶。",
            "流水声中视公事，寒山影里见人家。",
            "观风竞美新为政，计日还知旧触邪。",
            "可惜陶潜无限酒，不逢篱菊正开花。"
        ],
        "notes": "",
        "id": "0601e14157dd0524c94b6fc32ba5f7c5"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠窦十九",
        "preface": "",
        "paragraphs": [
            "灵台暮宿意多违，木落花开羡客归。",
            "江海几时传锦字，风尘不觉化缁衣。",
            "山阳会里同人少，灞曲农时故老稀。",
            "幸得汉皇容直谏，怜君未遇觉人非。"
        ],
        "notes": "",
        "id": "faed40c8105dea4f3dd1a502431bac6f"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "虔州见郑表新诗因以寄赠",
        "preface": "",
        "paragraphs": [
            "梅花岭里见新诗，感激情深过楚词。",
            "平子四愁今莫比，休文八咏自同时。",
            "萍乡露冕真堪惜，凤沼鸣珂已讶迟。",
            "才子风流定难见，湖南春草但相思。"
        ],
        "notes": "",
        "id": "1488c7a393ea40fc26505eae6b40cd2d"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠元秘书",
        "preface": "",
        "paragraphs": [
            "旧书稍稍出风尘，孤客逢秋感此身。",
            "秦地谬为门下客，淮阴徒笑市中人。",
            "也闻阮籍寻常醉，见说陈平不久贫。",
            "幸有故人茅屋在，更将心事问情亲。"
        ],
        "notes": "",
        "id": "624eba2035c672c92ac4eb0bd7f4896b"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送韦八少府判官归东京",
        "preface": "",
        "paragraphs": [
            "玄成世业紫真官，文似相如貌胜潘。",
            "鸿鴈南飞人独去，云山一别岁将阑。",
            "清淮水急桑林晚，古驿霜多柿叶寒。",
            "琼树相思何日见，银钩数字莫为难。"
        ],
        "notes": "",
        "id": "2ea6c0dcd5d9053cc84287e4dcef689a"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送冯八将军奏事毕归滑台幕府",
        "preface": "",
        "paragraphs": [
            "王门别后到沧洲，帝里相逢俱白头。",
            "自欢马卿常带疾，还嗟李广不封侯。",
            "棠梨宫里瞻龙衮，细柳营中著虎裘。",
            "想到滑台桑叶落，黄河东注杏园秋。"
        ],
        "notes": "",
        "id": "0c6e9dbfa9388863a7c33747e3f3a304"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送王侍御佐婺州",
        "preface": "",
        "paragraphs": [
            "闻君作尉向江潭，吴越风烟到自谙。",
            "客路寻常经竹径，人家大底傍山岚。",
            "缘溪花木偏宜远，避地衣冠尽向南。",
            "惟有夜猨啼海树，思乡望北意难堪。"
        ],
        "notes": "",
        "id": "d16612e53b4c6555b488a1a611eae955"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "越中送王使君赴江华",
        "preface": "",
        "paragraphs": [
            "皂盖春风自越溪，独寻芳树桂阳西。",
            "远水浮云随马去，空山弱筿向云低。",
            "遥知异政荆门北，旧许新诗康乐齐。",
            "万里相思在何处，九疑残雪白猿啼。"
        ],
        "notes": "",
        "id": "86ab86c8c31e7358be64ef837508b11c"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送皇甫冉往白田",
        "preface": "",
        "paragraphs": [
            "江边尽日雉鸣飞，君向白田何日归。",
            "楚地蒹葭连海逈，隋朝杨柳映堤稀。",
            "津楼故市无行客，山馆空庭闭落晖。",
            "试问疲人与征战，使君双泪定沾衣。"
        ],
        "notes": "",
        "id": "aebf41c6ecf0d0b2cea2f67d76553ad7"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题兰若",
        "preface": "",
        "paragraphs": [
            "绝顶茅庵老此生，寒云孤木独经行。",
            "世人那得知幽迳，遥向青峰礼磬声。"
        ],
        "notes": "",
        "id": "2567cb82af3208f09de17c7b95e6c3e1"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送贺兰广赴选",
        "preface": "",
        "paragraphs": [
            "而今用武尔攻文，流辈干时独卧云。",
            "白发青袍趋会府，定应衡镜却慙君。"
        ],
        "notes": "",
        "id": "4ec6eef138bfadff443c974374012388"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "清江曲内一绝",
        "preface": "",
        "paragraphs": [
            "八月长江去浪平，片帆一道带风轻。",
            "极目不分天水色，南山南是岳阳城。"
        ],
        "notes": "",
        "id": "4dbf4e28fb064917030c2487a601609e"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "武康郭外望许纬先生山居",
        "preface": "",
        "paragraphs": [
            "湖上千峰带落晖，白云开处见柴扉。",
            "松门一迳仍生草，应是仙人向郭稀。"
        ],
        "notes": "",
        "id": "c18269873f2fc2341d71727656028de6"
    },
    {
        "author": "崔峒",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "句",
        "preface": "",
        "paragraphs": [
            "石梯青嶂下，茅屋白云间。",
            "（见明刻本《锦绣万花谷》卷二十五「隐逸」门）。"
        ],
        "notes": "",
        "id": "0d550ae82bbb8b9798fe9ed7e26941d1"
    }
]