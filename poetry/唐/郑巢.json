[
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "泊灵溪馆",
        "preface": "",
        "paragraphs": [
            "孤吟疎雨绝，荒馆乱峰前。",
            "晓鹭栖危石，秋萍满败船。",
            "溜从华顶落，树与赤城连。",
            "已有求闲意，相期在暮年。"
        ],
        "notes": "",
        "id": "5f48c2752e85407918c29ad140eb0a8b"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "瀑布寺贞上人院",
        "preface": "",
        "paragraphs": [
            "林疎多暮蝉，师去宿山烟。",
            "古壁灯熏画，秋琴雨润弦。",
            "竹间窥远鹤，岩上取寒泉。",
            "西岳沙房在，归期更几年。"
        ],
        "notes": "",
        "id": "f3f07c3dd77e35c26bc0633ff6af7f02"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寄贞法师",
        "preface": "",
        "paragraphs": [
            "巡礼知难尽，幽人见亦稀。",
            "几年潭上过，何待雪中归。",
            "远瀑穿经室，寒螀发定衣。",
            "无因寻道者，独坐对松扉。"
        ],
        "notes": "",
        "id": "6ffa5c9a7f563d85ea99d7cfb025417e"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送灵溪李侍郎",
        "preface": "",
        "paragraphs": [
            "貂裘离阙下，初佐汉元勋。",
            "河偃流澌叠，沙晴远树分。",
            "牛羊下暮霭，鼓角调寒云。",
            "中夕萧关宿，边声不可闻。"
        ],
        "notes": "",
        "id": "6b5d72f5f287e542045c8d67abf3a093"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送姚郎中罢郡游越",
        "preface": "",
        "paragraphs": [
            "逍遥方罢郡，高兴接东瓯。",
            "几处行杉迳，何时宿石楼。",
            "湘声穿古窦，华影在空舟。",
            "惆怅云门路，无因得从游。"
        ],
        "notes": "",
        "id": "f90ea0296ab8330efd013aa4001405c7"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送魏校书赴夏口从事",
        "preface": "",
        "paragraphs": [
            "西风吹远蝉，驿路在云边。",
            "独梦诸山外，高谈大斾前。",
            "夜灯分楚塞，秋角满湘船。",
            "郡邑多岩窦，何方便学仙。"
        ],
        "notes": "",
        "id": "8854d528411501838f9a2c9cdefb67d5"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送衡州薛从事",
        "preface": "",
        "paragraphs": [
            "吟去望双旌，沧洲晚气清。",
            "遥分高岳色，乱出远蝉声。",
            "楚霁云连寺，湘寒浪浸城。",
            "孤猨不可听，一听白髭生。"
        ],
        "notes": "",
        "id": "3e3ec89bb473ff0b78711f34f0bfac3e"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送边使",
        "preface": "",
        "paragraphs": [
            "关河度几重，边色上离容。",
            "灞水方为别，沙场又入冬。",
            "曙雕回大旆，夕雪没前峰。",
            "汉使多长策，须令远国从。"
        ],
        "notes": "",
        "id": "a95913c1d50c6970e0cd055384c795d6"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送人赴举",
        "preface": "",
        "paragraphs": [
            "篇章动玉京，坠叶满前程。",
            "旧国与僧别，秋江罢钓行。",
            "马过隋代寺，樯出楚山城。",
            "应近嵩阳宿，潜闻瀑布声。"
        ],
        "notes": "",
        "id": "b314b6067d838de06a82cea1f64b6601"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送袁肇归山阴",
        "preface": "",
        "paragraphs": [
            "论文意有违，寒雨洒行衣。",
            "南渡久谁语，后吟今独归。",
            "河帆因树落，沙鸟背潮飞。",
            "若值云门侣，多因宿翠微。"
        ],
        "notes": "",
        "id": "37a772b103082471563cbc2df23b6575"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送李式",
        "preface": "",
        "paragraphs": [
            "潇湘路杳然，清兴起秋前。",
            "去寺多随磬，看山半在船。",
            "绿云天外鹤，红树雨中蝉。",
            "莫使游华顶，逍遥更过年。"
        ],
        "notes": "",
        "id": "5caa28be610b51ff451631f0b9c1b6cf"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送韦弇",
        "preface": "",
        "paragraphs": [
            "挂席曙钟初，家山半在吴。",
            "𫇛声过远寺，江色润秋芜。",
            "陂鹤巢城木，边鸿宿岸芦。",
            "知君当永夜，独钓五湖隅。"
        ],
        "notes": "",
        "id": "3fb5aac4db60d4386ea6a73e785a5715"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送人南游",
        "preface": "",
        "paragraphs": [
            "南京路悄然，欹石漱流泉。",
            "远寺寒云外，扬帆暑雨前。",
            "鴈行回晓岫，蜃色上湖田。",
            "更想清吟处，多同隐者眠。"
        ],
        "notes": "",
        "id": "bc3a17c4abcac71dde6b837c74cd0bb1"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送省空上人归南岳",
        "preface": "",
        "paragraphs": [
            "又归衡岳寺，旧院树冥冥。",
            "坐石逢寒衲，寻龛补坏经。",
            "峤云笼曙磬，潭草落秋萍。",
            "谁伴高牕宿，禅衣挂桂馨。"
        ],
        "notes": "",
        "id": "3da9505920c8eb9aae0a1599cdb716d6"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送象上人还山中",
        "preface": "",
        "paragraphs": [
            "竹锡与袈裟，灵山笑暗霞。",
            "泉痕生净藓，烧力落寒花。",
            "高户闲听雪，空窗静捣茶。",
            "终期宿华顶，须会说三巴。"
        ],
        "notes": "",
        "id": "6f904a5c9088cf7dc91ee84abe7985d9"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送僧归富春",
        "preface": "",
        "paragraphs": [
            "忆过僧禅处，遥山抱竹门。",
            "古房关藓色，秋径扫潮痕。",
            "石净闻泉落，沙寒见鹤翻。",
            "终当从此望，更与道人言。"
        ],
        "notes": "",
        "id": "680a9211d06004f96f64abb3b127934d"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送琇上人",
        "preface": "",
        "paragraphs": [
            "古殿焚香外，清羸坐石棱。",
            "茶烟开瓦雪，鹤迹上潭冰。",
            "孤磬侵云动，灵山隔水登。",
            "白云归意远，旧寺在庐陵。"
        ],
        "notes": "",
        "id": "4295bae4677a4da34fc11fb48cfaffce"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "哭虚海上人",
        "preface": "",
        "paragraphs": [
            "一化西风外，禅流稍稍分。",
            "买碑行暮雨，斲石葬寒云。",
            "静户关松色，荒斋聚鸟群。",
            "朗吟声不倦，高传有遗文。"
        ],
        "notes": "",
        "id": "4abbe0c0ff176a07e06fa072cba4c3b2"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和姚郎中题凝公院",
        "preface": "",
        "paragraphs": [
            "后房寒竹连，白昼坐冥然。",
            "片衲何山至，空堂几夜禅。",
            "叶侵经上字，冰结砚中泉。",
            "雪夕谁同话，悬灯古像前。"
        ],
        "notes": "",
        "id": "90b22a5876c9080090975f8513acfa07"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠丘先生",
        "preface": "",
        "paragraphs": [
            "云泉心不爽，垂日坐柴关。",
            "砚取簷前雨，图开异国山。",
            "原僧招过宿，沙鸟伴长闲。",
            "地与中峰近，残阳独不还。"
        ],
        "notes": "",
        "id": "9bdbd66b64938a9af2b46196c50c7b6e"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠蛮僧",
        "preface": "",
        "paragraphs": [
            "南海何年过，中林一磬微。",
            "病逢秋雨发，心逐暮潮归。",
            "久卧前山寺，犹逢故国衣。",
            "近来慵步履，石藓满柴扉。"
        ],
        "notes": "",
        "id": "2d03eb3c66f36b67286d6229acc750c6"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "秋思",
        "preface": "",
        "paragraphs": [
            "寒蛩鸣不定，郭外水云幽。",
            "南浦鴈来日，北窗人卧秋。",
            "病身多在远，生计少于愁。",
            "薄暮西风急，清碪响未休。"
        ],
        "notes": "",
        "id": "a48b3d880bd1247cacbbe3bda6593821"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "楚城秋夕",
        "preface": "",
        "paragraphs": [
            "故苑多愁夕，西风木叶黄。",
            "寒江浸雾月，晓角满城霜。",
            "弟姪来书少，关河去路长。",
            "几时停桂檝，故国隔潇湘。"
        ],
        "notes": "",
        "id": "7e98a8059ddcd6527a0df6218aa02fb5"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "秋日陪姚郎中登郡中南亭",
        "preface": "",
        "paragraphs": [
            "云水生寒色，高亭发远心。",
            "鴈来疎角韵，槐落减秋阴。",
            "隔石尝茶坐，当山抱瑟吟。",
            "谁知潇洒意，不似有朝簪。"
        ],
        "notes": "",
        "id": "bb2fe6d4c651d2ef3446f0c739a7e10b"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿天竺寺",
        "preface": "",
        "paragraphs": [
            "暮过潭上寺，独宿白云间。",
            "钟磬遥连树，星河半隔山。",
            "石中泉暗落，松外户初关。",
            "却忆终南里，前秋此夕还。"
        ],
        "notes": "",
        "id": "cb632830969f066bac296ed8dce8fca6"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "陈氏园林",
        "preface": "",
        "paragraphs": [
            "当门三四峰，高兴几人同。",
            "寻鹤新泉外，留僧古木中。",
            "蝉鸣槐叶雨，鱼散芰荷风。",
            "多喜陪幽赏，清吟绕石丛。"
        ],
        "notes": "",
        "id": "1c7717e96fdb4ee3d218a1fc91ed2717"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题崔中丞北斋",
        "preface": "",
        "paragraphs": [
            "湖近草侵庭，秋来道兴生。",
            "寒潮添井味，远漏带松声。",
            "放卷听泉坐，寻僧踏雪行。",
            "何年各无事，高论宿青城。"
        ],
        "notes": "",
        "id": "a09687f474f6bbeb858d2e18487e9881"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题崔行先石室别墅",
        "preface": "",
        "paragraphs": [
            "山空水绕篱，几日此栖迟。",
            "采菊频秋醉，留僧拟夜棋。",
            "桂阴生野菌，石缝结寒澌。",
            "更喜连幽洞，唯君与我知。"
        ],
        "notes": "",
        "id": "e76973835f81f762282f3c8d4c8eda86"
    },
    {
        "author": "郑巢",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "题灵隐寺皖公院",
        "preface": "",
        "paragraphs": [
            "山寒叶满衣，孤鹤偶清羸。",
            "已在云房老，休为内殿期。",
            "岚昏声磬早，菓熟唤猨迟。",
            "未得终高论，明朝更别离。"
        ],
        "notes": "",
        "id": "e80fd34d2f1b80e23e6102e5e5c43eba"
    }
]