[
    {
        "author": "开元宫人",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "袍中诗",
        "preface": "",
        "paragraphs": [
            "沙场征戍客，寒苦若为眠。",
            "战袍经手作，知落阿谁边。",
            "蓄意多添线，含情更著绵。",
            "今生已过也，结取后生缘。"
        ],
