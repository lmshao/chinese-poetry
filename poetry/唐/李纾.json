[
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 迎神",
        "preface": "",
        "paragraphs": [
            "元尊九德，佐尧光宅。",
            "烈祖太宗，方周作伯。",
            "响怀霜露，乐变金石。",
            "白云青风，髣髴来格。"
        ],
        "notes": "",
        "id": "31f4f3cc8063695ee1b56e3de34d91c7"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 登歌奠币",
        "preface": "",
        "paragraphs": [
            "四时有典，百事来祭。",
            "尊祖奉宗，严禋大帝。",
            "礼先苍璧，奠备黝制。",
            "于万斯年，熙成帝系。"
        ],
        "notes": "",
        "id": "827db8bd70d70fa73d3dfdba09b62eff"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 迎俎",
        "preface": "",
        "paragraphs": [
            "盛牲实俎，涓选休成。",
            "鼎煁阳燧，玉盥阴精。",
            "有𫗣嘉豆，既和大羹。",
            "侑以清乐，细齐人情。"
        ],
        "notes": "",
        "id": "f0ac40a7d5fe50f53af7db0d181ceea9"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 德明酌献",
        "preface": "",
        "paragraphs": [
            "清庙弈弈，和乐雍雍。",
            "器尊牺象，礼属宗公。",
            "白水方[祼]，黄流在中。",
            "谟明之德，万古清风。"
        ],
        "notes": "",
        "id": "a766743afee6d6793d4078440786530c"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 兴圣酌献",
        "preface": "",
        "paragraphs": [
            "閟宫静谧，合乐周张。",
            "泰尊始献，百末重觞。",
            "震澹存诚，庶几迪尝。",
            "遥源之祚，天汉灵长。"
        ],
        "notes": "",
        "id": "56723745958cedae59ca0d89a1142dbe"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 亚献终献",
        "preface": "",
        "paragraphs": [
            "惟清惟肃，靡闻靡见。",
            "举备九成，俛终三献。",
            "庆彰曼寿，胙撤嘉荐。",
            "瘗玉埋牲，礼神斯徧。"
        ],
        "notes": "",
        "id": "e6545766eadbf6a3b13f34ea316555bc"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "唐德明兴圣庙乐章 送神",
        "preface": "",
        "paragraphs": [
            "元精回复，灵贶繁滋。",
            "风洒兰路，云摇桂旗。",
            "高丘缅邈，凉部逶迟。",
            "瞻望靡及，缠绵永思。"
        ],
        "notes": "",
        "id": "eeea30a02ba25f4c75be8dc213021dbc"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "让皇帝庙乐章 迎神",
        "preface": "",
        "paragraphs": [
            "皇矣天宗，德先王季。",
            "因心则友，克让以位。",
            "爰命有司，式尊前志。",
            "神其降灵，昭飨祀事。"
        ],
        "notes": "",
        "id": "52586ed6f60b599b4a8c5cc36252bf94"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "让皇帝庙乐章 奠币",
        "preface": "",
        "paragraphs": [
            "惟帝时若，去而上仙。",
            "祀用商武，乐备宫悬。",
            "白壁加荐，玄𫄸告虔。",
            "子孙拜后，承兹吉蠲。"
        ],
        "notes": "",
        "id": "3a4ee7ca13f55de4255bb6f56df20fd7"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "让皇帝庙乐章 迎俎",
        "preface": "",
        "paragraphs": [
            "祀盛体荐，礼协粢盛。",
            "方周假庙，用鲁纯牲。",
            "捧撤祗敬，击拊和鸣。",
            "受厘归胙，既戒而平。"
        ],
        "notes": "",
        "id": "3f763bc6c1fa5cad32044cbf2c3876ab"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "让皇帝庙乐章 酌献",
        "preface": "",
        "paragraphs": [
            "八音具举，三寿既盥。",
            "洁兹宗彝，瑟彼圭瓒。",
            "兰肴重错，椒醑飘散。",
            "降祚维城，永为藩翰。"
        ],
        "notes": "",
        "id": "9603c82fd94b57888e3e8d92f05ad28b"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "让皇帝庙乐章 亚献终献",
        "preface": "",
        "paragraphs": [
            "秩礼有序，和音既同。",
            "九仪不忒，三揖将终。",
            "孝感藩后，相维辟公。",
            "四时之典，永永无穷。"
        ],
        "notes": "",
        "id": "5d6f1565b54e7dd52a0e65f59e959820"
    },
    {
        "author": "李纾",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "让皇帝庙乐章 送神",
        "preface": "",
        "paragraphs": [
            "奠献已事，昏昕载分。",
            "风摇雨散，灵卫𬘡缊。",
            "龙驾帝服，已腾五云。",
            "泮宫复閟，寂寞无闻。"
        ],
        "notes": "",
        "id": "c7e04cd384213b992bed4c8e71da6229"
    }
]