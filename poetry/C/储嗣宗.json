[
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "登芜城",
        "preface": "",
        "paragraphs": [
            "昔人登此地，丘垅已前悲。",
            "今日又非昔，春风能几时。"
        ],
        "notes": "",
        "id": "a4c205d49a1f26ab12287d64111121d0"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "沧浪峡",
        "preface": "",
        "paragraphs": [
            "沧浪临古道，道上石成尘。",
            "自有沧浪峡，谁为无事人。"
        ],
        "notes": "",
        "id": "fbbba8c6bdb75aaed52c842be9bef426"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "垓城",
        "preface": "",
        "paragraphs": [
            "百战未言非，孤军惊夜围。",
            "山河意气尽，泪湿美人衣。"
        ],
        "notes": "",
        "id": "90f5a22f740b32498f22b192137e8ced"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "南陂远望",
        "preface": "",
        "paragraphs": [
            "闲门横古塘，红树已惊霜。",
            "独立望秋草，野人耕夕阳。",
            "孤烟起蜗舍，飞鹭下渔梁。",
            "唯有田家事，依依似故乡。"
        ],
        "notes": "",
        "id": "6b506e4318ef861299727b9cc3bf97ea"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿玉箫宫",
        "preface": "",
        "paragraphs": [
            "尘飞不到空，露湿翠微宫。",
            "鹤影石桥月，箫声松殿风。",
            "绿毛辞世女，白发入壶翁。",
            "借问烧丹处，桃花几遍红。"
        ],
        "notes": "",
        "id": "c0e6517676609127841c69d7b94b9af5"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "晚眺徐州延福寺",
        "preface": "",
        "paragraphs": [
            "杉风振旅尘，晚景籍芳茵。",
            "片水明在野，万花深见人。",
            "静依归鹤思，远惜旧山春。",
            "今日惜携手，寄怀吟白苹。"
        ],
        "notes": "",
        "id": "c866ca5a3aacfc622b4f20998fc03369"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿甘棠馆",
        "preface": "",
        "paragraphs": [
            "尘迹入门尽，悄然江海心。",
            "水声巫峡远，山色洞庭深。",
            "风桂落寒子，岚烟凝夕阴。",
            "前轩鹤归处，萝月思沈沈。"
        ],
        "notes": "",
        "id": "114f3b5c3843a5365f648ca4e1b7fc86"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和茅山高拾遗忆山中杂题五首 山泉",
        "preface": "",
        "paragraphs": [
            "香味清机仙府回，萦纡乱石便流桮。",
            "春风莫泛桃花去，恐引凡人入洞来。"
        ],
        "notes": "",
        "id": "5236091ff71cb4e63dfac0fe637285df"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和茅山高拾遗忆山中杂题五首 巢鹤",
        "preface": "",
        "paragraphs": [
            "千万云间丁令威，殷勤仙骨莫先飞。",
            "若逢茅氏传消息，贞白先生不久归。"
        ],
        "notes": "",
        "id": "40490c8eefc4bb9e58275524e0e5f481"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和茅山高拾遗忆山中杂题五首 胡山",
        "preface": "",
        "paragraphs": [
            "犬入五云音信绝，凤楼凝碧悄无声。",
            "焚香古洞步虚夜，露湿松花空月明。"
        ],
        "notes": "",
        "id": "f34fcf14ec9e438dcf32a13170d4bf00"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和茅山高拾遗忆山中杂题五首 小楼",
        "preface": "",
        "paragraphs": [
            "松杉风外乱山青，曲几焚香对石屏。",
            "空忆去年春雨后，燕泥时污太玄经。"
        ],
        "notes": "",
        "id": "c3ffb394be38bccfb14e7886d0202f49"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和茅山高拾遗忆山中杂题五首 山邻",
        "preface": "",
        "paragraphs": [
            "石桥春涧已归迟，梦入仙山山不知。",
            "柱史从来非俗吏，青牛道士莫相疑。"
        ],
        "notes": "",
        "id": "637dbc9f93f1e14c048f21adc7475a10"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送道士",
        "preface": "",
        "paragraphs": [
            "泠然御风客，与道自浮沈。",
            "黄鹤有归语，白云无忌心。",
            "药炉经月净，天路入壶深。"
        ],
        "notes": "",
        "id": "310f83f5512ca5b6e8327b3c69993fac"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿山馆",
        "preface": "",
        "paragraphs": [
            "寂寞对衰草，地凉凝露华。",
            "蝉鸣月中树，风落客前花。",
            "山馆无宿伴，秋琴初别家。",
            "自怜千万里，笔砚寄生涯。"
        ],
        "notes": "",
        "id": "70d391683fe99ad007cc3614b4233306"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "入浮石山",
        "preface": "",
        "paragraphs": [
            "斜日出门去，残花已过春。",
            "鸟声穿叶远，虎迹渡溪新。",
            "入洞几时路，耕田何代人。",
            "自慙非避俗，不敢问迷津。"
        ],
        "notes": "",
        "id": "bf80a0b4d15504acdbcda1b4eb80606d"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送人归故园",
        "preface": "",
        "paragraphs": [
            "远节惨言别，况予心久违。",
            "从来忆家泪，今日送君归。",
            "野路正风雪，还乡犹布衣。",
            "里中耕稼者，应笑读书非。"
        ],
        "notes": "",
        "id": "b69ce8cc66a1a35a556e22a565f71099"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "得越中书",
        "preface": "",
        "paragraphs": [
            "芳草离离思，悠悠春梦余。",
            "池亭千里月，烟水一封书。",
            "诗想怀康乐，文应吊子胥。",
            "扁舟恋南越，岂独为鲈鱼。"
        ],
        "notes": "",
        "id": "1d47270102a8d319a2485c205fd89bac"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "秋墅",
        "preface": "",
        "paragraphs": [
            "欲暮候樵者，望山空翠微。",
            "虹随余雨散，鵶带夕阳归。",
            "穷巷长秋草，孤村时捣衣。",
            "谁知多病客，寂寞掩柴扉。"
        ],
        "notes": "",
        "id": "7fe59e363288e147ee467283a835cf85"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "哭彭先生",
        "preface": "",
        "paragraphs": [
            "谷口溪声客自伤，那堪呜咽吊残阳。",
            "空阶鹤恋丹青影，秋雨苔封白石床。",
            "主祭孤儿初学语，无媒旅榇未还乡。",
            "门人远赴心丧夜，月满千山旧草堂。"
        ],
        "notes": "",
        "id": "595184bedfa846d3d8d469b29605e942"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和顾非熊先生题茅山处士闲居",
        "preface": "",
        "paragraphs": [
            "归耕地肺绝尘喧，匣里青萍未报恩。",
            "浊酒自怜终日醉，古风时得野人言。",
            "鸟啼碧树闲临水，花满青山静掩门。",
            "唯有阶前芳草色，年年惆怅忆王孙。"
        ],
        "notes": "",
        "id": "be011b87e43e0a3ea1b9e8af1da56ca5"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "长安怀古",
        "preface": "",
        "paragraphs": [
            "祸稔萧墙终不知，生人力屈尽边陲。",
            "赤龙已赴东方暗，黄犬徒怀上蔡悲。",
            "面缺崩城山寂寂，土埋冤骨草离离。",
            "秋风解怨扶苏死，露泣烟愁红树枝。"
        ],
        "notes": "",
        "id": "8836414fb68f2990a37d3253e4d09b27"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "过王右丞书堂二首 一",
        "preface": "",
        "paragraphs": [
            "澄潭昔卧龙，章句世为宗。",
            "独步声名在，千岩水石空。",
            "野禽悲灌木，落日吊清风。",
            "后学攀遗址，秋山闻草虫。"
        ],
        "notes": "",
        "id": "dd60a5da43cd3ad11e01b19b99bae5d6"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "过王右丞书堂二首 二",
        "preface": "",
        "paragraphs": [
            "万树影参差，石床藤半垂。",
            "萤光虽散草，鸟迹尚临池。",
            "风雅传今日，云山想昔时。",
            "感深苏属国，千载五言诗。"
        ],
        "notes": "",
        "id": "fd2ed3ed6e78717b45f986e029ca77ff"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "孤鴈",
        "preface": "",
        "paragraphs": [
            "孤鴈暮飞急，萧萧天地秋。",
            "关河正黄叶，消息断青楼。",
            "湘渚烟波远，骊山风雨愁。",
            "此时万里道，魂梦遶沧洲。"
        ],
        "notes": "",
        "id": "8308422835a4fda13e11f6f1fbaf4a55"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠隐者",
        "preface": "",
        "paragraphs": [
            "尽室居幽谷，乱山为四邻。",
            "雾深知有术，窗静似无人。",
            "鹤语松上月，花明云里春。",
            "生涯更何许，尊酒与垂纶。"
        ],
        "notes": "",
        "id": "3610f4db5991fd4c8f2063bea94ab729"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "早春怀薛公裕",
        "preface": "",
        "paragraphs": [
            "幽独自成愚，柴门日渐芜。",
            "陆机初入洛，孙楚又游吴。",
            "失意怨杨柳，异乡闻鹧鸪。",
            "相思复相望，春草满南湖。"
        ],
        "notes": "",
        "id": "70756fe93ff54e077a9da12fa0135a6c"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "村月",
        "preface": "",
        "paragraphs": [
            "月午篱南道，前村半隐林。",
            "田翁独归处，荞麦露花深。"
        ],
        "notes": "",
        "id": "48b7d625fdf7b06916224f163e74b8ce"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "早春",
        "preface": "",
        "paragraphs": [
            "野树花初发，空山独见时。",
            "踟蹰历阳道，乡思满南枝。"
        ],
        "notes": "",
        "id": "b86dd5390a4e184f6c1217db3a050d8b"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送友人游吴",
        "preface": "",
        "paragraphs": [
            "吴山青楚吟，草色异乡心。",
            "一酌水边酒，数声花下琴。",
            "登楼旧国远，探穴九疑深。",
            "更想逢秋节，那堪闻夜砧。"
        ],
        "notes": "",
        "id": "e46e6612033278b647c7787211f0e6bd"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠别",
        "preface": "",
        "paragraphs": [
            "兰泽伤秋色，临风远别期。",
            "东城草虽绿，南浦柳无枝。",
            "直道岂易枉，暗投谁不疑。",
            "因君问行役，有泪湿江蓠。"
        ],
        "notes": "",
        "id": "7706a4093dfef0c97f3e151eb0eaf515"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "经故人旧居",
        "preface": "",
        "paragraphs": [
            "万里访遗尘，莺声泪湿巾。",
            "古书无主散，废宅与山邻。",
            "宿草风悲夜，荒村月吊人。",
            "凄凉问残柳，今日为谁春。"
        ],
        "notes": "",
        "id": "dc9b71d79cc9042a5e6b434062e4bdd4"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "随边使过五原",
        "preface": "",
        "paragraphs": [
            "偶逐星车犯虏尘，故乡常恐到无因。",
            "五原西去阳关废，日漫平沙不见人。"
        ],
        "notes": "",
        "id": "ff59ca3451fd88d679020ae108898391"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "春怀寄秣陵知友",
        "preface": "",
        "paragraphs": [
            "庐江城外柳堪攀，万里行人尚未还。",
            "借问景阳台下客，谢家谁更卧东山。"
        ],
        "notes": "",
        "id": "04fa03b4aa18d29953970bd08775aa3d"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宋州月夜感怀",
        "preface": "",
        "paragraphs": [
            "鴈池衰草露沾衣，河水东流万事微。",
            "寂寞青陵台上月，秋风满树鹊南飞。"
        ],
        "notes": "",
        "id": "6c9602ce87a820afbf6ccdde17d716a5"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "吴宫",
        "preface": "",
        "paragraphs": [
            "荒台荆棘多，忠谏竟如何。",
            "细草迷宫巷，闲花悮绮罗。",
            "前溪徒自绿，子夜不闻歌。",
            "怅望清江暮，悠悠东去波。"
        ],
        "notes": "",
        "id": "ec1fd03c7b3ea29abe94cd7a941d82d6"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "春游望仙谷",
        "preface": "",
        "paragraphs": [
            "清无车马尘，深洞百花春。",
            "鸡犬疑沾药，耕桑似避秦。",
            "登山采樵路，临水浣纱人。",
            "若得心无事，移家便卜邻。"
        ],
        "notes": "",
        "id": "6e567394679847137b08144d0758e381"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "圣女祠",
        "preface": "",
        "paragraphs": [
            "石屏苔色凉，流水遶祠堂。",
            "巢鹊疑天汉，潭花似镜妆。",
            "神来云雨合，神去蕙兰香。",
            "不复闻双佩，山门空夕阳。"
        ],
        "notes": "",
        "id": "f44d7c03411f290e82c73edf6a6e5de9"
    },
    {
        "author": "储嗣宗",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宿范水",
        "preface": "",
        "paragraphs": [
            "行人倦游宦，秋草宿湖边。",
            "露湿芙蓉渡，月明渔网船。",
            "寒机深竹里，远浪到门前。",
            "何处思乡甚，歌声闻采莲。"
        ],
        "notes": "",
