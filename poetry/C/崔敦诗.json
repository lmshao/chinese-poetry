[
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "双秀云牟实，三眠雪茧丰。",
            "无人知帝力，浑在舜弦中。"
        ],
        "notes": "",
        "id": "a049219a217bf16ad8ec1666af11a9a6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "御柳垂波绿，宫槐覆幄凉。",
            "香风随步辇，偏到殿中央。"
        ],
        "notes": "",
        "id": "04661df4bec826c34aa3d1bdf52f28d5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "采缕萦朱户，芳菰剪翠筒。",
            "风光天上别，景物世间同。"
        ],
        "notes": "",
        "id": "f6ca88e91ebaa4855d62b2efa7f8f454"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "千年桃印明金殿，百子榴房照绮疏。",
            "清晓宫门放鱼钥，内家催进恤刑书。"
        ],
        "notes": "",
        "id": "198aaef050f3e7b49bfb3a17abcd68e9"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "幽芳拂拂朝香度，密叶阴阴昼影圆。",
            "玉食未应须角黍，君王端是念忠贤。"
        ],
        "notes": "",
        "id": "a166b964d0dc8a5bc0df34ea85e48864"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "双人绿艾消民沴，五色朱丝奉帝龄。",
            "向晚封章都阅遍，翠舆初过水心亭。"
        ],
        "notes": "",
        "id": "a34b36e28d429330c50cdd8679043fb5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "嶰管今朝应，璇杓昨夜旋。",
            "耕桑三万里，一样乐尧天。"
        ],
        "notes": "",
        "id": "a713e9c328c6fa83cedf898657acf478"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "事已高超古，心犹切为民。",
            "慈颜应有喜，房宿正当晨。"
        ],
        "notes": "",
        "id": "8d9b92a5207cbc1eb8e1047a83171bc6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "采柏浮仙醁，凝酥点寿盘。",
            "君王千万岁，长奉两宫欢。"
        ],
        "notes": "",
        "id": "f1289617a28852780531ae6f5f81c6e4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "九重云气鬰崔嵬，日转青旗瑞色开。",
            "晨跸一声春又到，太平天子上瑶杯。"
        ],
        "notes": "",
        "id": "432d94f8e3c3c2ff8af8abd3c1dd619a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "楼阁隔年余旧雪，园林连夜著新花。",
            "东皇拟作行春计，先到长生大帝家。"
        ],
        "notes": "",
        "id": "eb5380895de4ce1691ee183df9186c70"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "景龙门上华灯动，黄鹄池边翠浪通。",
            "一一春光须作主，从今二十四番风。"
        ],
        "notes": "",
        "id": "3620a2aab39444dc8adade89351f48b4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "玉管和声度，金铺丽景迟。",
            "思齐文母圣，春日受春祺。"
        ],
        "notes": "",
        "id": "c0f9b72c43e9a80a26a72248dc26dbf1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "宝殿春朝退，瑶池夜宴深。",
            "明朝是人日，时复问晴阴。"
        ],
        "notes": "",
        "id": "970189caac8e3c009f165e76c7298056"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "翠辇西湖路，雕盘北苑花。",
            "时平无外事，随意趁年华。"
        ],
        "notes": "",
        "id": "a13412176acca2d95f64b89dbc43103a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "扇开雉影红云起，帘卷虾须画烛明。",
            "三十六宫齐进酒，盘金小胜缕长生。"
        ],
        "notes": "",
        "id": "41709bd0f9c93d5318ab454bb5f8ab2b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "日溶凤沼摇波暖，云护龙楼倒影长。",
            "读罢黄庭无一事，好风吹动百花香。"
        ],
        "notes": "",
        "id": "fb899efb153686e77e553eba4b6a4e98"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "院落韶光归柳色，郊原耕信到菖芽。",
            "望春台下□□软，不见游龙有外家。"
        ],
        "notes": "",
        "id": "695ca291a2f499cb8ff31b6c39040480"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "今岁韶光好，时逢大有年。",
            "条风方被物，菖叶又催田。"
        ],
        "notes": "",
        "id": "8febb84df4fed1998b91de7ebd1fbf21"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "今岁韶光好，年中两见春。",
            "余寒九日在，芳意一朝新。"
        ],
        "notes": "",
        "id": "98be913e06a30a1345e912a1c12ad60f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "今岁韶光好，田闾气象淳。",
            "政平无横赋，粟贱少穷民。"
        ],
        "notes": "",
        "id": "5192f941d1170c86c31068569f14dcce"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "东风先满殿前旗，御柳宫花次第知。",
            "连夜芳心相待发，侵朝柔思不胜垂。"
        ],
        "notes": "",
        "id": "a704e65ca9dd7f4ee73fa1e375134ead"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "千门晓日山河丽，万国春风草木香。",
            "造化无私随发育，圣明天子是东皇。"
        ],
        "notes": "",
        "id": "8030e03d7bfba8eb690cf90151729dd5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "雕庭清跸转轻雷，凤管吹春入仗来。",
            "万籁有声皆善颂，八荒无地不熙台。"
        ],
        "notes": "",
        "id": "d13f03e3541e93808c288f056a8e0b9c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其一",
        "preface": "",
        "paragraphs": [
            "何处春来早，光风入九门。",
            "未翻池荇翠，先著壁椒温。"
        ],
        "notes": "",
        "id": "429412538c8de98bca3cf41cf06cc67c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其二",
        "preface": "",
        "paragraphs": [
            "何处春来早，坤仪物意熙。",
            "烟生金屋重，日上玉阶迟。"
        ],
        "notes": "",
        "id": "db0ed2d775e96fe35f10259927f2104d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其三",
        "preface": "",
        "paragraphs": [
            "明朝嘉气满慈闱，亲侍君王献玉巵。",
            "一样酥盘俱手制，百般彩胜总春宜。"
        ],
        "notes": "",
        "id": "40c206a361414494453831947908f1c1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其四",
        "preface": "",
        "paragraphs": [
            "风光天上倍常殊，花压阑干昼影舒。",
            "虚几自临中令帖，明窗时展大家书。"
        ],
        "notes": "",
        "id": "396aa119466e03ef9fed835bf61808d1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其五",
        "preface": "",
        "paragraphs": [
            "清明小苑条桑地，和暖平川浴种天。",
            "圣主俭勤游乐少，只将敦朴示民先。"
        ],
        "notes": "",
        "id": "efd85ff7234a3849557b220ff50fd157"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "玉斝浮菖细，金盘荐术香。",
            "皇龄千万寿，高拱舜衣裳。"
        ],
        "notes": "",
        "id": "22be44bedf57e2d2f2fc94a6f848bd6c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "兰气浮丹殿，槐阴被紫宸。",
            "薰琴多在御，挥拂寄深仁。"
        ],
        "notes": "",
        "id": "30838e3c437e1e869482374ce9208aca"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "清晓开鱼钥，红云缥缈间。",
            "催裁恤刑诏，传放谢衣班。"
        ],
        "notes": "",
        "id": "a072537aa2e8a781b29f9a2c4054fee5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "夜披章奏凉生扇，午对臣邻汗浃衣。",
            "水阁风亭松竹邃，翠舆临幸日来稀。"
        ],
        "notes": "",
        "id": "ac19168d4f936931b49d6b33e2935e3d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "仁风长养群生遂，化日清明百沴驱。",
            "桃印艾人消底用，殿心高设敬天图。"
        ],
        "notes": "",
        "id": "32e3f8ef61f2dd862a6c31081aa6d1ed"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "避暑深宫消永昼，函风广殿起凉秋。",
            "宸心未惬高明适，志在山东二百州。"
        ],
        "notes": "",
        "id": "f08b65bff9bc7ca4ea9ffd73501aac4a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其一",
        "preface": "",
        "paragraphs": [
            "剪玉菰筒翠，盘金彩缕长。",
            "宫中多燕喜，天下正明昌。"
        ],
        "notes": "",
        "id": "9107e37fb1c66cb5c77dc13b0045cc9d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其二",
        "preface": "",
        "paragraphs": [
            "瑞麦登时物，香蒲荐寿祺。",
            "珠囊新样小，蹙就葛覃诗。"
        ],
        "notes": "",
        "id": "cfd616555e7f145799fa3306f0aad1d3"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其三",
        "preface": "",
        "paragraphs": [
            "鬭草人归午漏徐，宫帘不动玉窗虚。",
            "旋开蕊笈寻仙典，闲点松腴试法书。"
        ],
        "notes": "",
        "id": "642d485cfc1a56f75e92b51b4b805dbe"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其四",
        "preface": "",
        "paragraphs": [
            "蚕馆风清下箔时，六宫□从献丝归。",
            "便教渊室催机织，要及明堂制衮衣。"
        ],
        "notes": "",
        "id": "c99efaf64149bc54739649719b817a6a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其五",
        "preface": "",
        "paragraphs": [
            "圣主恭勤少燕游，生衣趁得未明求。",
            "随时但献长生缕，当午犹闲竞渡舟。"
        ],
        "notes": "",
        "id": "ff895b80576d59046e51c592ce9e17b2"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "欲识春囘处，长生大帝家。",
            "万年枝耸翠，千岁果开花。"
        ],
        "notes": "",
        "id": "47cab6fa7cd68e86ecdd7e4b37b1012c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "欲识春囘处，君王献寿时。",
            "矞云扶宝座，和气入瑶巵。"
        ],
        "notes": "",
        "id": "9db09f479121fd8b7fde6d1a07268552"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "欲识春囘处，晴峰俯碧泉。",
            "岩峦犹积雪，草木已生烟。"
        ],
        "notes": "",
        "id": "34c81c51f133b32f6781d24c7fe0e1d7"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "冰消宿沼悠扬动，烟暖寒林约略青。",
            "试上龙楼囘远望，朝来生绿画罗屏。"
        ],
        "notes": "",
        "id": "932d65afb0be1310a26ef14926ad2ab0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "聚景园中芳意换，望湖楼下绿波长。",
            "厖眉自识车音美，时有𬘡缊夹道香。"
        ],
        "notes": "",
        "id": "175a271efd305f03d25c6a732e77c96e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "一气暗随鸾辂动，万祥给会衮衣朝。",
            "康衢歌吹东风里，满听儿童善祝尧。"
        ],
        "notes": "",
        "id": "efb8e9c447dcbc61054519b311391783"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "天上春光别，东皇管领来。",
            "初从旗气转，旋逐跸声囘。"
        ],
        "notes": "",
        "id": "1e9d4e32effc24cbecab06cd7e49767f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "天上春光别，融融物态宜。",
            "彤庭花气暖，黄道日轮迟。"
        ],
        "notes": "",
        "id": "0ff77399514649559f9ce398708577d4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "天上春光别，时康乐事繁。",
            "烟花浮阆苑，露叶簇金盘。"
        ],
        "notes": "",
        "id": "85ee6bb36894df4a880678928c58b229"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "苍龙檠辇道春囘，尽把韶华入酒杯。",
            "但见殿前移仗退，已闻苑内奏花开。"
        ],
        "notes": "",
        "id": "3952f76c7a9eaafdf134a7a3e15e7332"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "玉压阑干日影长，云窗侍女昼焚香。",
            "琅函自检长生箓，金管时书急就章。"
        ],
        "notes": "",
        "id": "e680e629db6793853e20221801f5a41b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "花边破日红千叠，柳际匀烟翠一层。",
            "阅尽风光长不老，绿车当见从云仍。"
        ],
        "notes": "",
        "id": "a0f127ce7a71163bebf8143caa078114"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "赤伏祥晖盛，黄离治象亨。",
            "天时四序正，君德万方明。"
        ],
        "notes": "",
        "id": "cfebaae1cf76547ce26114101059fa54"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "采缕盘金丽，香蒲镂玉匀。",
            "君王千万寿，岁岁乐兹辰。"
        ],
        "notes": "",
        "id": "4b319cee30ba019094b9cc903199f013"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "日御秋秋永，云峰叠叠繁。",
            "宸心定忘暑，长算入中原。"
        ],
        "notes": "",
        "id": "8fbeb486ab6ff31ae77750b9d8198c8c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "光浮水槛虚黄盖，凉满冰台待翠舆。",
            "日转槐龙天近午，汗衣犹听讲筵书。"
        ],
        "notes": "",
        "id": "3305f5e77e79563984a800540ff26384"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "松碧涵风清度曲，梅黄过雨密留阴。",
            "翠寒堂上全无暑，定起人间扇暍心。"
        ],
        "notes": "",
        "id": "f6f6607c177057571ea5ddff7facf1f5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "黄道星辰移企翼，青冥风露近飞簷。",
            "翠华晚过凌虚殿，一色明珠十二帘。"
        ],
        "notes": "",
        "id": "71b1741aa26fc2bd0d5b1dde84a08a5a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "玉燕垂符小，珠囊结艾青。",
            "更将长命缕，侵晓奉慈庭。"
        ],
        "notes": "",
        "id": "edff7b2778b2a0476a24efe0f2753bbd"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "厚德承三殿，柔风被八纮。",
            "欲知勤俭化，当暑澣衣清。"
        ],
        "notes": "",
        "id": "cb1087fbd09c22acbfc9741e65010da9"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "玉斝浮菖追令节，宝箱储药趁灵辰。",
            "细开角黍询前事，应助吾皇念直臣。"
        ],
        "notes": "",
        "id": "aadf0790165f953f455f0ff851b30e19"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "日永荇池闲画舫，风清蚕馆入新丝。",
            "内人鬭草归能说，今岁山中艾出迟。"
        ],
        "notes": "",
        "id": "f84baf18c2e30804c8d67e14e2472259"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "翠云幕卷迎风殿，零露盘高待月台。",
            "应上君王千万寿，霓裳吹下九天来。"
        ],
        "notes": "",
        "id": "e8b0e2a82f6d9ddf83ef9270de498472"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "翠管吹寒去，青旗卷仗来。",
            "慈皇千万寿，春日上春杯。"
        ],
        "notes": "",
        "id": "0d8b32f37ec3d1a6ed44bffc7a4ab5ae"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "剪玉酥花细，盘金彩胜宜。",
            "六宫呈妙巧，春日颂春祺。"
        ],
        "notes": "",
        "id": "917a98c172a01e736ed47f28d65a1f7e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "凤苑开丹燕，龙楼下翠华。",
            "天工不敢住，春日著春花。"
        ],
        "notes": "",
        "id": "2587d6a919e78451c3b705e78caf166a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "亲提神器授今皇，帝德王功日日昌。",
            "万宇熙台无一事，湖山好处赏风光。"
        ],
        "notes": "",
        "id": "2de78908c327665a89044b7aeee8c08b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "今岁东皇作意迟，春朝恰近上元期。",
            "长生宫殿花开了，便放人间十万枝。"
        ],
        "notes": "",
        "id": "871801062e7e53329b7c0bf6ec5c8a96"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "高蹈殊庭二十春，随时游乐为同民。",
            "翠舆黄伞西湖路，老耨年年喜望尘。"
        ],
        "notes": "",
        "id": "0493dc2da5b0ea25accc6841515937eb"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "春晓慈闱启，君王奏问安。",
            "和声调嶰管，欢颂献椒盘。"
        ],
        "notes": "",
        "id": "2b1a45bdb03d1058fd907ca3e0d129f8"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "春昼慈闱静，宫帘日上徐。",
            "焚香开竺典，滴露写仙书。"
        ],
        "notes": "",
        "id": "b3b2d5d93f49a23aee4315ea616b507a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "春夕慈闱永，瑶池乐未央。",
            "管弦声合奏，灯月影交光。"
        ],
        "notes": "",
        "id": "c816da6fdb05d4f14cd2f69b9580e27b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "昨夜三更斗柄旋，东风催放百花鲜。",
            "飞来峰下溶新绿，流得春光到外边。"
        ],
        "notes": "",
        "id": "335f202665a758db0703f9ff45aa6b9d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "鬰葱嘉气满蓬莱，天子双称万寿杯。",
            "朝罢乐音迎复道，雕舆同上望春台。"
        ],
        "notes": "",
        "id": "e785b727d0edff71e92edfe231571d3d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "有象升平属好春，九衢歌舞乐芳辰。",
            "濯龙门外车如水，应笑豪华汉外亲。"
        ],
        "notes": "",
        "id": "313f684a3a718055fc45550ac20272a4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "待月长生殿，迎风太液池。",
            "慈颜欢乐日，圣德盛明时。"
        ],
        "notes": "",
        "id": "5982a2822e268131257b837a2d51bbff"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "濯濯风涵柳，英英露泻荷。",
            "微凉无限意，分付舜弦歌。"
        ],
        "notes": "",
        "id": "12977b457e62929a9f69c0a31a97e8cc"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "莲叶看龟上，桐花引凤栖。",
            "圣人千万寿，福禄与天齐。"
        ],
        "notes": "",
        "id": "bb5507133fd7ea47f9f70a3e205bac6b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "采索谩萦长命缕，紫芽安用引年菖。",
            "只将三纪休兵德，圣寿宜同泰华长。"
        ],
        "notes": "",
        "id": "60c02b989de7b211c6ee42133eaa3c38"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "了无尘累可关情，坐见寰区乐太平。",
            "弄水看花聊燕适，倚松餐菊偶经行。"
        ],
        "notes": "",
        "id": "12d743a14b2baa8b3aa43e579914432c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "竹殿阴阴间绿槐，日长棋罢看流杯。",
            "北窗风味沈沈静，南内时新节节来。"
        ],
        "notes": "",
        "id": "8217a0f161fa84619627077a9ec9d646"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "菰黍团云白，菖花剪玉长。",
            "晚凉新月上，水殿按霓裳。"
        ],
        "notes": "",
        "id": "b4add11c1dbb4f7724b7bae510512b7e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "海上千年实，峰头十丈花。",
            "年年当令节，同献玉皇家。"
        ],
        "notes": "",
        "id": "c9c18c5fe051017197fe3225a51d63bb"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "翰墨消长日，尊罍占远风。",
            "纤尘吹不到，人在玉壶中。"
        ],
        "notes": "",
        "id": "e158ce26723446c6cd26cdd9d7ab9956"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "宝月骖鸾采结垂，朝来新写上皇诗。",
            "金盘有露凉生早，玉宇无尘冻解迟。"
        ],
        "notes": "",
        "id": "3f53dac6222eaf210a6d12477dd27e4c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "水晶宫阙浄涵虚，历历南风度玉除。",
            "金井辘轳声欲晓，内人来奏问安书。"
        ],
        "notes": "",
        "id": "3877ddf1eac9bb9ec2d0fc396286e474"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "万年枝下绿阴长，拂石时来坐晚凉。",
            "别殿笙歌催宴早，千门铺月静焚香。"
        ],
        "notes": "",
        "id": "5710ca38ff9c0e1248ffb04e8b3ae816"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国使人到阙紫宸殿宴参军色致语口号",
        "preface": "",
        "paragraphs": [
            "春风黄繖下清厢，缨弁蝉联宴未央。",
            "万国文明周礼乐，九重端穆舜衣裳。",
            "云低殿幄星辰近，漏转宫花日月长。",
            "圣主宽仁盟好永，和声细入鹿鸣章。"
        ],
        "notes": "",
        "id": "ee4744a92a651585eb909acd500f024e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙紫宸殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "九重帐殿敞层空，清晓群臣宴镐宫。",
            "神护翠华居北极，春随黄伞下东风。",
            "觞行隠隠红云里，仗转曈曈瑞日中。",
            "看取乾坤俱寿域，两朝时节聘车通。"
        ],
        "notes": "",
        "id": "864e1e48d810a8de2322f8bb64073368"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺会应圣节使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "良月欣临震夙辰，恩涵雨露燕群臣。",
            "遥连汉岳三呼应，徐听虞韶九奏淳。",
            "仙篆萦风浮广殿，卿云擎日护严宸。",
            "奉觞更喜嘉宾会，同祝吾皇万寿春。"
        ],
        "notes": "",
        "id": "2b45de5124edaa97bd2bb87f0d9be163"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "殿头清跸起晴雷，万岁声中扇影开。",
            "晓日青旗明绛阙，春风黄伞傍瑶台。",
            "卿云涌瑞扶天座，仙露流甘入御杯。",
            "要识八荒俱寿域，年年常看使星来。"
        ],
        "notes": "",
        "id": "91986ee20895742863c6809cdbce722e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "会庆圣节使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "云护轻霜漏晓晴，日高黄伞影亭亭。",
            "六龙卫跸来临座，五凤仪韶下舞庭。",
            "地涌南山朝帝殿，天囘北斗挹仙酿。",
            "侍臣匠识宸颜喜，时顾皇皇二使星。"
        ],
        "notes": "",
        "id": "4d5a13dab67835a32505cc3723c929dc"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "四时和气转洪钧，昨夜璇杓正指寅。",
            "万玉鸣班朝广殿，五云扶辇下端宸。",
            "翠旄低引仙韶度，宝斝徐传御醴醇。",
            "看取年年来信使，定知天地一家春。"
        ],
        "notes": "",
        "id": "5d4c9dfdb82f7793ddda6dd96349dd09"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺会庆节使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "盛节簪绅宴玉京，红云深护赭袍明。",
            "宝花合座天香满，仙羽分陈御斝行。",
            "原隰有光交信睦，庙堂无事乐和平。",
            "欢心协气知多少，尽在今朝九奏声。"
        ],
        "notes": "",
        "id": "297aa12060daf5bc991cab517d710bc8"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙紫宸殿宴致语口号  其一",
        "preface": "",
        "paragraphs": [
            "东风初满殿前旗，环珮舂容侍燕时。",
            "帝泽普涵春自暖，人心同乐日先迟。",
            "三峰影里瞻鼇动，九奏声中看凤仪。",
            "更喜使华齐献寿，八方何地不熙熙。"
        ],
        "notes": "",
        "id": "b6382f539793805402518bd4de6c46a3"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙紫宸殿宴致语口号  其二",
        "preface": "",
        "paragraphs": [
            "仗转曈曈晓色暾，千官齐望赭袍尊。",
            "食苹鸣鹿迷天乐，泳藻游鱼沐圣恩。",
            "香泛流霞涵雨露，春随和奏满乾坤。",
            "使华同上南山寿，近识宸颜一倍温。"
        ],
        "notes": "",
        "id": "182ae468aee95266791e7338fd572421"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 登门肆赦皇帝升御座宫架奏黄钟宫干安之曲",
        "preface": "",
        "paragraphs": [
            "大礼备矣，銮车阗阗。",
            "帝来维何，赉及幅员。",
            "有声伊肃，有容伊虔。",
            "彼雉斯开，帝坐中天。"
        ],
        "notes": "",
        "id": "44167dc9cb467a57c3b98dbb2d902541"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 皇帝降御座宫架奏黄钟宫干安之曲",
        "preface": "",
        "paragraphs": [
            "帝坐中天，流泽演迤。",
            "厥蒙伊民，歌舞以喜。",
            "泽流普矣，民喜偕矣。",
            "驾言徂归，福禄同矣。"
        ],
        "notes": "",
        "id": "31257f22519f40759d0f98acbd95c71e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 太祖皇帝位酌献登歌作大吕宫彰安之曲",
        "preface": "",
        "paragraphs": [
            "一德开基，百年垂统。",
            "中天禘郊，薄海朝贡。",
            "宝龟相承，器鼎加重。",
            "泽深庆绵，帝复命宋。"
        ],
        "notes": "",
        "id": "d6ba459f6be28e64523b080c64a8fde1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 皇帝入小次宫架奏黄钟宫仪安之曲",
        "preface": "",
        "paragraphs": [
            "匏尊既举，苏席未移。",
            "有德斯顾，靡神不娭。",
            "物情肃穆，天宇清夷。",
            "宅中受命，永复邦基。"
        ],
        "notes": "",
        "id": "9a0587a2ecb61f3199c6886864343f4e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 亚献宫架奏黄钟宫穆安之乐威功睿德之舞",
        "preface": "",
        "paragraphs": [
            "四阿有严，神既戾止。",
            "备物虽仪，洁诚惟己。",
            "有来振振，相我熙事。",
            "载酌陶匏，以成毖祀。"
        ],
        "notes": "",
        "id": "cfea1726ed17681ab5e936d7fb9286da"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 送神宫架奏圜钟宫诚安之曲一成",
        "preface": "",
        "paragraphs": [
            "奕奕宗祀，煌煌礼文。",
            "高灵下堕，精意升闻。",
            "熙事既毕，忽乘青云。",
            "敢拜明况，永清世氛。"
        ],
        "notes": "",
        "id": "031bed8580d667025c105f38f59e042c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 皇帝还大次宫架奏黄钟宫憩安之曲",
        "preface": "",
        "paragraphs": [
            "应天以实，已事而踆。",
            "毡案朝帝，竹宫拜神。",
            "灵光下烛，协气斯陈。",
            "神禄时万，基图日新。"
        ],
        "notes": "",
        "id": "f6e104ff5af4fc3c18dd7574d328fe21"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 升坐用圣安之曲",
        "preface": "",
        "paragraphs": [
            "簪缨列序，羽卫分行。",
            "雉扇爰彻，天威炜煌。",
            "清明在躬，颙颙昂昂。",
            "咸拜稽首，万寿无疆。"
        ],
        "notes": "",
        "id": "881a24620b4f545dca7b173ef2bdb947"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 公卿入门用礼安之曲",
        "preface": "",
        "paragraphs": [
            "端闱肃启，金奏和声。",
            "有来济济，烨其华缨。",
            "度中规矩，步协英茎。",
            "一人达孝，百辟丕承。"
        ],
        "notes": "",
        "id": "0b9c6ef862f704a189252bf9ffb2f526"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 皇帝上太上皇寿酒用福安之曲",
        "preface": "",
        "paragraphs": [
            "笙镛皦绎，簨簴腾骧。",
            "奉觞介寿，龙衮黼裳。",
            "盛仪克举，至德用章。",
            "刑于四海，化洽风扬。"
        ],
        "notes": "",
        "id": "dad4a8e7b788bdd8887291920d1da7cb"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 上寿用崇安之曲",
        "preface": "",
        "paragraphs": [
            "凝旒肃穆，鸣佩舂容。",
            "有酒伊醑，管磬其从。",
            "典礼绚缛，威仪恪共。",
            "颂尧之寿，与天比隆。"
        ],
        "notes": "",
        "id": "fb3a0efc509beec31a565c4126f3b03f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 太上皇帝初举酒用蟠桃之曲",
        "preface": "",
        "paragraphs": [
            "结根阆苑，登实瑶池。",
            "红云骈艳，瑞露含滋。",
            "味参石髓，名配松脂。",
            "王母一笑，河水涟漪。"
        ],
        "notes": "",
        "id": "00fc8c37688af8192cd077b55480aee5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 再举酒用大椿之曲",
        "preface": "",
        "paragraphs": [
            "培植自古，质淳以全。",
            "凌傲霜雹，盘薄风烟。",
            "朝菌焉识，楚蓂讵肩。",
            "耸柯擢干，终古苍然。"
        ],
        "notes": "",
        "id": "ccd900165deb1b24a4a9c714b5597af4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 三举酒用瑞鹤之曲",
        "preface": "",
        "paragraphs": [
            "彼肃者羽，名纪仙经。",
            "芝田下啄，容止安停。",
            "引吭警露，响彻青冥。",
            "圣德格物，来仪帝庭。"
        ],
        "notes": "",
        "id": "6413321fd9d3580a7d69cafaff8fa3c9"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 四举酒用祥龟之曲",
        "preface": "",
        "paragraphs": [
            "粤有灵物，负图效祥。",
            "莲栖蓍宿，锦文有章。",
            "天和发越，睿泽滂洋。",
            "溯于宫沼，美应昭彰。"
        ],
        "notes": "",
        "id": "e2c50c956aba5d8f7ee18a1674c532af"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 五举酒用老人星见之曲",
        "preface": "",
        "paragraphs": [
            "奕奕南极，应期曜灵。",
            "祥晖昭衍，次丙躔丁。",
            "事垂汉史，名著甘经。",
            "锡符祚圣，亿万斯龄。"
        ],
        "notes": "",
        "id": "921becdd6515fbd9ebaa17ec5a63f498"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 降坐用福安之曲",
        "preface": "",
        "paragraphs": [
            "庆典具举，皇慈溥将。",
            "礼光长乐，事掩未央。",
            "佩环济济，金石锵锵。",
            "天容有怿，言旋建章。"
        ],
        "notes": "",
        "id": "e4cf39d3017d86b57931948008c4df81"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 群臣酒三行竝用正安之曲 其一",
        "preface": "",
        "paragraphs": [
            "晓日敞天门，星环萃盍簪。",
            "明良赓舜日，富寿祝尧心。",
            "穿仗摇旗影，鸣珂混乐音。",
            "圣朝敦孝理，百辟尽丕钦。"
        ],
        "notes": "",
        "id": "f023acc1cae31293bb362603642f1100"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 群臣酒三行竝用正安之曲 其二",
        "preface": "",
        "paragraphs": [
            "缥缈殿中央，红云捧紫皇。",
            "香风飘彩仗，仙露滴霞觞。",
            "乐度仪凰曲，恩流振鹭行。",
            "万年天下养，宝祚享灵长。"
        ],
        "notes": "",
        "id": "489b273b939a7e67d006e73d9f3f41c1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 群臣酒三行竝用正安之曲 其三",
        "preface": "",
        "paragraphs": [
            "清漏滴彤壶，皇仁雨露濡。",
            "遥瞻天一笑，同效岳三呼。",
            "御醴传觞速，宫花覆座敷。",
            "欲知归美意，歌颂在康衢。"
        ],
        "notes": "",
        "id": "6adae65aacc4931fd75280289268626c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 酒一行罢用万寿无疆之舞",
        "preface": "",
        "paragraphs": [
            "三纪御图，道丰德盛。",
            "化洽兆民，功光列圣。",
            "畀托有归，脱屣万乘。",
            "优游法宫，颐神养性。",
            "岧岧具阙，丰丽靖渊。",
            "游神象外，玩意物先。",
            "为众父父，尊荣配天。",
            "大福备禄，何千万年。"
        ],
        "notes": "",
        "id": "c4335e4169f6683a2b6f9e793a842cc1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 酒再行罢用圣德重光之舞",
        "preface": "",
        "paragraphs": [
            "於穆圣子，仁天智神。",
            "体刚立极，宝俭化民。",
            "美政毕举，旧章聿遵。",
            "舞以象德，拟韶实伦。",
            "龙楼问寝，前殿奉巵。",
            "密承慈训，不怠其祗。",
            "海宇丰阜，边陲晏夷。",
            "于以养志，福禄来宜。"
        ],
        "notes": "",
        "id": "78b20bc0a017641eeab71901897b6ca6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "正月六日集玉壶次才元韵",
        "preface": "",
        "paragraphs": [
            "铃索文书晚罢休，归来欲赋畔牢愁。",
            "感君尊酒招要意，适我湖山烂熳游。",
            "隔岸柳烟匀拂掠，近溪梅粉半漂流。",
            "千林紫翠无穷在，后会应须到上头。"
        ],
        "notes": "",
        "id": "b022344101c70673bda866890b566d56"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "方壶",
        "preface": "",
        "paragraphs": [
            "垂杨阴下小亭池，浓绿繁红掩映宜。",
            "挟册独来谁共语，海棠相对笑人痴。"
        ],
        "notes": "",
        "id": "4312188d3de49240f114d167ebe9949f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和唐致远蜡梅",
        "preface": "",
        "paragraphs": [
            "万木折冰雪，千葩谢华秾。",
            "谁约金童仙，冲寒下严风。",
            "恼人风味深，缕衣立隆冬。",
            "旁有玉京子，含香笑相逢。",
            "夜水吐玉龙，月窗影相重。",
            "诗情太牢落，聊以二友供。",
            "清标体黄裳，素姿掩凡容。",
            "东君著意先，群英绝追踪。"
        ],
        "notes": "",
        "id": "b376782e7aef230c0fc0997536624914"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和甄监簿述怀",
        "preface": "",
        "paragraphs": [
            "一鸣当日众俱惊，曾掞天庭赋太清。",
            "宦路喜君方晚遇，文场推我是虚声。",
            "短材漫饱支离粟，藻思宜粲沆瀣英。",
            "近喜和光成莫逆，嘉名洋溢鲁诸生。"
        ],
        "notes": "",
        "id": "81c9925b9a75921cd3d218b4dd695f2e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "词",
        "rhythmic": "六州",
        "title": "",
        "preface": "",
        "paragraphs": [
            "商秋吉，嘉会协中辛。",
            "涓路寝，修禋祀，圣德昭清。",
            "端志虑，罄竭斋精。",
            "锦绣排天仗，羽卫滨纷。",
            "朝太室、返中宸。",
            "被衮接神明。",
            "时平天时俱清晏，兼丰年和气，品物达芳馨。",
            "瞻熉座，舂容娭燕三灵。",
            "奠瑶爵，荐量币，清思呦冥冥。",
            "望昆仑。",
            "嘉祥塞絪緼。",
            "诚殚礼洽庆休成。",
            "润泽被生民。",
            "端门肆眚，昕庭称贺，俱将景福万寿、祝双亲。"
        ],
        "notes": "",
        "id": "d039c995d37048be00050b6d100887ec"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "词",
        "rhythmic": "十二时・忆少年",
        "title": "",
        "preface": "",
        "paragraphs": [
            "勋华并、天胙昌期。",
            "圣德茂重离。",
            "英明经远，浚哲昭微。",
            "宝俭更深慈。",
            "观万国、累洽重熙。",
            "对明时。",
            "报礼秩神祗。",
            "玉帛奏华夷。",
            "雍肃显相，百辟各钦祗。",
            "奄嘉虞英璧奠华滋。",
            "神安坐、景气澄虚。",
            "极光焰、烛长丽。",
            "展诗应律，万舞逶迟。",
            "三献洽皇仪。",
            "垂露浸庆祜来宜。",
            "礼无违。",
            "鸣鸾临帝阙，飞凤下天倪。",
            "清和寰宇，霈泽一朝驰。",
            "醇化无为。",
            "万祀巩洪基。"
        ],
        "notes": "",
        "id": "15aa819687641020d5f4b8e78ff2666b"
    }
]