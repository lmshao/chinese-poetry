[
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "双秀云牟实，三眠雪茧丰。",
            "无人知帝力，浑在舜弦中。"
        ],
        "notes": "",
        "id": "7099b89fdad933c47c554a31e633c006"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "御柳垂波绿，宫槐覆幄凉。",
            "香风随步辇，偏到殿中央。"
        ],
        "notes": "",
        "id": "573c8df0c8c15f82daa9981cbafc49fc"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "采缕萦朱户，芳菰剪翠筒。",
            "风光天上别，景物世间同。"
        ],
        "notes": "",
        "id": "98e9f452b1a01939ca06e9e433154e85"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "千年桃印明金殿，百子榴房照绮疏。",
            "清晓宫门放鱼钥，内家催进恤刑书。"
        ],
        "notes": "",
        "id": "f9fee25f9c7e4ff5fd33411f354e1eb1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "幽芳拂拂朝香度，密叶阴阴昼影圆。",
            "玉食未应须角黍，君王端是念忠贤。"
        ],
        "notes": "",
        "id": "5592b8057417f6662a26b56b700fb6f8"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙元年端午帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "双人绿艾消民沴，五色朱丝奉帝龄。",
            "向晚封章都阅遍，翠舆初过水心亭。"
        ],
        "notes": "",
        "id": "8ad4e032186d1c59c5b1972e1d800409"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "嶰管今朝应，璇杓昨夜旋。",
            "耕桑三万里，一样乐尧天。"
        ],
        "notes": "",
        "id": "0837b1188d101608335f353c9f529bf4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "事已高超古，心犹切为民。",
            "慈颜应有喜，房宿正当晨。"
        ],
        "notes": "",
        "id": "869efb3459af1e955471d170e9db1b82"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "采柏浮仙醁，凝酥点寿盘。",
            "君王千万岁，长奉两宫欢。"
        ],
        "notes": "",
        "id": "1a44d0baf1181269c807e69971ee4091"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "九重云气鬰崔嵬，日转青旗瑞色开。",
            "晨跸一声春又到，太平天子上瑶杯。"
        ],
        "notes": "",
        "id": "2e5ed4c88191a80d395bcc6f517fc9cc"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "楼阁隔年余旧雪，园林连夜著新花。",
            "东皇拟作行春计，先到长生大帝家。"
        ],
        "notes": "",
        "id": "ff1394ef32d222bf771309e35e25a543"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 光尧寿圣宪天体道太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "景龙门上华灯动，黄鹄池边翠浪通。",
            "一一春光须作主，从今二十四番风。"
        ],
        "notes": "",
        "id": "67d351d57401948d003bd5b37f92d854"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "玉管和声度，金铺丽景迟。",
            "思齐文母圣，春日受春祺。"
        ],
        "notes": "",
        "id": "128f8a2bee9a7f9146b6c7953c02f412"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "宝殿春朝退，瑶池夜宴深。",
            "明朝是人日，时复问晴阴。"
        ],
        "notes": "",
        "id": "ca9d93696131ee182024b6a003243772"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "翠辇西湖路，雕盘北苑花。",
            "时平无外事，随意趁年华。"
        ],
        "notes": "",
        "id": "18a17b647eb5fa700028b4c89126d8b9"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "扇开雉影红云起，帘卷虾须画烛明。",
            "三十六宫齐进酒，盘金小胜缕长生。"
        ],
        "notes": "",
        "id": "84ebffbc339675c33cc4e452a0cdccd0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "日溶凤沼摇波暖，云护龙楼倒影长。",
            "读罢黄庭无一事，好风吹动百花香。"
        ],
        "notes": "",
        "id": "62e634706f1377a307a1b18efcbca5c8"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙二年春帖子词 寿圣明慈太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "院落韶光归柳色，郊原耕信到菖芽。",
            "望春台下□□软，不见游龙有外家。"
        ],
        "notes": "",
        "id": "4b7d87f190b19732091665d32bc2a12f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "今岁韶光好，时逢大有年。",
            "条风方被物，菖叶又催田。"
        ],
        "notes": "",
        "id": "e052d8288362e3c03568c99583b0dbd0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "今岁韶光好，年中两见春。",
            "余寒九日在，芳意一朝新。"
        ],
        "notes": "",
        "id": "e052d8288362e3c03568c99583b0dbd0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "今岁韶光好，田闾气象淳。",
            "政平无横赋，粟贱少穷民。"
        ],
        "notes": "",
        "id": "e052d8288362e3c03568c99583b0dbd0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "东风先满殿前旗，御柳宫花次第知。",
            "连夜芳心相待发，侵朝柔思不胜垂。"
        ],
        "notes": "",
        "id": "dc65bb8f43974a5b4b6e1cf9f53465e4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "千门晓日山河丽，万国春风草木香。",
            "造化无私随发育，圣明天子是东皇。"
        ],
        "notes": "",
        "id": "18da135cc58f5f1c37f485bb1b1ed563"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "雕庭清跸转轻雷，凤管吹春入仗来。",
            "万籁有声皆善颂，八荒无地不熙台。"
        ],
        "notes": "",
        "id": "0821c6a98371f627d590dc352fb51180"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其一",
        "preface": "",
        "paragraphs": [
            "何处春来早，光风入九门。",
            "未翻池荇翠，先著壁椒温。"
        ],
        "notes": "",
        "id": "77632746c3496a7ca4a1f34dc83a8e0f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其二",
        "preface": "",
        "paragraphs": [
            "何处春来早，坤仪物意熙。",
            "烟生金屋重，日上玉阶迟。"
        ],
        "notes": "",
        "id": "77632746c3496a7ca4a1f34dc83a8e0f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其三",
        "preface": "",
        "paragraphs": [
            "明朝嘉气满慈闱，亲侍君王献玉巵。",
            "一样酥盘俱手制，百般彩胜总春宜。"
        ],
        "notes": "",
        "id": "d1054e17966a9bb6f59feca475fd7c61"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其四",
        "preface": "",
        "paragraphs": [
            "风光天上倍常殊，花压阑干昼影舒。",
            "虚几自临中令帖，明窗时展大家书。"
        ],
        "notes": "",
        "id": "2d43f033e9d45a04f5ce0a65be232c55"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年春帖子词 皇后合五首 其五",
        "preface": "",
        "paragraphs": [
            "清明小苑条桑地，和暖平川浴种天。",
            "圣主俭勤游乐少，只将敦朴示民先。"
        ],
        "notes": "",
        "id": "87e3f7db58813861d929e6abd5e3d012"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "玉斝浮菖细，金盘荐术香。",
            "皇龄千万寿，高拱舜衣裳。"
        ],
        "notes": "",
        "id": "644b2da6ea00279b298f03d3f3049825"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "兰气浮丹殿，槐阴被紫宸。",
            "薰琴多在御，挥拂寄深仁。"
        ],
        "notes": "",
        "id": "a82eca04395d693d0151cd5cadc0f908"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "清晓开鱼钥，红云缥缈间。",
            "催裁恤刑诏，传放谢衣班。"
        ],
        "notes": "",
        "id": "c915e89448010a5865afc29546e94c40"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "夜披章奏凉生扇，午对臣邻汗浃衣。",
            "水阁风亭松竹邃，翠舆临幸日来稀。"
        ],
        "notes": "",
        "id": "a8e9305bf04adf70da0863ce966706f8"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "仁风长养群生遂，化日清明百沴驱。",
            "桃印艾人消底用，殿心高设敬天图。"
        ],
        "notes": "",
        "id": "23e6e11557671d0faf2125a0afc64eea"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "避暑深宫消永昼，函风广殿起凉秋。",
            "宸心未惬高明适，志在山东二百州。"
        ],
        "notes": "",
        "id": "44b4722cb6c8f577f225bbb8445000e0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其一",
        "preface": "",
        "paragraphs": [
            "剪玉菰筒翠，盘金彩缕长。",
            "宫中多燕喜，天下正明昌。"
        ],
        "notes": "",
        "id": "9ace474f63ac50c36400a204a655b416"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其二",
        "preface": "",
        "paragraphs": [
            "瑞麦登时物，香蒲荐寿祺。",
            "珠囊新样小，蹙就葛覃诗。"
        ],
        "notes": "",
        "id": "46142a3f7b98df0bed9ed3aafb378a97"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其三",
        "preface": "",
        "paragraphs": [
            "鬭草人归午漏徐，宫帘不动玉窗虚。",
            "旋开蕊笈寻仙典，闲点松腴试法书。"
        ],
        "notes": "",
        "id": "dce5e1aa1e7e8c85b9d1203ffe999060"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其四",
        "preface": "",
        "paragraphs": [
            "蚕馆风清下箔时，六宫□从献丝归。",
            "便教渊室催机织，要及明堂制衮衣。"
        ],
        "notes": "",
        "id": "e167512483d4c55f0b74af7acb0813d1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙六年端午帖子词 皇后合五首 其五",
        "preface": "",
        "paragraphs": [
            "圣主恭勤少燕游，生衣趁得未明求。",
            "随时但献长生缕，当午犹闲竞渡舟。"
        ],
        "notes": "",
        "id": "45c0168cb91ebffecb3610763a453586"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "欲识春囘处，长生大帝家。",
            "万年枝耸翠，千岁果开花。"
        ],
        "notes": "",
        "id": "8067ebcac9ef2ca77e1e9b97ed3a8275"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "欲识春囘处，君王献寿时。",
            "矞云扶宝座，和气入瑶巵。"
        ],
        "notes": "",
        "id": "8067ebcac9ef2ca77e1e9b97ed3a8275"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "欲识春囘处，晴峰俯碧泉。",
            "岩峦犹积雪，草木已生烟。"
        ],
        "notes": "",
        "id": "8067ebcac9ef2ca77e1e9b97ed3a8275"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "冰消宿沼悠扬动，烟暖寒林约略青。",
            "试上龙楼囘远望，朝来生绿画罗屏。"
        ],
        "notes": "",
        "id": "9c6a90b3a68e8a53576872de7d827738"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "聚景园中芳意换，望湖楼下绿波长。",
            "厖眉自识车音美，时有𬘡缊夹道香。"
        ],
        "notes": "",
        "id": "8242b1b0b3930fc6a6066587c2a8fa6b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 光尧寿圣宪天体道性仁诚德经武纬文太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "一气暗随鸾辂动，万祥给会衮衣朝。",
            "康衢歌吹东风里，满听儿童善祝尧。"
        ],
        "notes": "",
        "id": "eb5167062ca3b69caff67b4749fc3b63"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "天上春光别，东皇管领来。",
            "初从旗气转，旋逐跸声囘。"
        ],
        "notes": "",
        "id": "c69128c0a39e723ffc539f49d485e191"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "天上春光别，融融物态宜。",
            "彤庭花气暖，黄道日轮迟。"
        ],
        "notes": "",
        "id": "c69128c0a39e723ffc539f49d485e191"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "天上春光别，时康乐事繁。",
            "烟花浮阆苑，露叶簇金盘。"
        ],
        "notes": "",
        "id": "c69128c0a39e723ffc539f49d485e191"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "苍龙檠辇道春囘，尽把韶华入酒杯。",
            "但见殿前移仗退，已闻苑内奏花开。"
        ],
        "notes": "",
        "id": "abb8d0c13c02b46e9e101ad173b59eb0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "玉压阑干日影长，云窗侍女昼焚香。",
            "琅函自检长生箓，金管时书急就章。"
        ],
        "notes": "",
        "id": "b34b61e3f2002d767f80192ee90eaabf"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年春帖子 寿圣齐明广慈太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "花边破日红千叠，柳际匀烟翠一层。",
            "阅尽风光长不老，绿车当见从云仍。"
        ],
        "notes": "",
        "id": "43e299b90f7c7b6b40f575432182f4b8"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "赤伏祥晖盛，黄离治象亨。",
            "天时四序正，君德万方明。"
        ],
        "notes": "",
        "id": "4d3e0eea76bfe5f02cedd2d6f4233515"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "采缕盘金丽，香蒲镂玉匀。",
            "君王千万寿，岁岁乐兹辰。"
        ],
        "notes": "",
        "id": "291a334f58230e5ac268e0c27730ddd6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "日御秋秋永，云峰叠叠繁。",
            "宸心定忘暑，长算入中原。"
        ],
        "notes": "",
        "id": "1673e3209749b76c2c9dfd628a42fe4d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "光浮水槛虚黄盖，凉满冰台待翠舆。",
            "日转槐龙天近午，汗衣犹听讲筵书。"
        ],
        "notes": "",
        "id": "91c4cc56297e7ee833784899c19caa73"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "松碧涵风清度曲，梅黄过雨密留阴。",
            "翠寒堂上全无暑，定起人间扇暍心。"
        ],
        "notes": "",
        "id": "f4f749ae098a4cdb927d7d65ac8f0c28"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "黄道星辰移企翼，青冥风露近飞簷。",
            "翠华晚过凌虚殿，一色明珠十二帘。"
        ],
        "notes": "",
        "id": "65649f08b1aa7b05f14cd22da0181c4d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "玉燕垂符小，珠囊结艾青。",
            "更将长命缕，侵晓奉慈庭。"
        ],
        "notes": "",
        "id": "69df6c78227b757b961e4d9bc046c9c5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "厚德承三殿，柔风被八纮。",
            "欲知勤俭化，当暑澣衣清。"
        ],
        "notes": "",
        "id": "cc93c8e9dbc2180b32262fa3e9bb51a1"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "玉斝浮菖追令节，宝箱储药趁灵辰。",
            "细开角黍询前事，应助吾皇念直臣。"
        ],
        "notes": "",
        "id": "9bfb6fa7e09e3b034ebd382db67c352f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "日永荇池闲画舫，风清蚕馆入新丝。",
            "内人鬭草归能说，今岁山中艾出迟。"
        ],
        "notes": "",
        "id": "aa89973e94ec6b587198aedbec6cbd55"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙七年端午帖子词 皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "翠云幕卷迎风殿，零露盘高待月台。",
            "应上君王千万寿，霓裳吹下九天来。"
        ],
        "notes": "",
        "id": "7cc0e77bae943be53fbfa53dd972b946"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "翠管吹寒去，青旗卷仗来。",
            "慈皇千万寿，春日上春杯。"
        ],
        "notes": "",
        "id": "723451566ffb33d9136692983d3e3740"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "剪玉酥花细，盘金彩胜宜。",
            "六宫呈妙巧，春日颂春祺。"
        ],
        "notes": "",
        "id": "886b11a44c8fb7d38bb0c8b8f3acbd00"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "凤苑开丹燕，龙楼下翠华。",
            "天工不敢住，春日著春花。"
        ],
        "notes": "",
        "id": "56fbbc3d719947d81407b001e6ed6c1c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "亲提神器授今皇，帝德王功日日昌。",
            "万宇熙台无一事，湖山好处赏风光。"
        ],
        "notes": "",
        "id": "902641ebee3c6ec8cde8db2ecf85b062"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "今岁东皇作意迟，春朝恰近上元期。",
            "长生宫殿花开了，便放人间十万枝。"
        ],
        "notes": "",
        "id": "c6c1604e15dc49bb6bf5488807ef8154"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "高蹈殊庭二十春，随时游乐为同民。",
            "翠舆黄伞西湖路，老耨年年喜望尘。"
        ],
        "notes": "",
        "id": "921ad8f2601be15ec91ab799fd1832a6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "春晓慈闱启，君王奏问安。",
            "和声调嶰管，欢颂献椒盘。"
        ],
        "notes": "",
        "id": "c9705e6ef13713a82cc661146c5af715"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "春昼慈闱静，宫帘日上徐。",
            "焚香开竺典，滴露写仙书。"
        ],
        "notes": "",
        "id": "f73314118ddaf66f4a9a26e5d05e0436"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "春夕慈闱永，瑶池乐未央。",
            "管弦声合奏，灯月影交光。"
        ],
        "notes": "",
        "id": "8016defd21f063f0dad342cec235d646"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "昨夜三更斗柄旋，东风催放百花鲜。",
            "飞来峰下溶新绿，流得春光到外边。"
        ],
        "notes": "",
        "id": "432e0d9218c14d11f8e95be95719a168"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "鬰葱嘉气满蓬莱，天子双称万寿杯。",
            "朝罢乐音迎复道，雕舆同上望春台。"
        ],
        "notes": "",
        "id": "e5b98316534f5ac23e88a875b655045f"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年春帖子词 太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "有象升平属好春，九衢歌舞乐芳辰。",
            "濯龙门外车如水，应笑豪华汉外亲。"
        ],
        "notes": "",
        "id": "9f155b02e3b354c1056d2e5443bcf99c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其一",
        "preface": "",
        "paragraphs": [
            "待月长生殿，迎风太液池。",
            "慈颜欢乐日，圣德盛明时。"
        ],
        "notes": "",
        "id": "295dc4b10218bbf4daca246fee3640ab"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其二",
        "preface": "",
        "paragraphs": [
            "濯濯风涵柳，英英露泻荷。",
            "微凉无限意，分付舜弦歌。"
        ],
        "notes": "",
        "id": "5642142b26f8ece3a348c80d68b338da"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其三",
        "preface": "",
        "paragraphs": [
            "莲叶看龟上，桐花引凤栖。",
            "圣人千万寿，福禄与天齐。"
        ],
        "notes": "",
        "id": "493010725696b6af719583d8990d3da6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其四",
        "preface": "",
        "paragraphs": [
            "采索谩萦长命缕，紫芽安用引年菖。",
            "只将三纪休兵德，圣寿宜同泰华长。"
        ],
        "notes": "",
        "id": "04d2ee79bd9ff6a5b62275673f0fc6bf"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其五",
        "preface": "",
        "paragraphs": [
            "了无尘累可关情，坐见寰区乐太平。",
            "弄水看花聊燕适，倚松餐菊偶经行。"
        ],
        "notes": "",
        "id": "762040d3e25003ad4ab972d7b0889ce6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇帝合六首 其六",
        "preface": "",
        "paragraphs": [
            "竹殿阴阴间绿槐，日长棋罢看流杯。",
            "北窗风味沈沈静，南内时新节节来。"
        ],
        "notes": "",
        "id": "7944e954de2f4c0bd39f385115d6352c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其一",
        "preface": "",
        "paragraphs": [
            "菰黍团云白，菖花剪玉长。",
            "晚凉新月上，水殿按霓裳。"
        ],
        "notes": "",
        "id": "42c2bcd457ac98eb6f0a3f6d03b5b4b4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其二",
        "preface": "",
        "paragraphs": [
            "海上千年实，峰头十丈花。",
            "年年当令节，同献玉皇家。"
        ],
        "notes": "",
        "id": "e67c102f86d2dfe6f644f82ed4229b8c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其三",
        "preface": "",
        "paragraphs": [
            "翰墨消长日，尊罍占远风。",
            "纤尘吹不到，人在玉壶中。"
        ],
        "notes": "",
        "id": "ae07a6528abf67bc2632c4a8e58d7308"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其四",
        "preface": "",
        "paragraphs": [
            "宝月骖鸾采结垂，朝来新写上皇诗。",
            "金盘有露凉生早，玉宇无尘冻解迟。"
        ],
        "notes": "",
        "id": "0674fbbce8f625725997495e55c80b04"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其五",
        "preface": "",
        "paragraphs": [
            "水晶宫阙浄涵虚，历历南风度玉除。",
            "金井辘轳声欲晓，内人来奏问安书。"
        ],
        "notes": "",
        "id": "336ae1babb43a51abe3e273ad8d7c339"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "淳熙八年端午帖子词 太上皇后合六首 其六",
        "preface": "",
        "paragraphs": [
            "万年枝下绿阴长，拂石时来坐晚凉。",
            "别殿笙歌催宴早，千门铺月静焚香。"
        ],
        "notes": "",
        "id": "a9e4d8e47ef173bbf55da409215d5f45"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国使人到阙紫宸殿宴参军色致语口号",
        "preface": "",
        "paragraphs": [
            "春风黄繖下清厢，缨弁蝉联宴未央。",
            "万国文明周礼乐，九重端穆舜衣裳。",
            "云低殿幄星辰近，漏转宫花日月长。",
            "圣主宽仁盟好永，和声细入鹿鸣章。"
        ],
        "notes": "",
        "id": "cae26f9d92d87b07385c5cb4b85ab3b9"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙紫宸殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "九重帐殿敞层空，清晓群臣宴镐宫。",
            "神护翠华居北极，春随黄伞下东风。",
            "觞行隠隠红云里，仗转曈曈瑞日中。",
            "看取乾坤俱寿域，两朝时节聘车通。"
        ],
        "notes": "",
        "id": "b1ec6139db000b9b9a87d3a59073c742"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺会应圣节使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "良月欣临震夙辰，恩涵雨露燕群臣。",
            "遥连汉岳三呼应，徐听虞韶九奏淳。",
            "仙篆萦风浮广殿，卿云擎日护严宸。",
            "奉觞更喜嘉宾会，同祝吾皇万寿春。"
        ],
        "notes": "",
        "id": "e98c7db23ceb367e4d53af6a1ff26d6a"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "殿头清跸起晴雷，万岁声中扇影开。",
            "晓日青旗明绛阙，春风黄伞傍瑶台。",
            "卿云涌瑞扶天座，仙露流甘入御杯。",
            "要识八荒俱寿域，年年常看使星来。"
        ],
        "notes": "",
        "id": "1cc3fd04dd6de8882ebc7a7b82057e5b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "会庆圣节使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "云护轻霜漏晓晴，日高黄伞影亭亭。",
            "六龙卫跸来临座，五凤仪韶下舞庭。",
            "地涌南山朝帝殿，天囘北斗挹仙酿。",
            "侍臣匠识宸颜喜，时顾皇皇二使星。"
        ],
        "notes": "",
        "id": "74aaee404be0b185404c7d1135a33dd5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "四时和气转洪钧，昨夜璇杓正指寅。",
            "万玉鸣班朝广殿，五云扶辇下端宸。",
            "翠旄低引仙韶度，宝斝徐传御醴醇。",
            "看取年年来信使，定知天地一家春。"
        ],
        "notes": "",
        "id": "c332176ffdf2b5398bec4a8262aff27d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺会庆节使人到阙集英殿宴致语口号",
        "preface": "",
        "paragraphs": [
            "盛节簪绅宴玉京，红云深护赭袍明。",
            "宝花合座天香满，仙羽分陈御斝行。",
            "原隰有光交信睦，庙堂无事乐和平。",
            "欢心协气知多少，尽在今朝九奏声。"
        ],
        "notes": "",
        "id": "6c2848278fd226fd82c77b7bd16b3802"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙紫宸殿宴致语口号  其一",
        "preface": "",
        "paragraphs": [
            "东风初满殿前旗，环珮舂容侍燕时。",
            "帝泽普涵春自暖，人心同乐日先迟。",
            "三峰影里瞻鼇动，九奏声中看凤仪。",
            "更喜使华齐献寿，八方何地不熙熙。"
        ],
        "notes": "",
        "id": "f0e4d0289d1697a9d3788591cbb0b1c5"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "金国贺正旦使人到阙紫宸殿宴致语口号  其二",
        "preface": "",
        "paragraphs": [
            "仗转曈曈晓色暾，千官齐望赭袍尊。",
            "食苹鸣鹿迷天乐，泳藻游鱼沐圣恩。",
            "香泛流霞涵雨露，春随和奏满乾坤。",
            "使华同上南山寿，近识宸颜一倍温。"
        ],
        "notes": "",
        "id": "4363757a9497a94d9f9a3440a266b67e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 登门肆赦皇帝升御座宫架奏黄钟宫干安之曲",
        "preface": "",
        "paragraphs": [
            "大礼备矣，銮车阗阗。",
            "帝来维何，赉及幅员。",
            "有声伊肃，有容伊虔。",
            "彼雉斯开，帝坐中天。"
        ],
        "notes": "",
        "id": "c9e6e4c05044266598e812c14d074c86"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 皇帝降御座宫架奏黄钟宫干安之曲",
        "preface": "",
        "paragraphs": [
            "帝坐中天，流泽演迤。",
            "厥蒙伊民，歌舞以喜。",
            "泽流普矣，民喜偕矣。",
            "驾言徂归，福禄同矣。"
        ],
        "notes": "",
        "id": "01ac3fc8d70267fc9146efc7cf2ae910"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 太祖皇帝位酌献登歌作大吕宫彰安之曲",
        "preface": "",
        "paragraphs": [
            "一德开基，百年垂统。",
            "中天禘郊，薄海朝贡。",
            "宝龟相承，器鼎加重。",
            "泽深庆绵，帝复命宋。"
        ],
        "notes": "",
        "id": "cac539cfba4c38427856117578879d57"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 皇帝入小次宫架奏黄钟宫仪安之曲",
        "preface": "",
        "paragraphs": [
            "匏尊既举，苏席未移。",
            "有德斯顾，靡神不娭。",
            "物情肃穆，天宇清夷。",
            "宅中受命，永复邦基。"
        ],
        "notes": "",
        "id": "d72c49769effff25e1148d8ecdb40937"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 亚献宫架奏黄钟宫穆安之乐威功睿德之舞",
        "preface": "",
        "paragraphs": [
            "四阿有严，神既戾止。",
            "备物虽仪，洁诚惟己。",
            "有来振振，相我熙事。",
            "载酌陶匏，以成毖祀。"
        ],
        "notes": "",
        "id": "5883b1dfdbd0f6725b4ef7ac2538888d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 送神宫架奏圜钟宫诚安之曲一成",
        "preface": "",
        "paragraphs": [
            "奕奕宗祀，煌煌礼文。",
            "高灵下堕，精意升闻。",
            "熙事既毕，忽乘青云。",
            "敢拜明况，永清世氛。"
        ],
        "notes": "",
        "id": "f39dc33a4b6c277ff1a0fcd823647784"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "郊祀乐章 皇帝还大次宫架奏黄钟宫憩安之曲",
        "preface": "",
        "paragraphs": [
            "应天以实，已事而踆。",
            "毡案朝帝，竹宫拜神。",
            "灵光下烛，协气斯陈。",
            "神禄时万，基图日新。"
        ],
        "notes": "",
        "id": "90da34d219c5baa657baec4349e7b72b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 升坐用圣安之曲",
        "preface": "",
        "paragraphs": [
            "簪缨列序，羽卫分行。",
            "雉扇爰彻，天威炜煌。",
            "清明在躬，颙颙昂昂。",
            "咸拜稽首，万寿无疆。"
        ],
        "notes": "",
        "id": "2a271967adebcbe8b2edf9851713ef28"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 公卿入门用礼安之曲",
        "preface": "",
        "paragraphs": [
            "端闱肃启，金奏和声。",
            "有来济济，烨其华缨。",
            "度中规矩，步协英茎。",
            "一人达孝，百辟丕承。"
        ],
        "notes": "",
        "id": "704b227bb31c324c7ce3abcc83948d5b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 皇帝上太上皇寿酒用福安之曲",
        "preface": "",
        "paragraphs": [
            "笙镛皦绎，簨簴腾骧。",
            "奉觞介寿，龙衮黼裳。",
            "盛仪克举，至德用章。",
            "刑于四海，化洽风扬。"
        ],
        "notes": "",
        "id": "4b91dc24da2c9c9fa2394964808bd88b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 上寿用崇安之曲",
        "preface": "",
        "paragraphs": [
            "凝旒肃穆，鸣佩舂容。",
            "有酒伊醑，管磬其从。",
            "典礼绚缛，威仪恪共。",
            "颂尧之寿，与天比隆。"
        ],
        "notes": "",
        "id": "5ed6a3e53d3a5204f015c0deb527be3e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 太上皇帝初举酒用蟠桃之曲",
        "preface": "",
        "paragraphs": [
            "结根阆苑，登实瑶池。",
            "红云骈艳，瑞露含滋。",
            "味参石髓，名配松脂。",
            "王母一笑，河水涟漪。"
        ],
        "notes": "",
        "id": "a387f2cee6afa1ef46768f335de8fb9d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 再举酒用大椿之曲",
        "preface": "",
        "paragraphs": [
            "培植自古，质淳以全。",
            "凌傲霜雹，盘薄风烟。",
            "朝菌焉识，楚蓂讵肩。",
            "耸柯擢干，终古苍然。"
        ],
        "notes": "",
        "id": "36a919b909faa2cf75f6aefafc9e2b38"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 三举酒用瑞鹤之曲",
        "preface": "",
        "paragraphs": [
            "彼肃者羽，名纪仙经。",
            "芝田下啄，容止安停。",
            "引吭警露，响彻青冥。",
            "圣德格物，来仪帝庭。"
        ],
        "notes": "",
        "id": "7677f399b9100210f5e989503514f51d"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 四举酒用祥龟之曲",
        "preface": "",
        "paragraphs": [
            "粤有灵物，负图效祥。",
            "莲栖蓍宿，锦文有章。",
            "天和发越，睿泽滂洋。",
            "溯于宫沼，美应昭彰。"
        ],
        "notes": "",
        "id": "0ede73f35eec30559d1e6dd5614875d3"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 五举酒用老人星见之曲",
        "preface": "",
        "paragraphs": [
            "奕奕南极，应期曜灵。",
            "祥晖昭衍，次丙躔丁。",
            "事垂汉史，名著甘经。",
            "锡符祚圣，亿万斯龄。"
        ],
        "notes": "",
        "id": "96cdc90e2f4541a30e0bc830cbebf5f4"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 降坐用福安之曲",
        "preface": "",
        "paragraphs": [
            "庆典具举，皇慈溥将。",
            "礼光长乐，事掩未央。",
            "佩环济济，金石锵锵。",
            "天容有怿，言旋建章。"
        ],
        "notes": "",
        "id": "9c76e2fd154d0e736ab5790f31a59d2b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 群臣酒三行竝用正安之曲 其一",
        "preface": "",
        "paragraphs": [
            "晓日敞天门，星环萃盍簪。",
            "明良赓舜日，富寿祝尧心。",
            "穿仗摇旗影，鸣珂混乐音。",
            "圣朝敦孝理，百辟尽丕钦。"
        ],
        "notes": "",
        "id": "11f3edc57fb87d8154b2077bfeeac8a0"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 群臣酒三行竝用正安之曲 其二",
        "preface": "",
        "paragraphs": [
            "缥缈殿中央，红云捧紫皇。",
            "香风飘彩仗，仙露滴霞觞。",
            "乐度仪凰曲，恩流振鹭行。",
            "万年天下养，宝祚享灵长。"
        ],
        "notes": "",
        "id": "7cdc9f6d6190c5dcc11b8a9206bcd4cc"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 群臣酒三行竝用正安之曲 其三",
        "preface": "",
        "paragraphs": [
            "清漏滴彤壶，皇仁雨露濡。",
            "遥瞻天一笑，同效岳三呼。",
            "御醴传觞速，宫花覆座敷。",
            "欲知归美意，歌颂在康衢。"
        ],
        "notes": "",
        "id": "601754b024f06bd3ecaffe592d7ead8e"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 酒一行罢用万寿无疆之舞",
        "preface": "",
        "paragraphs": [
            "三纪御图，道丰德盛。",
            "化洽兆民，功光列圣。",
            "畀托有归，脱屣万乘。",
            "优游法宫，颐神养性。",
            "岧岧具阙，丰丽靖渊。",
            "游神象外，玩意物先。",
            "为众父父，尊荣配天。",
            "大福备禄，何千万年。"
        ],
        "notes": "",
        "id": "d6210562f2f91023fdbbad3fba54b5f6"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "皇帝上太上皇帝寿乐曲 酒再行罢用圣德重光之舞",
        "preface": "",
        "paragraphs": [
            "於穆圣子，仁天智神。",
            "体刚立极，宝俭化民。",
            "美政毕举，旧章聿遵。",
            "舞以象德，拟韶实伦。",
            "龙楼问寝，前殿奉巵。",
            "密承慈训，不怠其祗。",
            "海宇丰阜，边陲晏夷。",
            "于以养志，福禄来宜。"
        ],
        "notes": "",
        "id": "f13eec238f7c8190a1e26e3b07ea6e5c"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "正月六日集玉壶次才元韵",
        "preface": "",
        "paragraphs": [
            "铃索文书晚罢休，归来欲赋畔牢愁。",
            "感君尊酒招要意，适我湖山烂熳游。",
            "隔岸柳烟匀拂掠，近溪梅粉半漂流。",
            "千林紫翠无穷在，后会应须到上头。"
        ],
        "notes": "",
        "id": "270232b4d34f3ce85f5d2efc72b2e28b"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "方壶",
        "preface": "",
        "paragraphs": [
            "垂杨阴下小亭池，浓绿繁红掩映宜。",
            "挟册独来谁共语，海棠相对笑人痴。"
        ],
        "notes": "",
        "id": "c1df4cf292cec9a84955fbf330499946"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和唐致远蜡梅",
        "preface": "",
        "paragraphs": [
            "万木折冰雪，千葩谢华秾。",
            "谁约金童仙，冲寒下严风。",
            "恼人风味深，缕衣立隆冬。",
            "旁有玉京子，含香笑相逢。",
            "夜水吐玉龙，月窗影相重。",
            "诗情太牢落，聊以二友供。",
            "清标体黄裳，素姿掩凡容。",
            "东君著意先，群英绝追踪。"
        ],
        "notes": "",
        "id": "13e014bc6c9e74dfdea9527223f07786"
    },
    {
        "author": "崔敦诗",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "和甄监簿述怀",
        "preface": "",
        "paragraphs": [
            "一鸣当日众俱惊，曾掞天庭赋太清。",
            "宦路喜君方晚遇，文场推我是虚声。",
            "短材漫饱支离粟，藻思宜粲沆瀣英。",
            "近喜和光成莫逆，嘉名洋溢鲁诸生。"
        ],
        "notes": "",
        "id": "0759b6e08269a2095b512d7894b42ba5"
    }
]