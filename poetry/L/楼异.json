[
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 青童峰",
        "preface": "",
        "paragraphs": [
            "斩新高髻掠云开，翠色罗衣一样裁。",
            "知有真仙此中住，故命天女捧书来。"
        ],
        "notes": "",
        "id": "410cede3bed80b098c23a8fa9b9c8e0e"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 黄盖峰",
        "preface": "",
        "paragraphs": [
            "一片黄云驻不飞，中天帝子欲何知。",
            "不须更问玉人事，自有嵩高峻极诗。"
        ],
        "notes": "",
        "id": "08cc832432a1401969691d6c9ef056aa"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 浮邱峰",
        "preface": "",
        "paragraphs": [
            "谁知方丈与瀛洲，尘世纷纷漫白头。",
            "不到嵩山最高处，世人容易揖浮邱。"
        ],
        "notes": "",
        "id": "2157881d1373bd3517634b080303609e"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 三鹤峰",
        "preface": "",
        "paragraphs": [
            "昔有仙翁住此间，炼成青发与朱颜。",
            "一朝跨鹤升天去，涧草岩花岂复攀。"
        ],
        "notes": "",
        "id": "5ab0e4760efad4cf620606794707e4ee"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 遇圣峰",
        "preface": "",
        "paragraphs": [
            "汉家天子学神仙，曾遇真人耳过肩。",
            "不待菖蒲长黑发，须知逸乐自延年。"
        ],
        "notes": "",
        "id": "77e3d6855ec017db35937f75636e023e"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 万岁峰",
        "preface": "",
        "paragraphs": [
            "仙仗西来感百神，泥金检玉尚如新。",
            "山灵知是真天子，万岁声中第一人。"
        ],
        "notes": "",
        "id": "899a90c01cc39eb8eedd33501cab6be0"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 玉镜峰",
        "preface": "",
        "paragraphs": [
            "皎皎冰盘莹百围，广寒宫殿见依稀。",
            "青山万叠浑如洗，浮翠光中一镜飞。"
        ],
        "notes": "",
        "id": "9e7911cad9c9ead9bf122e34bbcbbf05"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 狮子峰",
        "preface": "",
        "paragraphs": [
            "苍苔藤蔓饰须眉，狮子标形是与非。",
            "果解嚬呻作哮吼，窟中狐兔定魂飞。"
        ],
        "notes": "",
        "id": "d4a2d94606ed7a069e10b5969c030b03"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 虎头峰",
        "preface": "",
        "paragraphs": [
            "万里封侯相不如，道人曾此获阴符。",
            "轩牙露舌惊樵采，猛士还当捋尔须。"
        ],
        "notes": "",
        "id": "7aa2ea22a45104fa2391e09c34fd74d1"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 起云峰",
        "preface": "",
        "paragraphs": [
            "触石孤飞一叶蓬，雷公电母昼鞭龙。",
            "但知四海为霖雨，不道中天第几峰。"
        ],
        "notes": "",
        "id": "dd9fc2aa5040710ac271780817d5483b"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 凤凰峰",
        "preface": "",
        "paragraphs": [
            "当年翔凤此徘徊，曾为真人览德来。",
            "今日飞鸣天外过，纷纷燕雀不须猜。"
        ],
        "notes": "",
        "id": "3a2a146cf590594e8f6c362ca1bea180"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 金壶峰",
        "preface": "",
        "paragraphs": [
            "想见金壶泻墨河，皂林余润鬰嵯峨。",
            "伯阳当日传经后，肯向山阴与换鹅。"
        ],
        "notes": "",
        "id": "ed4e5ab3276e6a091a9b34bc426c5313"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 华盖峰",
        "preface": "",
        "paragraphs": [
            "华盖峰高未易穷，扪参历井到天中。",
            "回头却顾人间世，但见群青似小童。"
        ],
        "notes": "",
        "id": "46f6b2d7e18f4019f217469c2e688664"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 玄龟峰",
        "preface": "",
        "paragraphs": [
            "玄武名峰古所传，晨昏吐气作云烟。",
            "虽无钻灼刳肠患，岁晚风霜亦可怜。"
        ],
        "notes": "",
        "id": "cafa6540e0b216bcfe4016fdfc4211b0"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 卧龙峰",
        "preface": "",
        "paragraphs": [
            "头角低回藓蔓封，蜿蜒端似卧真龙。",
            "旱干岁祷多灵应，时见油云出此峰。"
        ],
        "notes": "",
        "id": "eb41e387a9ce2c6056beaebec3888281"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 会仙峰",
        "preface": "",
        "paragraphs": [
            "鹤乘云𫐌下九天，玉盘星子竟谁先。",
            "相应汉武题花品，留作人间聚八仙。"
        ],
        "notes": "",
        "id": "21972e70be274e9b7ec99098fa46976e"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 子晋峰",
        "preface": "",
        "paragraphs": [
            "当年曾悟镜中形，道骨仙风拟紫冥。",
            "二十四峰明月夜，玉笙须向揖仙听。"
        ],
        "notes": "",
        "id": "f0b58b8506e05cd69df7da78581bd004"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 玉柱峰",
        "preface": "",
        "paragraphs": [
            "丹楹未羡人间侈，玉柱一峰天外高。",
            "自古大材须大用，扶持广厦岂辞劳。"
        ],
        "notes": "",
        "id": "14be144d9f2fe5e8b87cc83aafbcf173"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 老翁峰",
        "preface": "",
        "paragraphs": [
            "翳雾埋云皓首翁，难将书传考前踪。",
            "商山羽翼朝天后，化作中天一石峰。"
        ],
        "notes": "",
        "id": "38bef175be5944d6595616edb67f5deb"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 玉人峰",
        "preface": "",
        "paragraphs": [
            "汉武求仙未得仙，玉人何事落峰前。",
            "故知帝子乘云下，神盖峰头启洞天。"
        ],
        "notes": "",
        "id": "d37227def60d6a9c3ea365df99e6499f"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 玉女峰",
        "preface": "",
        "paragraphs": [
            "玉仙曾此驻云车，日薄窗纱映雪肤。",
            "七字天书人不辨，定知玄女手中符。"
        ],
        "notes": "",
        "id": "89e7aa34a70855cbeef787753b6886a2"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 独秀峰",
        "preface": "",
        "paragraphs": [
            "孕玉怀金不待媒，蓊然佳气罩崔嵬。",
            "天教秀拔诸峰上，未信狂飚解折摧。"
        ],
        "notes": "",
        "id": "8086f26b6a2f62bcdff727f29ecca090"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 积翠峰",
        "preface": "",
        "paragraphs": [
            "翠幄笼云自一家，萝梯夜碾七香车。",
            "举头雨过余霞散，绿芰池开菡萏花。"
        ],
        "notes": "",
        "id": "ef437195412f2a4edbf3e9d097b84075"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "嵩山二十四咏 太白峰",
        "preface": "",
        "paragraphs": [
            "仰攀日月风云近，俯瞰冈峦培𪣻低。",
            "绝顶光芒长不断，人言太白此中栖。"
        ],
        "notes": "",
        "id": "d8d3f0f3a3797b6158dca117d3a71d13"
    },
    {
        "author": "楼异",
        "dynasty": "宋",
        "type": "诗",
        "rhythmic": "",
        "title": "存目",
        "preface": "",
        "paragraphs": [
            "诗题：宿少林寺首句：六六仙峰绕佛居。"
        ],
        "notes": "",
        "id": "ff98a19dd1675ed542429e4b1d2df0c9"
    }
]