[
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "吴宫",
        "preface": "",
        "paragraphs": [
            "吴王爱歌舞，夜夜醉婵娟。",
            "见日吹红烛，和尘扫翠钿。",
            "徒令句践霸，不信子胥贤。",
            "莫问长洲草，荒凉无限年。"
        ],
        "notes": "",
        "id": "b0567b0949ac8decf9496bb636df5f40"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "久雨",
        "preface": "",
        "paragraphs": [
            "云影蔽遥空，无端淡复浓。",
            "两旬绵密雨，二月似深冬。",
            "诗酒从教数，帘帏一任重。",
            "孰知春有地，微露小桃红。"
        ],
        "notes": "",
        "id": "47eb6d56ffdacef1a1c761437af20760"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "郊行逢社日",
        "preface": "",
        "paragraphs": [
            "酒熟送迎便，村村庆有年。",
            "妻孥亲稼穑，老稚効渔畋。",
            "红树青林外，黄芦白鸟边。",
            "稔看风景美，宁不羡归田。"
        ],
        "notes": "",
        "id": "031c9c144c1d13c481d9cdd7cfb92233"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "过友人幽居",
        "preface": "",
        "paragraphs": [
            "身坐众香国，蒲团诗思新。",
            "一贫曾累我，此兴未输人。",
            "陋巷谁为俗，寒窗不染尘。",
            "石斋盟四友，年下顿生春。"
        ],
        "notes": "",
        "id": "d8d0311eff2983410ee7eeb9630ec4d1"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "过雍陶博士邸中饮",
        "preface": "",
        "paragraphs": [
            "落叶下萧萧，幽居远市朝。",
            "偶成投辖饮，不待致书招。",
            "塞鴈冲寒过，山云傍槛飘。",
            "此身何所似，天地一渔樵。"
        ],
        "notes": "",
        "id": "e1144402e37be6ae1caec4d8b7121e91"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "游王羽士山房",
        "preface": "",
        "paragraphs": [
            "落日半楼明，琳宫事事清。",
            "山横万古色，鹤带九臯声。",
            "易作神仙侣，难忘父子情。",
            "道人应识我，未肯说长生。"
        ],
        "notes": "",
        "id": "a82b63f21d381b111a7136d1f66aa98b"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "署中答武功姚合",
        "preface": "",
        "paragraphs": [
            "原中多阴雨，惟留一室明。",
            "自宜居静者，谁得问先生。",
            "深井泉香出，危沙药更荣。",
            "全家笑无辱，曾不见戈兵。"
        ],
        "notes": "",
        "id": "92a25106fd20536b4c834f6aff8ddb57"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠龙阳尉马戴",
        "preface": "",
        "paragraphs": [
            "早学全身术，惟令耕近田。",
            "自输官税后，常卧晚云边。",
            "细草沿堦长，高萝出石悬。",
            "向来名姓茂，空被外情牵。"
        ],
        "notes": "",
        "id": "14ebcc1aed3661e9e598694ca8dfcb42"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "上巳日赠都上人 一",
        "preface": "",
        "paragraphs": [
            "三月初三日，千家与万家。",
            "蝶飞秦地草，莺入汉宫花。",
            "鞍马皆争丽，笙歌尽鬬奢。",
            "吾师无所愿，惟愿老烟霞。"
        ],
        "notes": "",
        "id": "4fab3b2ff531844ecb3fcae50e1c195f"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "上巳日赠都上人 二",
        "preface": "",
        "paragraphs": [
            "曲水公卿宴，香尘尽满街。",
            "无心修禊事，独步到禅斋。",
            "细草萦愁目，繁花逆旅怀。",
            "绮罗人走马，遗落凤皇钗。"
        ],
        "notes": "",
        "id": "543fdbc708a7435cb490c3670f67617d"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送沈亚之尉南康",
        "preface": "",
        "paragraphs": [
            "行迈南康路，客心离怨多。",
            "暮烟葵叶屋，秋月竹枝歌。",
            "孤鹤唳残梦，惊猨啸薜萝。",
            "对江翘首望，愁泪叠如波。"
        ],
        "notes": "",
        "id": "a06c589d0d24a4e53e1f7c618c0b841a"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "奉送刘使君王屋山隐居",
        "preface": "",
        "paragraphs": [
            "散发风簷下，沈沈日渐曛。",
            "鹰拳擒野雀，蛛网猎飞蚊。",
            "群动能归计，吾生亦谩勤。",
            "尘缘难著眼，晚兴寄青云。"
        ],
        "notes": "",
        "id": "959c43fafa95c3c78ef18a45e829aa61"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寄许浑秀才",
        "preface": "",
        "paragraphs": [
            "文字饥难煮，为农策最良。",
            "兴来鉏晓月，倦后卧斜阳。",
            "秋稼连千顷，春花醉几场。",
            "任他名利客，车马闹康庄。"
        ],
        "notes": "",
        "id": "4427e60dbb19b90f49622b81a4db4852"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送客游吴",
        "preface": "",
        "paragraphs": [
            "吴国水中央，波涛白渺茫。",
            "衣逢梅雨渍，船入稻花香。",
            "海戍通盐灶，山村带蜜房。",
            "欲知苏小小，君试到钱塘。"
        ],
        "notes": "",
        "id": "a4497aa7d660d7700b8687961f6956a2"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "陆丞相故宅",
        "preface": "",
        "paragraphs": [
            "衣冠零落久，今日事堪伤。",
            "厨起青烟薄，门开白日长。",
            "残梅欹古道，名石卧颓墙。",
            "山色依然好，兴衰未可量。"
        ],
        "notes": "",
        "id": "551129f9cd2c763e9a816fb2afc81d2e"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "中元日观诸道士步虚",
        "preface": "",
        "paragraphs": [
            "玄都开秘箓，白石礼先生。",
            "上界秋光净，中元夜气清。",
            "星辰朝帝处，鸾鹤步虚声。",
            "玉洞花长发，珠宫月最明。",
            "扫坛天地肃，投简鬼神惊。",
            "傥赐刀圭药，还留不死名。"
        ],
        "notes": "",
        "id": "1f7544533e05121c8a4299184e4566df"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "醉赠刘十二",
        "preface": "",
        "paragraphs": [
            "春草正凄凄，知君过恶溪。",
            "莺将吉了语，猿共猓然啼。",
            "别路魂先断，还家梦几迷。",
            "定寻雷令劒，应识越王笄。",
            "树色多于北，潮声少向西。",
            "椰花好为酒，谁伴醉如泥。"
        ],
        "notes": "",
        "id": "8c126e010e3ae7e2a52de01ef69e9084"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "早朝",
        "preface": "",
        "paragraphs": [
            "曙钟催入紫宸朝，列炬流虹映绛绡。",
            "天近鳌头花簇杖，风低豹尾乐鸣韶。",
            "衣冠一变无夷俗，律令重颁有正条。",
            "昨日钟山甘露降，玻璃满赐出宫瓢。"
        ],
        "notes": "",
        "id": "8bfa56432cf5b0ad8c255242181c718f"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "帝京二首 一",
        "preface": "",
        "paragraphs": [
            "列郡征才起俊髦，万机独使圣躬劳。",
            "开藩上相颁龙节，破虏将军展豹韬。",
            "地入黄图三辅壮，天垂华盖七星高。",
            "迎春别赐瑶池宴，捧进金盘五色桃。"
        ],
        "notes": "",
        "id": "9335b4cc34534a1a3f24c1cf54392bd9"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "帝京二首 二",
        "preface": "",
        "paragraphs": [
            "龙虎山河御气通，遥瞻帝阙五云红。",
            "英雄尽入江东籍，将相多收蓟北功。",
            "礼乐日稽三代盛，梯航岁贡万方同。",
            "都将俭德熙文治，淳俗应还太古风。"
        ],
        "notes": "",
        "id": "dcd099a1b9ddb5d845f084c45f3006e1"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宫词",
        "preface": "",
        "paragraphs": [
            "悄悄深宫不见人，倚阑惟见石麒麟。",
            "芙蓉帐冷愁长夜，翡翠帘垂隔小春。",
            "天远难通青鸟信，风寒欲动锦花茵。",
            "夜深怕有羊车过，自起笼灯看雪纹。"
        ],
        "notes": "",
        "id": "70ae7104bfa38b57cff13083cd4f8a16"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "郊居作",
        "preface": "",
        "paragraphs": [
            "碧树浓阴护短垣，苍江春煖渚凫喧。",
            "买鱼试唤鸣榔艇，寻鹤因行隔垅村。",
            "生理何凭文是业，世情纵遣酒盈樽。",
            "相逢谓我迂疎甚，欲辨还憎恐失言。"
        ],
        "notes": "",
        "id": "abaaa38eb86ad91c82bc1835798167c3"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "闲居",
        "preface": "",
        "paragraphs": [
            "茂苑闲居木石同，旋开小迳翦蒿蓬。",
            "虚游心在鸿蒙外，穴处身疑培𪣻中。",
            "花影一阑吟夜月，松声半榻卧秋风。",
            "百年寄傲聊容膝，何必高车驷马通。"
        ],
        "notes": "",
        "id": "c12c951537bca5cb2d281330a090dc53"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "暮春述怀",
        "preface": "",
        "paragraphs": [
            "为客山南二十年，愁来怳近落花天。",
            "阴云带雨连山脊，湿气成岚滴树巅。",
            "隣屋有声敲石火，野禽无语避茶烟。",
            "此时若遇孙阳顾，肯服盐车不受鞭。"
        ],
        "notes": "",
        "id": "5d87d58c4c9970fcdc6c92d87546da51"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "端午日",
        "preface": "",
        "paragraphs": [
            "少年佳节倍多情，老去谁知感慨生。",
            "不効艾符趋习俗，但祈蒲酒话升平。",
            "鬓丝日日添头白，榴锦年年照眼明。",
            "千载贤愚同瞬息，几人湮没几垂名。"
        ],
        "notes": "",
        "id": "3cf4e21072b9542b14be65092a7c96ec"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "九日",
        "preface": "",
        "paragraphs": [
            "万里飘零十二秋，不堪今倚夕阳楼。",
            "壮怀空掷班超笔，久客谁怜季子裘。",
            "瘴雨蛮烟朝暮景，平芜野草古今愁。",
            "酣歌欲尽登高兴，强把黄花插满头。"
        ],
        "notes": "",
        "id": "b96a1fbc2c908999aa344e244f501bc8"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "九日病起",
        "preface": "",
        "paragraphs": [
            "重阳开满菊花金，病起榰床惜赏心。",
            "紫蟹霜肥秋纵好，绿醅蚁滑晚慵斟。",
            "眼窥薄雾行殊倦，身怯寒风坐未禁。",
            "沈醉又成来岁约，遣怀聊作记时吟。"
        ],
        "notes": "",
        "id": "f2f17b0aeda40d49cd5a7a5aa024516b"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寒夜",
        "preface": "",
        "paragraphs": [
            "云冷江空岁暮时，竹阴梅影月参差。",
            "鸡催梦枕司晨早，更咽寒城报点迟。",
            "人事纷华潜动息，天心静默运推移。",
            "凭谁荡涤穷残候，入眼东风喜在期。"
        ],
        "notes": "",
        "id": "74f793921c8b51dd4dd6fdac07cb7c05"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "喜雨",
        "preface": "",
        "paragraphs": [
            "临岐终日自裴回，干我茅斋半亩苔。",
            "山上乱云随手变，淛东飞雨过江来。",
            "一元和气归中正，百怪苍渊起蛰雷。",
            "千里稻花应秀色，酒樽风月醉亭台。"
        ],
        "notes": "",
        "id": "2c1f88d4e4b0034397a127c8477bebe5"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "春游",
        "preface": "",
        "paragraphs": [
            "明日城东看杏花，叮咛童子蚤将车。",
            "路从丹凤楼前过，酒向金鱼馆里赊。",
            "绿水满沟生杜若，暖云将雨湿泥沙。",
            "绝胜羊傅襄阳道，车骑西风拥鼓笳。"
        ],
        "notes": "",
        "id": "511145ec50cef74f1a708e5305cdbcf9"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "夜酌溪楼",
        "preface": "",
        "paragraphs": [
            "楼居溪上凉生早，坐对城头起暮笳。",
            "打鼓泊船何处客，捣衣隔竹是谁家。",
            "玉绳低转宵初迥，银烛高烧月近斜。",
            "得意引杯须痛饮，好怀那许负年华。"
        ],
        "notes": "",
        "id": "f98f4ca933f11d4910a58c2a99917166"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "旅行",
        "preface": "",
        "paragraphs": [
            "烟树寒林半有无，野人行李更萧疎。",
            "堠长堠短逢官马，山北山南闻鹧鸪。",
            "万里关河成传舍，五更风雨忆呼卢。",
            "寂寥一点寒灯在，酒熟隣家许夜沽。"
        ],
        "notes": "",
        "id": "7f047116a9c5f6d6e974aa5a85ca2913"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "下第东归作",
        "preface": "",
        "paragraphs": [
            "十载驱驰倦荷锄，三年生计鬓萧疎。",
            "辛勤几逐英雄后，乙榜犹然姓氏虚。",
            "欲射狼星把弓箭，休将萤火读诗书。",
            "身贱自慙贫骨相，朗啸东归学钓鱼。"
        ],
        "notes": "",
        "id": "855a35d667004f8260be16ba3d48c2d3"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "还京口",
        "preface": "",
        "paragraphs": [
            "黄鹤山头雪未消，行人归计在今朝。",
            "城高铁瓮江山壮，地接金陵草木雕。",
            "北府市楼闻旧酒，南桥官柳识归桡。",
            "吏民莫见参军面，水宿风餐鬓发焦。"
        ],
        "notes": "",
        "id": "e2e4f5b7943daea5a7525b32dcb1ee56"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "襄口阻风",
        "preface": "",
        "paragraphs": [
            "雪浪排空接海门，孤舟三日阻龙津。",
            "曹瞒曾堕周郎计，王导难遮庚亮尘。",
            "鸥散白云沈远浦，花飞红雨送残春。",
            "篙师整缆候明发，仍谒荒祠问鬼神。"
        ],
        "notes": "",
        "id": "e0e958819435c8d8eaba6d0f914acb97"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "潭州独步",
        "preface": "",
        "paragraphs": [
            "鹤发垂肩懒著巾，晚凉独步楚江滨。",
            "一帆暝色鸥边雨，数尺筇枝物外身。",
            "习巧未逢医拙手，闻歌先识采莲人。",
            "笑看斥鷃飞翔去，乐处蓬莱便有春。"
        ],
        "notes": "",
        "id": "59e07786e37b24ba0bb2886c4c380580"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "金陵怀古",
        "preface": "",
        "paragraphs": [
            "黄道天清拥珮珂，东南王气秣陵多。",
            "江吞彭蠡来三蜀，地接昆仑带九河。",
            "凤阙晓霞红散绮，龙池春水绿生波。",
            "华夷混一归真主，端拱无为乐太和。"
        ],
        "notes": "",
        "id": "18dc8e9137ecd40bc7aca91c62833184"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "登凤凰台二首 一",
        "preface": "",
        "paragraphs": [
            "凤凰台上望长安，五色宫袍照水寒。",
            "彩笔十年留翰墨，银河一夜卧阑干。",
            "三山飞鸟江天暮，六代离宫草树残。",
            "始信人生如一梦，壮怀莫使酒杯干。"
        ],
        "notes": "",
        "id": "b730a3bd9f620499d8df8fcf8eb805c5"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "登凤凰台二首 二",
        "preface": "",
        "paragraphs": [
            "梧桐叶落秋风老，人去台空凤不来。",
            "梁武台成芳草合，吴王宫殿野花开。",
            "石头城下春生水，燕子堂前雨长苔。",
            "莫问人间兴废事，百年相遇且衔杯。"
        ],
        "notes": "",
        "id": "3075219b690eae9fb863d8505d8aa56e"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "韩信庙",
        "preface": "",
        "paragraphs": [
            "长空鸟尽将军死，无复中原入马蹄。",
            "身向九泉还属汉，功超诸将合封齐。",
            "荒凉古庙惟松柏，咫尺长陵又鹿麋。",
            "此日深怜萧相国，竟无一语到金闺。"
        ],
        "notes": "",
        "id": "8999f4df064e72160eb7f489ca723738"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "访许浑",
        "preface": "",
        "paragraphs": [
            "去郭来寻隐者居，柳阴假步小篮舆。",
            "每期会面初偿约，却计论心旧得书。",
            "浅绿垣墙绵薜荔，淡红池沼映芙蕖。",
            "为言肯共留连饮，涧有青芹罟有鱼。"
        ],
        "notes": "",
        "id": "cfdaeafada5f7dc2ad08f3324a344ca1"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "李舍人席上感遇",
        "preface": "",
        "paragraphs": [
            "微云歛雨天气清，松声出树秋泠泠。",
            "窗户长含碧萝色，溪流时带蛟龙腥。",
            "一官到手不可避，万事役我徒劳形。",
            "飘然曳杖出门去，无数好山江上横。"
        ],
        "notes": "",
        "id": "a2dcc6c6fd1b4ddd68e22f42f0582e35"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "和赵相公登鹳雀楼",
        "preface": "",
        "paragraphs": [
            "危楼高架泬寥天，上相闲登立彩旃。",
            "树色到京三百里，河流归汉几千年。",
            "晴峰耸日当周道，秋谷垂花满舜田。",
            "云路何人见高志，最看西面赤阑前。"
        ],
        "notes": "",
        "id": "9ba08855cff1481f15ba7b506487ef6a"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "冬至酬刘使君",
        "preface": "",
        "paragraphs": [
            "异乡冬至又今朝，回首家山入梦遥。",
            "渐喜一阳从地复，却怜群沴逐冰消。",
            "梅含露蘂知迎腊，柳拂宫袍忆候朝。",
            "多少故人承宴赏，五云堆里听箫韶。"
        ],
        "notes": "",
        "id": "0e05817eefd3bb8125833762fb8af98a"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "李节度平虏诗",
        "preface": "",
        "paragraphs": [
            "百万王师下日边，将军雄略可图全。",
            "元勋未论封茅异，捷势应知破竹然。",
            "燕警无烽清朔漠，秦文有宝进蓝田。",
            "太平从此销兵甲，记取红羊换劫年。"
        ],
        "notes": "",
        "id": "6e2194777a9ed994157d02f823f035f2"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "金陵上李公垂侍郎",
        "preface": "",
        "paragraphs": [
            "海国微茫散晓暾，郁葱佳气满乾坤。",
            "六朝空据长江险，一统今归圣代尊。",
            "西北诸峰连朔漠，东南众水合昆仑。",
            "愿从吾道禧文运，再使河清俗化淳。"
        ],
        "notes": "",
        "id": "48c71c9818f6d51aa355f8b9b70e4d69"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠惟俨师",
        "preface": "",
        "paragraphs": [
            "焕然文采照青春，一策江湖自在身。",
            "云锁木龛聊息影，雪香纸袄不生尘。",
            "谈禅早续灯无尽，护法重编论有神。",
            "拟扫绿阴浮佛寺，桫椤高树结为隣。"
        ],
        "notes": "",
        "id": "f679d560449e2d3c167135d576a28e5b"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寄许浑秀才",
        "preface": "",
        "paragraphs": [
            "万木惊秋叶渐稀，静探造化见玄机。",
            "眼前谁悟先天理，去后还知今日非。",
            "树拥秣陵千嶂合，云开萧寺一僧归。",
            "汉廷累下征贤诏，未许严陵老钓矶。"
        ],
        "notes": "",
        "id": "8ebe1d4131b7e39c02af19f247873314"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送白舍人渡江",
        "preface": "",
        "paragraphs": [
            "晓发龙江第一程，诸公同济似登瀛。",
            "海门日上千峰出，桃叶波平一棹轻。",
            "横锁已沈王濬筏，投鞭难阻谢玄兵。",
            "片时喜得东风便，回首钟声隔凤城。"
        ],
        "notes": "",
        "id": "7c562d92e92bcf1bce4ead1764239e9a"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送刘禹锡侍御出刺连州",
        "preface": "",
        "paragraphs": [
            "遐荒迢遰五羊城，归兴浓消客里情。",
            "家近似忘山路险，土甘殊觉瘴烟轻。",
            "梅花清入罗浮梦，荔子红分广海程。",
            "此去定知偿隐趣，石田春雨读书耕。"
        ],
        "notes": "",
        "id": "2fd1c7623a860e46365c3976f56e04c5"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送韦侍御报使西蕃",
        "preface": "",
        "paragraphs": [
            "归奏圣朝行万里，却衔天诏报蕃臣。",
            "本是诸生守文墨，今将匹马静烟尘。",
            "旅宿关河逢暮雨，春耕亭障识遗民。",
            "此去多应收故地，宁辞沙塞往来频。"
        ],
        "notes": "",
        "id": "139f56ff4913dbd3a5443d23293552ea"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寒食城南即事因访蓝田韦明府",
        "preface": "",
        "paragraphs": [
            "闲出城南禁火天，路傍骑马独摇鞭。",
            "青松古墓伤碑碣，红杏春园羡管弦。",
            "徒说䴙鹈膏玉劒，漫夸蚨血点铜钱。",
            "世间尽是悠悠事，且饮韦家冷酒眠。"
        ],
        "notes": "",
        "id": "0311a3a5f89db5525fb61416b2876cfd"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送源中丞使新罗",
        "preface": "",
        "paragraphs": [
            "赤墀奉命使殊方，官重霜台紫绶光。",
            "玉节在船清海怪，金函开诏抚夷王。",
            "云晴渐觉山川异，风便宁知道路长。",
            "谁得似君将雨露，海东万里洒扶桑。"
        ],
        "notes": "",
        "id": "bef371674aa306b44801c8190dadf17a"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "送景玄上人还山",
        "preface": "",
        "paragraphs": [
            "嵩阳听罢讲经钟，远访庭闱锡度空。",
            "蒲履谩从归后织，衲衣犹记别时缝。",
            "地横龙朔连沙暝，山入乌桓碧树重。",
            "梵宇传来金贝叶，花前拜捧慰亲容。"
        ],
        "notes": "",
        "id": "09287c457cf05a976585c46755ceebd7"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "宫人入道",
        "preface": "",
        "paragraphs": [
            "卸却宫妆锦绣衣，黄冠素服制相宜。",
            "锡名近奉君王旨，佩箓新参老氏师。",
            "白昼无情趋玉陛，清宵有梦步瑶池。",
            "绿鬟女伴含愁别，释尽当年妬宠私。"
        ],
        "notes": "",
        "id": "11dc482f8311afb79a403c667622e92e"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "友人山中梅花",
        "preface": "",
        "paragraphs": [
            "南国看花动远情，沈郎诗苦瘦容生。",
            "铁心自儗山中赋，玉笛谁将月下横。",
            "临水一枝春占早，照人千树雪同清。",
            "好风吹醒罗浮梦，莫听空林翠羽声。"
        ],
        "notes": "",
        "id": "c2ddc0b71e89f96459fb1966a2476f32"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "客中有感",
        "preface": "",
        "paragraphs": [
            "天地一身在，头颅五十过。",
            "流年消壮志，空使泪成河。"
        ],
        "notes": "",
        "id": "076bc61ac0bab1ad269fc90c58d8b4c4"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "忆家二首 一",
        "preface": "",
        "paragraphs": [
            "新霁飏林初，苹花贴岸舒。",
            "故乡今夜月，犹得照孤庐。"
        ],
        "notes": "",
        "id": "6cd50d9990168c168ae4fba7003c8388"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "忆家二首 二",
        "preface": "",
        "paragraphs": [
            "树拥溪边阁，山浮雨后岚。",
            "白头归未得，梦里望江南。"
        ],
        "notes": "",
        "id": "15dfdff5fdde9d0b3d8ce8937baa4d1f"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "江行二首 一",
        "preface": "",
        "paragraphs": [
            "暝色沧州迥，秋声玉峡长。",
            "只因江上月，不觉过浔阳。"
        ],
        "notes": "",
        "id": "54b280bd0eef2c227f897429ba13fb0c"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "江行二首 二",
        "preface": "",
        "paragraphs": [
            "晚泊长江口，寒沙白似霜。",
            "年光流不尽，东去水声长。"
        ],
        "notes": "",
        "id": "ae03bb1b622472dcc464a7e7485e6269"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "偶题",
        "preface": "",
        "paragraphs": [
            "越女收龙眼，蛮儿拾象牙。",
            "长安千万里，走马送谁家。"
        ],
        "notes": "",
        "id": "c0be843c1c854a5e54b520c2d36cdabc"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "关中伤乱后",
        "preface": "",
        "paragraphs": [
            "去岁干戈险，今年蝗旱忧。",
            "关西归战马，海内卖耕牛。"
        ],
        "notes": "",
        "id": "dc54bc3586f058c697f4b31e89f9a04c"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "楚江怀古",
        "preface": "",
        "paragraphs": [
            "骚灵不可见，楚些竟谁闻。",
            "欲采苹花去，沧州隔暮云。"
        ],
        "notes": "",
        "id": "737ba65619a7dc86af34177410cf3458"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "张飞庙",
        "preface": "",
        "paragraphs": [
            "威名垂万古，勇力冠当时。",
            "回首三分国，何人赋黍离。"
        ],
        "notes": "",
        "id": "7d47313c960d3483a125efb6c84bb5a3"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "生公讲台",
        "preface": "",
        "paragraphs": [
            "暝色护楼台，阴云昼未开。",
            "一尘无处著，花雨遍苍苔。"
        ],
        "notes": "",
        "id": "77d94f53ce8a5df6ea0c4e137d28a773"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "席上听琴",
        "preface": "",
        "paragraphs": [
            "高堂流月明，万籁不到耳。",
            "一听清心魂，飞絮春纷起。"
        ],
        "notes": "",
        "id": "ab53cd5b64233abb665bd40ea6de9193"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "酬雍秀才二首 一",
        "preface": "",
        "paragraphs": [
            "晚市人烟合，归帆带夕阳。",
            "栖迟未归客，犹著锦衣裳。"
        ],
        "notes": "",
        "id": "cb92fb3ffb19c024eefb6d52268eed4c"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "酬雍秀才二首 二",
        "preface": "",
        "paragraphs": [
            "卧病茅窗下，惊闻两月过。",
            "兴来聊赋咏，清婉逼阴何。"
        ],
        "notes": "",
        "id": "64e3a9e7eb983eb1a85393722373a9d6"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "竹",
        "preface": "",
        "paragraphs": [
            "窗户尽萧森，空堦凝碧阴。",
            "不缘冰雪里，为识岁寒心。"
        ],
        "notes": "",
        "id": "b13a4ab86e3a04ea8819135d3e90b2d8"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "春怨",
        "preface": "",
        "paragraphs": [
            "柳花扑帘春欲尽，绿阴障林莺乱啼。",
            "秖愁明日送春去，落日满园啼竹鸡。"
        ],
        "notes": "",
        "id": "c5f4d9746ea35cc12b4f29718da305df"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "馆娃宫",
        "preface": "",
        "paragraphs": [
            "宫女三千去不回，真珠翠羽是尘埃。",
            "夫差旧国久破碎，红燕自归花自开。"
        ],
        "notes": "",
        "id": "eec926369dfa6c9e76ce1c5a53993e38"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "汉宫词三首 一",
        "preface": "",
        "paragraphs": [
            "成帝夫人泪满怀，璧宫相趁落空堦。",
            "可怜玉貌花前死，惟有君恩白燕钗。"
        ],
        "notes": "",
        "id": "572300f78a4a9e9c581457f750deb28b"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "汉宫词三首 二",
        "preface": "",
        "paragraphs": [
            "霍家有女字成君，年少教人著绣裙。",
            "枉杀宫中许皇后，椒房恩泽是浮云。"
        ],
        "notes": "",
        "id": "41d57e2da58154da0eda0e769f159fe0"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "汉宫词三首 三",
        "preface": "",
        "paragraphs": [
            "骏马金鞍白玉鞭，宫中来取李延年。",
            "承恩直日鸳鸯殿，一曲清歌在九天。"
        ],
        "notes": "",
        "id": "d9f6a6bb68086942be3b027dc2c60c8c"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "同州端午",
        "preface": "",
        "paragraphs": [
            "鹤发垂肩尺许长，离家三十五端阳。",
            "儿童见说深惊讶，却问何方是故乡。"
        ],
        "notes": "",
        "id": "9a5868d07cbdd8e0fb0fc5fc1009f943"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "夜过洞庭",
        "preface": "",
        "paragraphs": [
            "笙歌只解闹花天，谁是敲冰掉小船。",
            "为觅潇湘幽隐处，夜深载月听鸣泉。"
        ],
        "notes": "",
        "id": "5cec86f001e4e63fa4148c0564b6c7ca"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "游山南寺二首 一",
        "preface": "",
        "paragraphs": [
            "山中尽日无人到，竹外交加百鸟鸣。",
            "昨日小楼微雨过，樱桃花落晚风晴。"
        ],
        "notes": "",
        "id": "d5af46f935f3f75fa9137b8ed0322ff8"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "游山南寺二首 二",
        "preface": "",
        "paragraphs": [
            "踏碎羊山黄叶堆，天飞细雨隐轻雷。",
            "朗陵莫讶来何晚，不忍听君话别杯。"
        ],
        "notes": "",
        "id": "6f94708ab507516cadf90b5a95056a64"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寄太仆田卿二首 一",
        "preface": "",
        "paragraphs": [
            "客窗强饮太怱怱，急雨寒风意万重。",
            "蓦上心来消未得，梦回又听五更钟。"
        ],
        "notes": "",
        "id": "cbeaefa1ca73dad9b763a81a83ba4fda"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "寄太仆田卿二首 二",
        "preface": "",
        "paragraphs": [
            "一阳才动伏群阴，万物于今寓太音。",
            "若喜长生添线日，微微消息识天心。"
        ],
        "notes": "",
        "id": "43bdd84394dd8e251321c1d2182ae24a"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "新昌井",
        "preface": "",
        "paragraphs": [
            "辘轳千转劳筋力，待得甘泉渴杀人。",
            "且共山麋同饮涧，玉沙铺底浅磷磷。"
        ],
        "notes": "",
        "id": "013bb940e5e481b49d603da4e9721c35"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "经靖安里",
        "preface": "",
        "paragraphs": [
            "巷底萧萧绝市尘，供愁疎雨打黄昏。",
            "悠然一曲泉明调，浅立闲愁轻闭门。"
        ],
        "notes": "",
        "id": "f66531083a31a21d0500d153a2688e75"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "闻筝歌",
        "preface": "",
        "paragraphs": [
            "凄凄切切断肠声，指滑音柔万种情。",
            "花影深沈遮不住，度帏穿幕又残更。"
        ],
        "notes": "",
        "id": "5f5694137c1b2795efce66fb9c148b71"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "吹笙歌",
        "preface": "",
        "paragraphs": [
            "伶儿竹声愁绕空，秦女泪湿燕支红。",
            "玉桃花片落不住，三十六簧能唤风。"
        ],
        "notes": "",
        "id": "07120e61ecdb8499707c7f546f0f91ec"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠歌人郭婉二首 一",
        "preface": "",
        "paragraphs": [
            "石家金谷旧歌人，起唱花筵泪满巾。",
            "红粉少年诸弟子，一时惆怅望梁尘。"
        ],
        "notes": "",
        "id": "b52528b186105bb271b854b758918cb7"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "赠歌人郭婉二首 二",
        "preface": "",
        "paragraphs": [
            "云满衣裳月满身，轻盈归步过流尘。",
            "五更无限留连意，常恐风花又一春。"
        ],
        "notes": "",
        "id": "b05aaa4120adb75bdda695604f33921b"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "潭州席上赠舞柘枝妓",
        "preface": "",
        "paragraphs": [
            "姑苏太守青娥女，流落长沙舞柘枝。",
            "坐满绣衣皆不识，可怜红脸泪双垂。"
        ],
        "notes": "",
        "id": "20048efb14c96f53b94655f236eb478c"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "句",
        "preface": "",
        "paragraphs": [
            "瘴雨出虹𬟽，蛮烟渡江急。",
            "尝闻岛夷俗，犀象满城邑。"
        ],
        "notes": "",
        "id": "9cf2312384f90e2de581199c6a267a26"
    },
    {
        "author": "殷尧藩",
        "dynasty": "唐",
        "type": "诗",
        "rhythmic": "",
        "title": "句",
        "preface": "",
        "paragraphs": [
            "云收碧海连天水，风动红蕉滴露光。"
        ],
        "notes": "",
        "id": "5c72ae6810656949517411fdc55ca8af"
    }
]